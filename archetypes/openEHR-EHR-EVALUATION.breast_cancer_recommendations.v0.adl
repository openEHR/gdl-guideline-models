definition
	EVALUATION[at0000] matches {    -- breast cancer recommendations
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0016] occurrences matches {0..1} matches {    -- Test recommendations
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Follow recommendation
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {    -- Reason for deviation
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Comments
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0002] occurrences matches {0..1} matches {    -- Order tests
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0003] occurrences matches {0..1} matches {    -- gBRCA-mutation
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
							ELEMENT[at0004] occurrences matches {0..1} matches {    -- PIK3CA-mutation
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
							ELEMENT[at0005] occurrences matches {0..1} matches {    -- SP142 (for PDL1 expression)
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
							ELEMENT[at0006] occurrences matches {0..1} matches {    -- 22C3 (for CPS)
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Treatment recommendation
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0008,    -- Neo1
									at0009,    -- Adj1
									at0010,    -- Rad1
									at0011]    -- Pall1
								}
							}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {    -- Recommendation text
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

