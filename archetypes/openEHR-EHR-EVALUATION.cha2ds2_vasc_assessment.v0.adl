definition
	EVALUATION[at0000] matches {	-- CHA2DS2-VASc assessment
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Risk assessment
						value matches {
							0|[local::at0005], 	-- Low risk
							1|[local::at0006], 	-- Intermediate risk
							2|[local::at0007]  	-- High risk
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Annual stroke risk
						value matches {
							0|[local::at0008], 	-- 0.6%
							1|[local::at0009], 	-- 2.2%
							2|[local::at0010], 	-- 3.2%
							3|[local::at0011], 	-- 4.8%
							4|[local::at0012], 	-- 7.2%
							5|[local::at0013], 	-- 9.7%
							6|[local::at0014], 	-- 11.2%
							7|[local::at0015], 	-- 10.8%
							8|[local::at0016]  	-- 12.2%
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Annual risk of stroke/TIA/thromboembolism
						value matches {
							0|[local::at0017], 	-- 0.9%
							1|[local::at0018], 	-- 2.9%
							2|[local::at0019], 	-- 4.6%
							3|[local::at0020], 	-- 6.7%
							4|[local::at0021], 	-- 10.0%
							5|[local::at0022], 	-- 13.6%
							6|[local::at0023], 	-- 15.7%
							7|[local::at0024], 	-- 15.2%
							8|[local::at0025]  	-- 17.4%
						}
					}
				}
			}
		}
	}

