definition
	EVALUATION[at0000] matches {    -- Genetically Inspired Prognostic Scoring System (GIPSS) for Primary Myelofibrosis (PMF) Assessment
		data matches {
			ITEM_TREE[at0003] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Risk
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0007,    -- Low
									at0008,    -- Intermediate-1
									at0009,    -- Intermediate-2
									at0010]    -- High
								}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- 5-year overall survival
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0011,    -- 94%
									at0012,    -- 73%
									at0013,    -- 40%
									at0014]    -- 14%
								}
							}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Median overall survival
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0015,    -- 26.4 years
									at0016,    -- 8.0 years
									at0017,    -- 4.2 years
									at0018]    -- 2 years
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0002] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

