definition
	EVALUATION[at0000] matches {    -- ICC Diagnostic Criteria for Primary Myelofibrosis assessment
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- PMF, Early/Prefibrotic Stage (Pre-PMF)
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0004,    -- Meets criteria
									at0005]    -- Does not meet criteria
								}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- PMF, Overt Fibrotic Stage
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0006,    -- Meets criteria
									at0007]    -- Does not meet criteria
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0008] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0009] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

