definition
	EVALUATION[at0000] matches {    -- mipps70 assessment
		data matches {
			ITEM_TREE[at0003] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Risk MIPSS70
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0008,    -- Very Low
									at0009,    -- Low
									at0010,    -- Intermediate
									at0011,    -- High
									at0012]    -- Very High
								}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Median Overall Survival
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0013,    -- N/A
									at0014,    -- 27.7
									at0015,    -- 10.3
									at0016,    -- 7.1
									at0017,    -- 7.0
									at0018,    -- 3.5
									at0019,    -- 2.3
									at0020]    -- 1.8
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0002] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

