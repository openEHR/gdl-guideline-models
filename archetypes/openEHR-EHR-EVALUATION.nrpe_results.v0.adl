definition
	EVALUATION[at0000] matches {    -- NRPE results
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Predicted 6-hour sodium (Na) output
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::384]>
								list = <
									["1"] = <
										units = <"mmol">
									>
								>
							>
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Predicted 6-hour urine output
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::129]>
								list = <
									["1"] = <
										units = <"mL">
									>
								>
							>
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Response Category
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0006] matches {*}    -- Item tree
		}
	}

