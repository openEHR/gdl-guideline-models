definition
	EVALUATION[at0000] matches {    -- Prostate cancer risk group assessment
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- T-stage
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- cT1c
									at0004,    -- cT1-cT2a
									at0005,    -- cT2b/cT2c
									at0006,    -- cT3a
									at0007]    -- cT3b/cT4
								}
							}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Gleason score
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0009,    -- 6 (ISUP 1)
									at0010,    -- 7 (3+4) (ISUP 2)
									at0011,    -- 7 (3+4) (ISUP 2)  in more than half of systematic biopsy specimens
									at0012,    -- 7 (4+3) (ISUP 3)
									at0013,    -- 8 (ISUP 4)
									at0014]    -- 9–10 (ISUP 5)
								}
							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- PSA
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0016,    -- <10 µg/l
									at0017,    -- 10-19 µg/l
									at0018,    -- 20-39 µg/l
									at0019]    -- ≥40 µg/l
								}
							}
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {    -- PSA density < 0.15 ug/l/cm3
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0021,    -- Yes
									at0022]    -- No
								}
							}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {    -- Histological findings ≤ 8 mm cancer in total in 1–4 of 8–12 systematic biopsy cores
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0021,    -- Yes
									at0022]    -- No
								}
							}
						}
					}
					ELEMENT[at0026] occurrences matches {0..1} matches {    -- Strong suspicion of extraprostatic growth on MRI (EPE/SVI 5)
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0021,    -- Yes
									at0022]    -- No
								}
							}
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {    -- Risk group
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0031,    -- Very low risk
									at0032,    -- Low risk
									at0033,    -- Favorable intermediate risk
									at0034,    -- Unfavorable intermediate risk
									at0035,    -- High risk
									at0036]    -- Very high risk
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0037] matches {*}    -- Item tree
		}
	}

