definition
	EVALUATION[at0000] matches {    -- Prostate cancer risk heredity assessment
		data matches {
			ITEM_TREE[at0003] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Absolute risk
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0005,    -- 5 %
									at0006,    -- 7 %
									at0007,    -- 9 %
									at0008,    -- 11 %
									at0009,    -- 13 %
									at0010,    -- 14 %
									at0011,    -- 15 %
									at0012,    -- 21 %
									at0013,    -- 23 %
									at0014,    -- 30 %
									at0015,    -- 44 %
									at0016,    -- 48 %
									at0017]    -- 64 %
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0002] occurrences matches {0..1} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

