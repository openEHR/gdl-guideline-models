definition
	EVALUATION[at0000] matches {    -- TNM classification for ovarian, fallopian tube and primary peritoneal carcinoma (clinical)
		data matches {
			ITEM_TREE[at0003] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- TNM stage group
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0005,    -- Stage I
									at0006,    -- Stage IA
									at0007,    -- Stage IB
									at0008,    -- Stage IC
									at0009,    -- Stage II
									at0010,    -- Stage IIA
									at0011,    -- Stage IIB
									at0012,    -- Stage IIC
									at0013,    -- Stage IIIA1
									at0014,    -- Stage IIIA2
									at0015,    -- Stage IIIB
									at0016,    -- Stage IIIC
									at0017,    -- Stage IV
									at0018,    -- Stage IVA
									at0019,    -- Stage IVB
									at0037]    -- Stage cannot be assigned
								}
							}
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {    -- FIGO stage
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0021,    -- Stage I
									at0022,    -- Stage IA
									at0023,    -- Stage IB
									at0024,    -- Stage IC
									at0025,    -- Stage II
									at0026,    -- Stage IIA
									at0027,    -- Stage IIB
									at0028,    -- Stage III
									at0029,    -- Stage IIIA1i
									at0030,    -- Stage IIIA1ii
									at0031,    -- Stage IIIA2
									at0032,    -- Stage IIIB
									at0033,    -- Stage IIIC
									at0034,    -- Stage IV
									at0035,    -- Stage IVA
									at0036,    -- Stage IVB
									at0038]    -- Stage cannot be assigned
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0002] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

