definition
	EVALUATION[at0000] matches {    -- TNM prostate cancer assessment
		data matches {
			ITEM_TREE[at0003] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Clinical T stage
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0007,    -- T0
									at0008,    -- Tx
									at0009,    -- T1
									at0010,    -- T1a
									at0011,    -- T1b
									at0012,    -- T1c
									at0013,    -- T2
									at0014,    -- T2a
									at0015,    -- T2b
									at0016,    -- T2c
									at0017,    -- T3
									at0018,    -- T3a
									at0019,    -- T3b
									at0020]    -- T4
								}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Clinical N stage
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0021,    -- N0
									at0022,    -- Nx
									at0023]    -- N1
								}
							}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Clinical M stage
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0024,    -- M0
									at0025,    -- M1
									at0026,    -- M1a
									at0027,    -- M1b
									at0028]    -- M1c
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0002] occurrences matches {0..1} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

