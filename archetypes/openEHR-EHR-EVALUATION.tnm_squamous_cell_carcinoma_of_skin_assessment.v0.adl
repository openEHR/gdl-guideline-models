definition
	EVALUATION[at0000] matches {    -- TNM squamous cell carcinoma of skin assessment
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0044] occurrences matches {0..1} matches {    -- Stage group
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0045,    -- Stage 0
									at0046,    -- Stage I
									at0047,    -- Stage IA
									at0048,    -- Stage IB
									at0049,    -- Stage II
									at0050,    -- Stage IIA
									at0051,    -- Stage IIB
									at0052,    -- Stage III
									at0053,    -- Stage IIIA
									at0054,    -- Stage IIIB
									at0055,    -- Stage IV
									at0056,    -- Stage IVA
									at0057]    -- Stage IVB
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0008] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0010] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

