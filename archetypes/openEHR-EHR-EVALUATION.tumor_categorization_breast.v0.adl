definition
	EVALUATION[at0000] matches {    -- Breast Cancer Tumor Categorization
		data matches {
			ITEM_TREE[at0002] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- ER status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0004,    -- Positive
									at0005]    -- Negative
								}
							}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- PR status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0007,    -- Positive
									at0008]    -- Negative
								}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- Ki-67 category
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0010,    -- Low ≤5%
									at0011,    -- Intermediate 6-29%
									at0012]    -- High ≥30%
								}
							}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- HER2 amplification
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0014,    -- Positive
									at0015]    -- Negative
								}
							}
						}
					}
					ELEMENT[at0016] occurrences matches {0..1} matches {    -- Molecular subtype
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0017,    -- Triple-negative
									at0018,    -- HER2-positive / Non-luminal
									at0019,    -- HER2-positive / Luminal
									at0020,    -- Low-risk Luminal (HER2-)
									at0021]    -- High-risk Luminal (HER2-)
								}
							}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {*}    -- Item tree
		}
	}

