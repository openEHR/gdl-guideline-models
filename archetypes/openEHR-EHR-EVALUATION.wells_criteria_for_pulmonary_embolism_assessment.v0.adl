definition
	EVALUATION[at0000] matches {    -- Wells Criteria for Pulmonary Embolism Assessment
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Two level clinical probability
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0006,    -- Pulmonary embolism unlikely
									at0007]    -- Pulmonary embolism likely
								}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Three level clinical probability
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0008,    -- Low
									at0009,    -- Moderate
									at0010]    -- High
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0011] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0012] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

