definition
	EVALUATION[at0000] matches {    -- Wisconsin criteria
		data matches {
			ITEM_TREE[at0003] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0011] occurrences matches {0..1} matches {    -- Wisconsin criteria
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0012,    -- Meets the criteria
									at0013]    -- Does not meet the criteria
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {*}    -- Item tree
		}
	}

