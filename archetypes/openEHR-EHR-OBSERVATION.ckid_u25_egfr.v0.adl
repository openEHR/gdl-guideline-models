	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the input parameters and the calculated result of the CKiD U25 eGFR equation.">
			keywords = <"ckid, egfr, gfr, pediatric, renal, kidney, nephrology, creatinine, bun", ...>
			copyright = <"© Cambio CDS">
			use = <"In children and young adults (< 25 years) with chronic kidney disease to estimate glomerular filtration rate.">
			misuse = <"Not to be used in adults aged 25 years or older; use the CKD-EPI equation instead. Not validated for assessing acute kidney injury (AKI) where creatinine levels are unstable. Do not use this archetype as the primary repository for individual data points; use dedicated archetypes such as openEHR-EHR-OBSERVATION.height.v0 for height and openEHR-EHR-OBSERVATION.lab_test-chemistry.v0 for storing serum creatinine and BUN results.">
