definition
	OBSERVATION[at0000] matches {    -- Genetically Inspired Prognostic Scoring System (GIPSS) for Primary Myelofibrosis (PMF)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {*}    -- Tree
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Karyotype classification
						value matches {
							0|[local::at0010],
							1|[local::at0011],
							2|[local::at0012]
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Type 1/like CALR mutation
						value matches {
							0|[local::at0013],
							1|[local::at0014]
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- ASXL1 mutation
						value matches {
							0|[local::at0015],
							1|[local::at0016]
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- SRSF2 mutation
						value matches {
							0|[local::at0017],
							1|[local::at0018]
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- U2AF1Q157 mutation
						value matches {
							0|[local::at0019],
							1|[local::at0020]
						}
					}
					ELEMENT[at0021] occurrences matches {0..1} matches {    -- Total score
						value matches {
							DV_COUNT matches {
								magnitude matches {|0..6|}
							}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {    -- Risk
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0023,    -- Low
									at0024,    -- Intermediate-1
									at0032,    -- Intermediate-2
									at0033]    -- High
								}
							}
						}
					}
					ELEMENT[at0026] occurrences matches {0..1} matches {    -- 5-year overall survival
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0027,    -- 94%
									at0028,    -- 73%
									at0029,    -- 40%
									at0030]    -- 14%
								}
							}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {    -- Median overall survival
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0034,    -- 26.4 years
									at0035,    -- 8.0 years
									at0036,    -- 4.2 years
									at0037]    -- 2 years
								}
							}
						}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Genetically Inspired Prognostic Scoring System (GIPSS) for Primary Myelofibrosis (PMF)">
					description = <"The Genetically Inspired Prognostic Scoring System (GIPPS) is a prognostic tool that relies exclusively on genetic markers to assess disease risk. It is designed for primary myelofibrosis (PMF), an aggressive myeloid malignancy with a median survival of approximately six years. Currently, allogeneic hematopoietic stem cell transplantation (alloSCT) remains the only potentially curative treatment for PMF. However, alloSCT carries significant risks of treatment-related morbidity and mortality, making personalized risk-benefit assessment essential before proceeding. GIPPS supports clinical decision-making by identifying patients in whom the potential benefits of alloSCT may outweigh the risks, thereby guiding more individualized and informed treatment strategies.">
				>
