	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Genetically Inspired Prognostic Scoring System (GIPSS) for Primary Myelofibrosis (PMF)">
					description = <"The Genetically Inspired Prognostic Scoring System (GIPPS) is a prognostic tool that relies exclusively on genetic markers to assess disease risk. It is designed for primary myelofibrosis (PMF), an aggressive myeloid malignancy with a median survival of approximately six years. GIPPS supports clinical decision-making by identifying PMF patients in whom the potential benefits of alloSCT may outweigh the risks, thereby guiding more individualized and informed treatment strategies.">
					comment = <"Original version, c 2018.">
