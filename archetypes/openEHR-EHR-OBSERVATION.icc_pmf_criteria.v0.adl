	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the individual findings relevant to the major and minor diagnostic criteria for PMF as specified by the ICC guidelines.">
			keywords = <"icc, international consensus classification, primary myelofibrosis, pmf, diagnosis, diagnostic criteria, myeloproliferative neoplasm, mpn, bone marrow, morphology, jak2, calr, mpl, asxl1, ezh2, tet2, idh1, idh2, srsf2, sf3b1, anemia, leukocytosis, splenomegaly, ldh, leukoerythroblastosis", ...>
			copyright = <"© Cambio CDS">
			use = <"Use to document the components required to assess the ICC criteria for PMF diagnosis. This includes specific bone marrow morphology findings, confirmation of exclusion of other myeloid neoplasms, relevant genetic mutation status, presence/absence of anemia, leukocytosis threshold, palpable splenomegaly, LDH level status, and leukoerythroblastosis. Designed to be used in conjunction with a GDL guideline (ICC_PMF_Dx.v0.0.1) to determine if the overall diagnostic criteria are met.">
			misuse = <"Not to be used for recording the diagnostic criteria of other myeloproliferative neoplasms (e.g., Polycythemia Vera, Essential Thrombocythemia) or other hematological conditions. Not intended for staging, prognosis, or treatment monitoring (use specific scoring systems like MIPSS70 or treatment response criteria archetypes for those purposes). Not to be used for recording the final diagnosis itself; use an appropriate EVALUATION archetype (e.g., EVALUATION.problem_diagnosis) for the confirmed diagnosis.">
		>
	>
