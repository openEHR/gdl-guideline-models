definition
	OBSERVATION[at0000] matches {    -- ICC Diagnostic Criteria for Primary Myelofibrosis
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Bone marrow biopsy showing megakaryocytic proliferation and atypia, bone marrow fibrosis grade <2, increased age-adjusted BM cellularity, granulocytic proliferation, and decreased erythropoiesis
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0007,    -- Yes
													at0008]    -- No
												}
											}
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- Bone marrow biopsy showing megakaryocytic proliferation and atypia, accompanied by reticulin and/or collagen fibrosis grades 2 or 3
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0010,    -- Yes
													at0011]    -- No
												}
											}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {    -- JAK2, CALR, or MPL mutation or presence of another clonal marker
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0013,    -- Yes
													at0014]    -- No
												}
											}
										}
									}
									ELEMENT[at0015] occurrences matches {0..1} matches {    -- Absence of reactive bone marrow reticulin fibrosis
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0016,    -- Yes
													at0017]    -- No
												}
											}
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- Absence of reactive myelofibrosis
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0019,    -- Yes
													at0020]    -- No
												}
											}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Diagnostic criteria for ET, PV, BCR::ABL1-positive CML, myelodysplastic syndrome, or other myeloid neoplasms are not met
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0022,    -- Yes
													at0023]    -- No
												}
											}
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {    -- Anemia not attributed to a comorbid condition
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0025,    -- Yes
													at0026]    -- No
												}
											}
										}
									}
									ELEMENT[at0027] occurrences matches {0..1} matches {    -- Leukocytosis ≥11 × 109/L
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0028,    -- Yes
													at0029]    -- No
												}
											}
										}
									}
									ELEMENT[at0030] occurrences matches {0..1} matches {    -- Palpable splenomegaly
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0031,    -- Yes
													at0032]    -- No
												}
											}
										}
									}
									ELEMENT[at0033] occurrences matches {0..1} matches {    -- Lactate dehydrogenase level above the reference range
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0034,    -- Yes
													at0035]    -- No
												}
											}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {    -- Leukoerythroblastosis
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0037,    -- Yes
													at0038]    -- No
												}
											}
										}
									}
									ELEMENT[at0039] occurrences matches {0..1} matches {    -- PMF, Early/Prefibrotic Stage (Pre-PMF)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0040,    -- Meets criteria
													at0041]    -- Does not meet criteria
												}
											}
										}
									}
									ELEMENT[at0042] occurrences matches {0..1} matches {    -- PMF, Overt Fibrotic Stage
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0043,    -- Meets criteria
													at0044]    -- Does not meet criteria
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

