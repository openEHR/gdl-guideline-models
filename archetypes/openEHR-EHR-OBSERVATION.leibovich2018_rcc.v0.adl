definition
	OBSERVATION[at0000] matches {    -- 2018 Leibovich Model for Renal Cell Carcinoma (RCC)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0113] occurrences matches {0..1} matches {    -- Type of RCC
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0114,    -- Clear cell RCC (ccRCC)
													at0115,    -- Papillary RCC (papRCC)
													at0116]    -- Chromophobe RCC (chrRCC)
												}
											}
										}
									}
									CLUSTER[at0127] occurrences matches {0..1} matches {    -- Clear cell RCC
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0128] occurrences matches {0..1} matches {    -- Age at surgery, years
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0129,    -- <60
															at0130]    -- >=60
														}
													}
												}
											}
											ELEMENT[at0131] occurrences matches {0..1} matches {    -- ECOG status
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0132,    -- 0
															at0133]    -- >=1
														}
													}
												}
											}
											ELEMENT[at0134] occurrences matches {0..1} matches {    -- Constitutional symptoms
												value matches {
													DV_BOOLEAN matches {*}
												}
											}
											ELEMENT[at0135] occurrences matches {0..1} matches {    -- Adrenalectomy
												value matches {
													DV_BOOLEAN matches {*}
												}
											}
											ELEMENT[at0136] occurrences matches {0..1} matches {    -- Surgical margins
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0137,    -- Negative
															at0138]    -- Positive
														}
													}
												}
											}
											ELEMENT[at0139] occurrences matches {0..1} matches {    -- Tumor grade
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0140,    -- 1
															at0141,    -- 2
															at0142,    -- 3
															at0143]    -- 4
														}
													}
												}
											}
											ELEMENT[at0144] occurrences matches {0..1} matches {    -- Coagulative necrosis
												value matches {
													DV_BOOLEAN matches {*}
												}
											}
											ELEMENT[at0145] occurrences matches {0..1} matches {    -- Sarcomatoid differentiation
												value matches {
													DV_BOOLEAN matches {*}
												}
											}
											ELEMENT[at0146] occurrences matches {0..1} matches {    -- Tumor size, cm
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0147,    -- <=4
															at0148,    -- >4 to <=7
															at0149,    -- >7 to <=10
															at0150]    -- >10
														}
													}
												}
											}
											ELEMENT[at0151] occurrences matches {0..1} matches {    -- Perinephric/renal sinus fat invasion
												value matches {
													DV_BOOLEAN matches {*}
												}
											}
											ELEMENT[at0152] occurrences matches {0..1} matches {    -- Tumor thrombus
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0153,    -- No
															at0154,    -- Level 0
															at0155]    -- Level 1-4
														}
													}
												}
											}
											ELEMENT[at0156] occurrences matches {0..1} matches {    -- Extension beyond kidney
												value matches {
													DV_BOOLEAN matches {*}
												}
											}
											ELEMENT[at0157] occurrences matches {0..1} matches {    -- Nodal involvement
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0158,    -- No nodal dissection
															at0159,    -- No
															at0160]    -- Yes
														}
													}
												}
											}
											ELEMENT[at0117] occurrences matches {0..1} matches {    -- ccRCC (CSS) score
												value matches {
													DV_COUNT matches {*}
												}
											}
											ELEMENT[at0118] occurrences matches {0..1} matches {    -- ccRCC (PFS) score
												value matches {
													DV_COUNT matches {*}
												}
											}
										}
									}
									CLUSTER[at0013] occurrences matches {0..1} matches {    -- Papillary RCC
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0119] occurrences matches {0..1} matches {    -- papRCC risk group
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0120,    -- Group 1
															at0121,    -- Group 2
															at0122]    -- Group 3
														}
													}
												}
											}
										}
									}
									CLUSTER[at0024] occurrences matches {0..1} matches {    -- Chromophobe RCC
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0123] occurrences matches {0..1} matches {    -- chrRCC risk group
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0124,    -- Group 1
															at0125,    -- Group 2
															at0126]    -- Group 3
														}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0111] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

