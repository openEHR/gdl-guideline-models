	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To capture key clinician-observed respiratory and thoracic findings that support assessment, staging, and diagnostic decision-making in suspected or confirmed lung cancer.">
			keywords = <"respiratory exam; lung cancer; auscultation; percussion; lymph nodes; supraclavicular; cervical; axillary", ...>
			copyright = <"Use Â© Cambio CDS">
			use = <"Use during physical examination in primary care, pulmonology, emergency assessment, or oncology workups to document respiratory sounds and lymph node palpation findings. Suitable for decision support, clinical documentation, and multidisciplinary evaluations.">
			misuse = <"Do not use to record patient-reported symptoms, imaging findings, or detailed neurological deficits. Not intended for documenting systemic examinations unrelated to respiratory or thoracic assessment.">
		>
	>
