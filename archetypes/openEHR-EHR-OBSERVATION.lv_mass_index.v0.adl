	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Use to record measurements of the left ventricular mass index for clinical evaluations, including cardiac hypertrophy diagnosis and cardiovascular risk assessment.">
			keywords = <"ventricular, mass, index, lvmi, cardiac, hypertrophy, cardiovascular, risk", ...>
			copyright = <"© Cambio CDS">
			use = <"Use to record the results for each component parameter and the calculated LV Mass Index. Additional information might provide guidance to modellers or implementers about how to utilise the archetype in templates or clinical systems.">
			misuse = <"Not to be used for recording information related to echocardiographic details beyond LV Mass Index calculations.">
		>
	>
