definition
	OBSERVATION[at0000] matches {    -- Myasthenia Gravis Foundation of America (MGFA) Clinical Classification
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Class
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0007,    -- Any ocular muscle weakness. All other muscle strength is normal.
													at0008,    -- Mild non-ocular muscle weakness.
													at0009,    -- Moderate non-ocular muscle weakness.
													at0010,    -- Severe non-ocular muscle weakness.
													at0011;    -- Defined by intubation, with or without mechanical ventilation.
													at0007]
												}
											}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {    -- Subclass
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0014,    -- Predominantly affecting limb, axial muscles, or both. May also have lesser involvement of oropharyngeal muscles.
													at0015]    -- Predominantly affecting oropharyngeal, respiratory muscles, or both. May also have lesser or equal involvement of limb, axial muscles, or both.
												}
											}
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- MGFA Classification System
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0019,    -- Class I
													at0020,    -- Class IIa
													at0021,    -- Class IIb
													at0022,    -- Class IIIa
													at0023,    -- Class IIIb
													at0024,    -- Class IVa
													at0025,    -- Class IVb
													at0026]    -- Class V
												}
											}
										}
									}
									ELEMENT[at0027] occurrences matches {0..1} matches {    -- Description
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0028,    -- Any ocular muscle weakness. May have weakness of eye closure. All other muscle strength is normal.
													at0029,    -- Mild non-ocular muscle weakness predominantly affecting limb, axial muscles, or both. May also have lesser involvement of oropharyngeal muscles and ocular muscle weakness of any severity.
													at0030,    -- Mild non-ocular muscle weakness predominantly affecting oropharyngeal, respiratory muscles, or both. May also have lesser or equal involvement of limb, axial muscles, or both, and ocular muscle weakness of any severity.
													at0031,    -- Moderate non-ocular muscle weakness predominantly affecting limb, axial muscles, or both. May also have lesser involvement of oropharyngeal muscles and ocular muscle weakness of any severity.
													at0032,    -- Moderate non-ocular muscle weakness predominantly affecting oropharyngeal, respiratory muscles, or both. May also have lesser or equal involvement of limb, axial muscles, or both, and ocular muscle weakness of any severity.
													at0033,    -- Severe non-ocular muscle weakness predominantly affecting limb and/or axial muscles. May also have lesser involvement of oropharyngeal muscles and ocular muscle weakness of any severity.
													at0034,    -- Severe non-ocular muscle weakness predominantly affecting oropharyngeal, respiratory muscles, or both. May also have lesser or equal involvement of limb, axial muscles, or both, and ocular muscle weakness of any severity.
													at0035]    -- Defined by intubation, with or without mechanical ventilation (except when used in routine postoperative management). Feeding tube without intubation = Class IVb.
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

