definition
	OBSERVATION[at0000] matches {    -- MIPSS70 Score
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0053] occurrences matches {0..1} matches {    -- Karyotype information
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0054,    -- Available
													at0055]    -- Not available
												}
											}
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- Hemoglobin <10 g/dL
										value matches {
											0|[local::at0008],
											1|[local::at0009]
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- Platelet count <100,000 /µL
										value matches {
											0|[local::at0011],
											2|[local::at0012]
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- Leukocyte count >25,000  /µL
										value matches {
											0|[local::at0014],
											2|[local::at0015]
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {    -- Circulating blasts ≥2%
										value matches {
											0|[local::at0017],
											1|[local::at0018]
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- Fibrosis grade ≥2
										value matches {
											0|[local::at0020],
											1|[local::at0021]
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Constitutional symptoms
										value matches {
											0|[local::at0023],
											1|[local::at0024]
										}
									}
									ELEMENT[at0025] occurrences matches {0..1} matches {    -- Absence of CALR type 1-like mutation
										value matches {
											0|[local::at0026],
											1|[local::at0027]
										}
									}
									ELEMENT[at0028] occurrences matches {0..1} matches {    -- HMR category
										value matches {
											0|[local::at0029],
											1|[local::at0030]
										}
									}
									ELEMENT[at0031] occurrences matches {0..1} matches {    -- Two or more HMR mutations
										value matches {
											0|[local::at0032],
											2|[local::at0033]
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {    -- Hemoglobin, mg/dL
										value matches {
											0|[local::at0035],
											1|[local::at0036],
											2|[local::at0037]
										}
									}
									ELEMENT[at0047] occurrences matches {0..1} matches {    -- Unfavorable karyotype
										value matches {
											0|[local::at0048],
											3|[local::at0049]
										}
									}
									ELEMENT[at0050] occurrences matches {0..1} matches {    -- Very high-risk karyotype
										value matches {
											0|[local::at0051],
											4|[local::at0052]
										}
									}
									ELEMENT[at0056] occurrences matches {0..1} matches {    -- Risk MIPSS70
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0057,    -- Very Low
													at0058,    -- Low
													at0059,    -- Intermediate
													at0060,    -- High
													at0061]    -- Very High
												}
											}
										}
									}
									ELEMENT[at0062] occurrences matches {0..1} matches {    -- Median Overall Survival
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0063,    -- N/A
													at0064,    -- 27.7
													at0065,    -- 10.3
													at0066,    -- 7.1
													at0067,    -- 7.0
													at0068,    -- 3.5
													at0069,    -- 2.3
													at0070]    -- 1.8
												}
											}
										}
									}
									ELEMENT[at0071] occurrences matches {0..1} matches {    -- Score
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0072,    -- MIPSS70+ v2.0 Score
													at0073]    -- MIPSS70
												}
											}
										}
									}
									ELEMENT[at0074] occurrences matches {0..1} matches {    -- Total points
										value matches {
											DV_COUNT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

