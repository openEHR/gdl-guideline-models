	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the component parameters and the total calculated scores for the MIPSS70 and MIPSS70+ v2.0 prognostic systems.">
			keywords = <"mipss70, mipss70+, mutation-enhanced international prognostic score system, primary myelofibrosis, pmf, prefibrotic myelofibrosis, prognosis, risk score, hematology, oncology, asxl1, ezh2, srsf2, idh1, idh2, calr, u2af1", ...>
			copyright = <"© Cambio CDS">
			use = <"Use to record the specific clinical findings, laboratory results thresholds, bone marrow fibrosis grade, and presence/absence/number of specific genetic mutations relevant to the MIPSS70 and MIPSS70+ v2.0 scores. Intended for use in patients diagnosed with prefibrotic/early or overt primary myelofibrosis. The GDL associated with this archetype will calculate the total score(s) and may provide risk stratification.">
			misuse = <"Not to be used for calculating prognosis in other myeloproliferative neoplasms or hematological malignancies. Not to be used solely for recording individual parameters (e.g., Hemoglobin level) outside the context of calculating this specific score; use dedicated archetypes for those purposes (e.g., openEHR-EHR-OBSERVATION.lab_test-haematology). Do not use for DIPSS or other myelofibrosis scoring systems.">
		>
	>
	other_details = <
		["licence"] = <"This work is licensed under the Creative Commons Attribution-ShareAlike 4.0 International License. To view a copy of this license, visit http://creativecommons.org/licenses/by-sa/4.0/.">
		["custodian_organisation"] = <"openEHR Foundation">
		["references"] = <"Guglielmelli P, Lasho TL, Rotunno G, Mudireddy M, Mannarelli C, Nicolosi M, Pacilli A, Pardanani A, Rumi E, Rosti V, Hanson CA, Mannelli F, Ketterling RP, Gangat N, Rambaldi A, Passamonti F, Barosi G, Barbui T, Cazzola M, Vannucchi AM, Tefferi A. MIPSS70: Mutation-Enhanced International Prognostic Score System for Transplantation-Age Patients With Primary Myelofibrosis. J Clin Oncol. 2018 Feb 1;36(4):310-318. doi: 10.1200/JCO.2017.76.4886. Epub 2017 Dec 9. PMID: 29226763.

Tefferi A, Guglielmelli P, Lasho TL, Gangat N, Ketterling RP, Pardanani A, Vannucchi AM. MIPSS70+ Version 2.0: Mutation and Karyotype-Enhanced International Prognostic Scoring System for Primary Myelofibrosis. J Clin Oncol. 2018 Jun 10;36(17):1769-1770. doi: 10.1200/JCO.2018.78.9867. Epub 2018 Apr 30. PMID: 29708808.">
