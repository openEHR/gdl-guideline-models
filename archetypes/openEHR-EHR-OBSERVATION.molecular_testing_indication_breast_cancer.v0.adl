definition
	OBSERVATION[at0000] matches {    -- Molecular Testing Indication for Breast Cancer
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- ER status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0008,    -- Positive
													at0009;    -- Negative
													at0009]
												}
											}
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- HER2 status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0011,    -- Positive
													at0012;    -- Negative
													at0012]
												}
											}
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {    -- Ki-67 category
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0014,    -- Low
													at0015,    -- Intermediate
													at0016;    -- High
													at0014]
												}
											}
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- NHG grade
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0018,    -- Grade 1
													at0019,    -- Grade 2
													at0020;    -- Grade 3
													at0018]
												}
											}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Menopausal status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0022,    -- Premenopausal
													at0023;    -- Postmenopausal
													at0022]
												}
											}
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {    -- Lymph node status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0025,    -- Node-negative (N0)
													at0026,    -- 1-3 positive lymph nodes (N1-3)
													at0027;    -- ≥4 positive lymph nodes (N≥4)
													at0025]
												}
											}
										}
									}
									ELEMENT[at0033] occurrences matches {0..1} matches {    -- Uncertain tumor risk categorization prior to selection of cytostatic treatment
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0028] occurrences matches {0..1} matches {    -- Possible candidate for PARP inhibitors
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0029] occurrences matches {0..1} matches {    -- Possible candidate for PI3K inhibitor
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0030] occurrences matches {0..1} matches {    -- Disease stage
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0031,    -- Early breast cancer
													at0032;    -- Metastatic breast cancer
													at0031]
												}
											}
										}
									}
									ELEMENT[at0050] occurrences matches {0..1} matches {    -- Eligible for ESCAT I targeted treatments
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0051] occurrences matches {0..1} matches {    -- Evidence-based treatment options exhausted
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0052] occurrences matches {0..1} matches {    -- Suitable for relevant clinical trials
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {    -- Gene expression indication
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0037,    -- Not applicable
													at0038,    -- Recommended
													at0039,    -- May be considered
													at0040]    -- With Oncotype Dx may be considered
												}
											}
										}
									}
									ELEMENT[at0041] occurrences matches {0..1} matches {    -- Germline BRCA1/2 testing indication
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0042,    -- Not applicable
													at0043]    -- Recommended
												}
											}
										}
									}
									ELEMENT[at0044] occurrences matches {0..1} matches {    -- Somatic PIK3CA testing indication
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0045,    -- Not applicable
													at0046]    -- Recommended
												}
											}
										}
									}
									ELEMENT[at0047] occurrences matches {0..1} matches {    -- Broad genomic profiling indication
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0048,    -- Not applicable
													at0049]    -- May be considered
												}
											}
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0006] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

