definition
	OBSERVATION[at0000] matches {    -- Natriuretic Response Prediction Equation (NRPE) results
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Predicted 6-hour sodium (Na) output
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::384]>
												list = <
													["1"] = <
														units = <"mmol">
													>
												>
											>
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- Predicted 6-hour urine output
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::129]>
												list = <
													["1"] = <
														units = <"mL">
													>
												>
											>
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- Response Category
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Natriuretic Response Prediction Equation (NRPE) results">
					description = <"An assessment equation used to predict early natriuretic response in adults hospitalized with acute decompensated heart failure (ADHF) receiving intravenous loop diuretics.">
					comment = <"Original version, c 2016">
				>
				["at0001"] = <
					text = <"History">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0005"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0006"] = <
					text = <"Predicted 6-hour sodium (Na) output">
					description = <"Calculated total sodium excreted over 6 hours predicted by the NRPE.">
				>
				["at0007"] = <
					text = <"Predicted 6-hour urine output">
					description = <"Calculated urine volume over 6 hours predicted by the NRPE.">
				>
				["at0008"] = <
					text = <"Response Category">
					description = <"Categorical interpretation of the predicted 6-hour sodium output. Used to classify patients into poor, suboptimal, or excellent natriuretic response groups, based on mmol Na thresholds.">
				>
			>
		>
	>
