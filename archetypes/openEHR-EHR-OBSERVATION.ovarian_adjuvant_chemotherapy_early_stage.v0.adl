definition
	OBSERVATION[at0000] matches {    -- Primary adjuvant chemotherapy for early-stage ovarian cancer
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Histology / differentiation grade
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0007,    -- High-grade serous (HGS)
													at0008,    -- Endometrioid - FIGO grade 1
													at0009,    -- Endometrioid – FIGO grade 2
													at0010,    -- Endometrioid – FIGO grade 3
													at0011,    -- Clear cell
													at0012,    -- Mucinous
													at0013,    -- Low-grade serous (LGS)
													at0014]    -- Undifferentiated / carcinosarcoma
												}
											}
										}
									}
									ELEMENT[at0015] occurrences matches {0..1} matches {    -- FIGO stage
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0016,    -- IA-B
													at0017,    -- IA-C
													at0018]    -- IC
												}
											}
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- Lymph node dissection status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0020,    -- Lymph node dissection indicated
													at0021,    -- Lymph node dissection not performed
													at0022]    -- Lymph node dissection not indicated
												}
											}
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {    -- Primary adjuvant treatment
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0024,    -- Carboplatin monotherapy (AUC 6), every 3 weeks for 6 cycles
													at0025,    -- Carboplatin (AUC 5) plus paclitaxel (175 mg/m²), every 3 weeks for 6 cycles
													at0026,    -- Carboplatin (AUC 5) plus paclitaxel (175 mg/m²), every 3 weeks for 6 cycles
													at0027,    -- No adjuvant chemotherapy recommended
													at0028]    -- Adjuvant chemotherapy cannot be determined based on available inputs
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

