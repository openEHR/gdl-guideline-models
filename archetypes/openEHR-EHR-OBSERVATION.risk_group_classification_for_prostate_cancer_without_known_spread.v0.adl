	openEHR-EHR-OBSERVATION.risk_group_classification_for_prostate_cancer_without_known_spread.v0

concept
	[at0000]	-- Risk group classification for prostate cancer without known spread.
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Kevin Arjona">
		["email"] = <"kevin.arjona@cambio.se">
		["organisation"] = <"Cambio CDS">
		["date"] = <"2025-07-15">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the necessary input parameters (T-stage, Gleason score, PSA) and the resulting risk group classification for patients with localized prostate cancer.">
			use = <"Use to document the risk factors in patients with prostate cancer confirmed to be without distant metastases to determine the patient's risk group ">
			keywords = <"prostate, cancer, risk, classification, T-stage, Gleason, score, ISUP, PSA, RCC, national guidelines, TNM.", ...>
			misuse = <"Not to be used for patients with known metastatic disease, recurrent prostate cancer after primary treatment, or for assessing the risk of other types of cancer. Do not use if any of the mandatory input parameters are unknown.">
			copyright = <"© Cambio CDS">
		>
	>
	lifecycle_state = <"NotSet">
	other_contributors = <>
	other_details = <
