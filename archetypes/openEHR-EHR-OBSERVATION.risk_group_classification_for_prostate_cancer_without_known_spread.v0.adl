definition
	OBSERVATION[at0000] matches {	-- Risk group classification for prostate cancer without known spread.
		data matches {
			HISTORY[at0001] matches {	-- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- T-stage
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0008, 	-- T1-T2a
													at0009, 	-- T2b/T2c
													at0010, 	-- T3a
													at0011, 	-- T3b/T4
													at0035]	-- T1c
												}
											}
										}
									}
									ELEMENT[at0039] occurrences matches {0..1} matches {	-- Strong suspicion of extraprostatic growth on MRI (EPE/SVI 5)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0040, 	-- Yes
													at0041]	-- No
												}
											}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Gleason score
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0013, 	-- 6 (ISUP 1)
													at0014, 	-- 7 (3+4) (ISUP 2)
													at0015, 	-- 7 (4+3) (ISUP 3)
													at0016, 	-- 8 (ISUP 4)
													at0017]	-- 9-10 (ISUP 5)
												}
											}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {	-- Histological findings ≤ 8 mm cancer in total in 1–4 of 8–12 systematic biopsy cores
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0037, 	-- Yes
													at0038]	-- No
												}
											}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {	-- Gleason score consistent in more than half of biopsy specimens
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0024, 	-- Yes
													at0025]	-- No
												}
											}
										}
									}
									ELEMENT[at0042] occurrences matches {0..1} matches {	-- PSA
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0043, 	-- < 10 µg/l
													at0044, 	-- 10–19 µg/l
													at0045, 	-- 20–39 µg/l
													at0046]	-- ≥ 40 µg/l
												}
											}
										}
									}
									ELEMENT[at0032] occurrences matches {0..1} matches {	-- PSA density < 0.15 ug/l/cm3
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0033, 	-- Yes
													at0034]	-- No
												}
											}
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {	-- Risk group classification
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0026, 	-- Very low risk
													at0027, 	-- Low risk
													at0028, 	-- Favorable intermediate risk
													at0029, 	-- Unfavorable intermediate risk
													at0030, 	-- High risk
													at0031]	-- Very high risk
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {	-- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

