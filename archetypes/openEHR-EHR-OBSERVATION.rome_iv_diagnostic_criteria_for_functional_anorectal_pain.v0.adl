	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the diagnostic criteria components used to determine Functional Anorectal Pain according to the Rome IV criteria.">
			keywords = <"functional anorectal pain, rome iv, proctalgia fugax, levator ani syndrome, anorectal disorder, chronic pelvic pain", ...>
			copyright = <"© Cambio CDS">
			use = <"Use to record the presence or absence of the Rome IV diagnostic criteria for Functional Anorectal Pain, including symptom duration, type of pain (proctalgia fugax or levator ani syndrome), and exclusion of organic anorectal diseases.">
			misuse = <"Not to be used for recording diagnoses related to organic anorectal disorders, such as hemorrhoids, rectal fissures, abscesses, infections, inflammatory bowel disease (IBD), or rectal cancer. Use instead openEHR-EHR-EVALUATION.anorectal_disorders.v0 or another appropriate archetype.">
		>
	>
