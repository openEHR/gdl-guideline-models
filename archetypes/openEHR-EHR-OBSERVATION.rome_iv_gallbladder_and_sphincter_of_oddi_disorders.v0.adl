	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the individual symptom characteristics, exclusionary findings, and specific investigational results required to assess a patient against the various Rome IV criteria for functional gallbladder and Sphincter of Oddi disorders.">
			keywords = <"rome iv, biliary pain,bp, gallbladder, disorder, fgd, functional, biliary, sphincter of oddi, disorder, pancreatic, fgid, diagnostic criteria, gastroenterology", ...>
			copyright = <"Â© Cambio CDS">
			use = <"Use to document the criteria for one or more specific functional biliary disorders. ">
			misuse = <"Not to be used for diagnosing disorders outside of this Rome IV category (e.g., Bowel or Gastroduodenal disorders) or for clear organic causes of pain like cholelithiasis, pancreatitis, or peptic ulcer disease. Not intended for recording the final diagnosis itself; use an appropriate EVALUATION.problem_diagnosis archetype for the confirmed diagnosis.">
		>
	>
