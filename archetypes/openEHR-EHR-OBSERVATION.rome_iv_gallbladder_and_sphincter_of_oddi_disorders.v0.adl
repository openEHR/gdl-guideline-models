definition
	OBSERVATION[at0000] matches {    -- Rome IV Gallbladder and Sphincter of Oddi Disorders
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0006] occurrences matches {0..1} matches {    -- E1. BILIARY PAIN
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0007] occurrences matches {0..1} matches {    -- Pain located in the epigastrium and/or right upper quadrant
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0008,    -- Yes
															at0009]    -- No
														}
													}
												}
											}
											ELEMENT[at0010] occurrences matches {0..1} matches {    -- Builds up to a steady level and lasting 30 minutes or longer
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0011,    -- Yes
															at0012]    -- No
														}
													}
												}
											}
											ELEMENT[at0013] occurrences matches {0..1} matches {    -- Occurs at different intervals (not daily)
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0014,    -- Yes
															at0015]    -- No
														}
													}
												}
											}
											ELEMENT[at0016] occurrences matches {0..1} matches {    -- Severe enough to interrupt daily activities or lead to an emergency department visit
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0017,    -- Yes
															at0018]    -- No
														}
													}
												}
											}
											ELEMENT[at0019] occurrences matches {0..1} matches {    -- Not significantly (<20%) related to bowel movements
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0020,    -- Yes
															at0021]    -- No
														}
													}
												}
											}
											ELEMENT[at0022] occurrences matches {0..1} matches {    -- Not significantly (<20%) relieved by postural change or acid suppression
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0023,    -- Yes
															at0024]    -- No
														}
													}
												}
											}
										}
									}
									CLUSTER[at0025] occurrences matches {0..1} matches {    -- E1a. Functional Gallbladder Disorder
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0026] occurrences matches {0..1} matches {    -- Biliary pain
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0027,    -- Yes
															at0028]    -- No
														}
													}
												}
											}
											ELEMENT[at0029] occurrences matches {0..1} matches {    -- Absence of gallstones or other structural pathology
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0030,    -- Yes
															at0031]    -- No
														}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..1} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

