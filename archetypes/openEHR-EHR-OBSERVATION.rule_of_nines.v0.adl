	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"rule, nines, burn, tbsa, thermal, injury, emergency, trauma, resuscitation,bsa, surface, area, burns, assessment, skin">
			keywords = <"To record the estimated burn percentage for each major body area and the calculated total TBSA.", ...>
			copyright = <"© Cambio CDS">
			use = <"Use in patients with second-degree (partial-thickness) or third-degree (full-thickness) thermal burns to estimate the extent of injury. This estimation guides fluid resuscitation protocols (e.g., using the Parkland formula) and transfer-to-burn-center decisions.">
			misuse = <"Not to be used to record the measured or calculated body surface area of an individual - use the OBSERVATION.body_surface_area for this purpose.

Not to be used to record the estimated proportion of body surface area affected by other conditions of the skin - use the parent archetype OBSERVATION.affected_body_surface_area for this purpose.">
