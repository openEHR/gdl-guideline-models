									ELEMENT[at0005] occurrences matches {0..1} matches {    -- Head and face
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0013,    -- 0%
													at0014,    -- 4.5%
													at0015]    -- 9%
