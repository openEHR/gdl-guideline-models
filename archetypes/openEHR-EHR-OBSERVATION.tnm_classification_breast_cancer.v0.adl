definition
	OBSERVATION[at0000] matches {    -- TNM Classification for Breast Cancer
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0059] occurrences matches {0..1} matches {    -- Primary tumour evaluable
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0060] occurrences matches {0..1} matches {    -- Primary tumour present
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Invasive tumour size
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::122]>
												list = <
													["1"] = <
														units = <"mm">
													>
												>
											>
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- Carcinoma in situ type
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0011,    -- None
													at0008,    -- DCIS
													at0009,    -- LCIS
													at0010]    -- Paget
												}
											}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {    -- Chest wall invasion
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {    -- Skin involvement
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- Inflammatory carcinoma
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- Regional lymph nodes evaluable
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0049] occurrences matches {0..1} matches {    -- Any regional node metastasis
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0062] occurrences matches {0..1} matches {    -- Nodal micrometastases only
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0050] occurrences matches {0..1} matches {    -- Axillary level I, II nodes involved
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0051] occurrences matches {0..1} matches {    -- Axillary node status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0052,    -- None
													at0053,    -- Movable
													at0054]    -- Fixed or Matted
												}
											}
										}
									}
									ELEMENT[at0055] occurrences matches {0..1} matches {    -- Clinically detected internal mammary nodes
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0056] occurrences matches {0..1} matches {    -- Clinically detected axillary nodes present
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0057] occurrences matches {0..1} matches {    -- Infraclavicular nodes involved
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0058] occurrences matches {0..1} matches {    -- Supraclavicular nodes involved
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {    -- Distant metastasis
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {    -- T - Primary tumour
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0024,    -- TX
													at0025,    -- T0
													at0026,    -- Tis
													at0027,    -- T1mi
													at0028,    -- T1a
													at0029,    -- T1b
													at0030,    -- T1c
													at0031,    -- T2
													at0032,    -- T3
													at0033,    -- T4a
													at0034,    -- T4b
													at0035,    -- T4c
													at0036]    -- T4d
												}
											}
										}
									}
									ELEMENT[at0037] occurrences matches {0..1} matches {    -- N - Regional lymph nodes
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0038,    -- NX
													at0039,    -- N0
													at0040,    -- N1
													at0041,    -- N2a
													at0042,    -- N2b
													at0043,    -- N3a
													at0044,    -- N3b
													at0045,    -- N3c
													at0061]    -- N1mi
												}
											}
										}
									}
									ELEMENT[at0046] occurrences matches {0..1} matches {    -- M - Distant metastasis
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0047,    -- M0
													at0048]    -- M1
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

