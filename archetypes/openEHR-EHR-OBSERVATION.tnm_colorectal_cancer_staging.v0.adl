	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the anatomical extent of colorectal cancer based on the Primary Tumor (T), Regional Lymph Nodes (N), and Distant Metastasis (M).">
			keywords = <"colorectal, cancer, colon, rectum, tnm, staging, uicc, oncology", ...>
			copyright = <"© Cambio CDS">
			use = <"Use to record the clinical (cTNM) or pathological (pTNM) classification.
T: Extent of invasion into the bowel wall.
N: Number of regional lymph nodes involved (and presence of tumor deposits - N1c).
M: Presence of distant metastasis (including peritoneal carcinomatosis).">
			misuse = <"Not to be used for other gastrointestinal malignancies. Use the proper arcehtypes for TNM classification.">
		>
	>
