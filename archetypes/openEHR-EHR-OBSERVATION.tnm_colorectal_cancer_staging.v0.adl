definition
	OBSERVATION[at0000] matches {    -- TNM colorectal cancer staging
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {    -- Primary Tumor
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0007,    -- No evidence of primary tumor
													at0008,    -- Primary tumor cannot be assessed
													at0009,    -- Carcinoma in situ: Intraepithelial or invasion into the lamina propria
													at0010,    -- The tumor invades the submucosa
													at0011,    -- Sm1: Invasion of 1/3 of the submucosa
													at0012,    -- Sm2: Invasion of 2/3 of the submucosa
													at0013,    -- Sm3: Invasion of 3/3 of the submucosa
													at0014,    -- The tumor invades through the muscularis propria into the pericolonic tissue
													at0015,    -- Minimal invasion: < 1 mm outside the boundaries of the muscularis propria
													at0016,    -- Slight invasion: 1–5 mm invasion outside the boundaries of the muscularis propria
													at0017,    -- Moderate invasion: > 5–15 mm outside the boundaries of the muscularis propria
													at0018,    -- Extensive invasion: > 15 mm outside the boundaries of the  propria
													at0019,    -- The tumor penetrates the surface of the visceral peritoneum
													at0020,    -- The tumor invades other organs or structures
													at0038]    -- The tumor invades the muscularis propria
												}
											}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Lymph Nodes
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0022,    -- No positive regional lymph nodes
													at0023,    -- Regional lymph nodes were not assessed
													at0031,    -- Metastases in 1–3 regional lymph nodes
													at0032,    -- Metastases in 1 regional lymph node
													at0033,    -- Metastases in 2–3 regional lymph nodes
													at0034,    -- Tumor deposits in the subserosa, mesentery, or non-peritonealized pericolic or perirectal tissue without regional lymph node metastasis
													at0035,    -- Metastases in 4 or more regional lymph nodes
													at0036,    -- Metastases in 4–6 regional lymph nodes
													at0037]    -- Metastases in 7 or more regional lymph nodes
												}
											}
										}
									}
									ELEMENT[at0025] occurrences matches {0..1} matches {    -- Distant Metastasis
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0026,    -- No distant metastasis
													at0027,    -- Distant metastasis
													at0028,    -- Metastasis to one organ or site but without peritoneal metastasis
													at0029,    -- Metastasis to more than one organ/site
													at0030]    -- Metastasis to the peritoneum with or without other metastasis
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..1} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

