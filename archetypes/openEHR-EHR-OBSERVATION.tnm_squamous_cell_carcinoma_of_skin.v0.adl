definition
	OBSERVATION[at0000] matches {    -- Clinical TNM classification for cutaneous squamous cell carcinoma of the skin
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0035] occurrences matches {0..1} matches {    -- TNM categories for head and neck skin
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0038] occurrences matches {0..1} matches {    -- T - Primary tumour
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0041,    -- TX - Primary tumour cannot be identified
															at0042,    -- T0 - No evidence of primary tumour
															at0043,    -- Tis - Carcinoma in situ
															at0044,    -- T1 - Tumour diameter ≤ 2.0 cm
															at0045,    -- T2 - Tumour diameter > 2 cm and ≤ 4 cm
															at0046,    -- T3 - Tumour > 4 cm or minor bone erosion or perineural invasion or deep invasion
															at0047,    -- T4a - Tumour with gross cortical bone/marrow invasion
															at0048]    -- T4b - Tumour with skull base or axial skeleton invasion including foraminal involvement and/or vertebral foramen involvement to the epidural space
														}
													}
												}
											}
											ELEMENT[at0039] occurrences matches {0..1} matches {    -- N - Lymph nodes
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0049,    -- N0 - No regional lymph node metastasis
															at0050,    -- N1 - Metastasis in single ipsilateral node ≤ 3 cm, no extranodal extension
															at0051,    -- N2a - Metastasis in single ipsilateral node > 3 cm and ≤ 6 cm, no extranodal extension
															at0052,    -- N2b - Metastasis in multiple ipsilateral nodes ≤ 6 cm, no extranodal extension
															at0053,    -- N2c - Metastasis in bilateral or contralateral nodes ≤ 6 cm, no extranodal extension
															at0054,    -- N3a - Metastasis in node > 6 cm, no extranodal extension
															at0055]    -- N3b - Metastasis in any node(s) with extranodal extension
														}
													}
												}
											}
											ELEMENT[at0040] occurrences matches {0..1} matches {    -- M - Distant metastasis
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0056,    -- M0 - No distant metastasis
															at0057]    -- M1 - Distant metastasis
														}
													}
												}
											}
										}
									}
									CLUSTER[at0037] occurrences matches {0..1} matches {    -- TNM categories for the skin of the eyelids
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0058] occurrences matches {0..1} matches {    -- T - Primary tumour
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0061,    -- T0 - No evidence of primary tumour
															at0062,    -- Tis - Carcinoma in situ
															at0063,    -- T1a - Tumour diameter ≤ 10 mm, no invasion of tarsal plate or eyelid margin
															at0064,    -- T1b - Tumour diameter ≤ 10 mm, invasion of tarsal plate or eyelid margin
															at0065,    -- T1c - Tumour diameter ≤ 10 mm, full-thickness eyelid involvement
															at0077,    -- T2a - Tumour > 10 mm and ≤ 20 mm, no invasion of tarsal plate or eyelid margin
															at0078,    -- T2b - Tumour > 10 mm and ≤ 20 mm, invasion of tarsal plate or eyelid margin
															at0079,    -- T2c - Tumour > 10 mm and ≤ 20 mm, full-thickness eyelid involvement
															at0080,    -- T3a - Tumour > 20 mm and ≤ 30 mm, no invasion of tarsal plate or eyelid margin
															at0081,    -- T3b - Tumour > 20 mm and ≤ 30 mm, invasion of tarsal plate or eyelid margin
															at0082,    -- T3c - Tumour > 20 mm and ≤ 30 mm, full-thickness eyelid involvement
															at0083,    -- T4a - Tumour invading ocular or intraorbital structures
															at0084]    -- T4b - Tumour invading orbital bone, paranasal sinus, lacrimal sac/duct, or brain
														}
													}
												}
											}
											ELEMENT[at0059] occurrences matches {0..1} matches {    -- N - Lymph nodes
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0073,    -- NX - Regional lymph nodes cannot be assessed
															at0074,    -- N0 - No evidence of lymph node involvement
															at0075,    -- N1 - Metastasis in a single ipsilateral node ≤ 3 cm
															at0076]    -- N2 - Metastasis in a single ipsilateral node > 3 cm or bilateral/contralateral nodal metastases
														}
													}
												}
											}
											ELEMENT[at0060] occurrences matches {0..1} matches {    -- M - Distant metastasis
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0071,    -- M0 - No distant metastasis
															at0072]    -- M1 - Distant metastasis
														}
													}
												}
											}
										}
									}
									CLUSTER[at0031] occurrences matches {0..1} matches {    -- TNM categories for non-head and neck skin
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0007] occurrences matches {0..1} matches {    -- T - Primary tumor
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0008,    -- Tx - Primary tumor cannot be identified
															at0009,    -- T0 - No evidence of primary tumour
															at0010,    -- Tis - Carcinoma in situ
															at0011,    -- T1 - Tumour diameter ≤ 2.0 cm
															at0012,    -- T2 - Tumour diameter > 2 cm and ≤ 4 cm
															at0013,    -- T3 - Tumour > 4 cm or minor bone erosion or perineural invasion or deep invasion
															at0014,    -- T4a - Tumour with gross cortical bone/marrow invasion
															at0015]    -- T4b - Tumour with axial skeleton invasion including foraminal involvement and/or vertebral foramen involvement to the epidural space
														}
													}
												}
											}
											ELEMENT[at0016] occurrences matches {0..1} matches {    -- N - Lymph nodes
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0017,    -- NX - Regional lymph nodes cannot be assessed
															at0018,    -- N0 - No regional lymph node metastasis
															at0019,    -- N1 - Single lymph node ≤ 3 cm
															at0020,    -- N2 - Node > 3 cm and ≤ 6 cm or multiple ipsilateral nodes ≤ 6 cm
															at0021]    -- N3 - Node > 6 cm
														}
													}
												}
											}
											ELEMENT[at0022] occurrences matches {0..1} matches {    -- M - Distant metastasis
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0023,    -- M0 - No distant metastasis
															at0024]    -- M1 - Distant metastatic disease
														}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0029] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

