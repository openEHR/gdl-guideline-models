  "actions" : [
    {
      "condition" : "T0_preliminary_investigation",
      "assignments" : [
        "prelscore_display.text = 'Score:' + T0_preliminary_investigation"
      ],
      "name" : "Set Display for Preliminary Investigation"
    },
    {
      "condition" : "!T0_vigilance || !T0_cough_or_throat_clearing || !T0_saliva_swallow || !T0_drooling || !T0_voice_change",
      "assignments" : [
        "indicator1_text.text = 'Please enter all variables'",
        "indicator_1.variant = \"info\""
      ],
      "name" : "Please Enter All (Preliminary)"
    },
    {
      "condition" : "T0_preliminary_investigation <= 4 && T0_preliminary_investigation >= 1",
      "assignments" : [
        "indicator_1.variant = \"warning\"",
        "indicator1_text.text = 'Investigate Further'"
      ],
      "name" : "Investigate Further (Preliminary)"
    },
    {
      "condition" : "T0_preliminary_investigation == 5",
      "assignments" : [
        "indicator_1.variant = \"success\"",
        "indicator1_text.text = 'Proceed to Direct Swallowing Test'"
      ],
      "name" : "Success Preliminary"
    },
    {
      "condition" : "T0_preliminary_investigation == 5",
      "assignments" : [
        "semisolid_display.hidden = false"
      ],
      "otherwise" : [
        "semisolid_display.hidden = true"
      ],
      "name" : "Display Semisolid"
    },
    {
      "condition" : "T0_semisolid_investigation_score",
      "assignments" : [
        "semisolidscore_display.text = 'Score:' + T0_semisolid_investigation_score"
      ],
      "name" : "Set Display for Semisolid Score"
    },
    {
      "condition" : "!T0_deglutition || !T0_cough_involuntary || !T0_drooling_1 || !T0_voice_change_1",
      "assignments" : [
        "indicator2_text.text = 'Please enter all variables'",
        "indicator2.variant = \"info\""
      ],
      "name" : "Please Enter All (Semisolid)"
    },
    {
      "condition" : "T0_semisolid_investigation_score <= 4 && T0_semisolid_investigation_score >= 1",
      "assignments" : [
        "indicator2.variant = \"warning\"",
        "indicator2_text.text = 'Investigate Further'"
      ],
      "name" : "Investigate Further Semisolid"
    },
    {
      "condition" : "T0_semisolid_investigation_score == 5",
      "assignments" : [
        "indicator2.variant = \"success\"",
        "indicator2_text.text = 'Proceed to Liquid Investigation'"
      ],
      "name" : "Success Semisolid"
    },
    {
      "condition" : "T0_semisolid_investigation_score == 5 && T0_preliminary_investigation == 5",
      "assignments" : [
        "liquid_display.hidden = false"
      ],
      "otherwise" : [
        "liquid_display.hidden = true"
      ],
      "name" : "Display Liquids"
    },
    {
      "condition" : "T0_liquid_investigation_score",
      "assignments" : [
        "liquidscore_display.text = 'Score:' + T0_liquid_investigation_score"
      ],
      "name" : "Set Display for Liquid"
    },
    {
      "condition" : "!T0_deglutition_1 || !T0_cough_involuntary_1 || !T0_drooling_2 || !T0_drooling_2",
      "assignments" : [
        "indicator3_text.text = 'Please enter all variables'",
        "indicator3.variant = \"info\""
      ],
      "name" : "Please Enter All (Liquid)"
    },
    {
      "condition" : "T0_liquid_investigation_score <= 4 && T0_liquid_investigation_score >= 1",
      "assignments" : [
        "indicator3_text.text = 'Investigate Further'",
        "indicator3.variant = \"warning\""
      ],
      "name" : "Investigate Further Liquid"
    },
    {
      "condition" : "T0_liquid_investigation_score == 5",
      "assignments" : [
        "indicator3_text.text = 'Proceed to Solid Investigation'",
        "indicator3.variant = \"success\""
      ],
      "name" : "Success Liquid"
    },
    {
      "condition" : "T0_preliminary_investigation == 5 && T0_semisolid_investigation_score == 5 && T0_liquid_investigation_score == 5",
      "assignments" : [
        "solid_display.hidden = false"
      ],
      "otherwise" : [
        "solid_display.hidden = true"
      ],
      "name" : "Display Solid"
    },
    {
      "condition" : "T0_solid_investigation_score",
      "assignments" : [
        "solidscore_display.text = 'Score:' + T0_solid_investigation_score"
      ],
      "name" : "Set Display for Solid"
    },
    {
      "condition" : "!T0_deglutition_2 || !T0_cough_involuntary_2 || !T0_drooling_3 || !T0_voice_change_3",
      "assignments" : [
        "indicator4_text.text = 'Please enter all variables'",
        "indicator4.variant = \"info\""
      ],
      "name" : "Please Enter All (Solid)"
    },
    {
      "condition" : "T0_solid_investigation_score <= 4 && T0_solid_investigation_score >= 1",
      "assignments" : [
        "indicator4_text.text = 'Investigate Further'",
        "indicator4.variant = \"warning\""
      ],
      "name" : "Investigate Further (Solid)"
    },
    {
      "condition" : "T0_solid_investigation_score == 5",
      "assignments" : [
        "indicator4_text.text = 'Test completed successfully'",
        "indicator4.variant = \"success\""
      ],
      "name" : "Success Solid"
    },
    {
      "condition" : "T0_total_score",
      "assignments" : [
        "totalscore_display.text = 'Total:' + T0_total_score"
      ],
      "name" : "Set Display Total Score"
    }
  ],
