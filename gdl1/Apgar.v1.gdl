				priority = <8>
			>
			["gt0020"] = (RULE) <
				when = <"$gt0003==1|local::at0011|Weak or irregular|",...>
				then = <"$gt0010=1|local::at0011|Weak or irregular|",...>
				priority = <9>
			>
			["gt0021"] = (RULE) <
