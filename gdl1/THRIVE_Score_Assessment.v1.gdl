				terms = <
					["gt0001"] = (TERM) <
						text = <"Thrive score assessment">
						description = <"THRIVE Score for Stroke Outcome provides an estimated prognosis after an acute ischemic stroke">
					>
					["gt0005"] = (TERM) <
						text = <"Total score">
						description = <"Total score">
					>
