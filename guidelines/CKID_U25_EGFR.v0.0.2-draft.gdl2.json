    "pre_conditions": [
      "$gt0004|Age|<=25"
    ],
    "rules": {
      "gt0013": {
        "id": "gt0013",
        "priority": 15,
        "when": [
          "$gt0003|Gender|==local::at0005|Male|",
          "$gt0004|Age|>=1",
          "$gt0004|Age|<12",
          "$gt0041|Presence|==local::at0018|Present|"
        ],
        "then": [
          "$gt0033|k|=39*(1.008^($gt0004|Age|.magnitude-12))"
        ]
      },
      "gt0015": {
        "id": "gt0015",
        "priority": 14,
        "when": [
          "$gt0003|Gender|==local::at0005|Male|",
          "$gt0004|Age|>=12&&$gt0004|Age|<18"
        ],
        "then": [
          "$gt0033|k|=39*(1.045^($gt0004|Age|.magnitude-12))"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 13,
        "when": [
          "$gt0003|Gender|==local::at0005|Male|",
          "$gt0004|Age|>=18&&$gt0004|Age|<25"
        ],
        "then": [
          "$gt0033|k|=50.8"
        ]
      },
      "gt0018": {
        "id": "gt0018",
        "priority": 12,
        "when": [
          "$gt0003|Gender|==local::at0006|Female|",
          "$gt0004|Age|>=1&&$gt0004|Age|<12"
        ],
        "then": [
          "$gt0033|k|=36.1*(1.008^($gt0004|Age|.magnitude-12))"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 11,
        "when": [
          "$gt0003|Gender|==local::at0006|Female|",
          "$gt0004|Age|>=12&&$gt0004|Age|<18"
        ],
        "then": [
          "$gt0033|k|=36.1*(1.023^($gt0004|Age|.magnitude-12))"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 10,
        "when": [
          "$gt0003|Gender|==local::at0006|Female|",
          "$gt0004|Age|>=18&&$gt0004|Age|<25"
        ],
        "then": [
          "$gt0033|k|=41.4"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 9,
        "when": [
          "$gt0003|Gender|==local::at0005|Male|",
          "$gt0004|Age|>=1&&$gt0004|Age|<15"
        ],
        "then": [
          "$gt0033|k|=87.2*(1.011^($gt0004|Age|.magnitude-15))"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 8,
        "when": [
          "$gt0003|Gender|==local::at0005|Male|",
          "$gt0004|Age|>=15&&$gt0004|Age|<18"
        ],
        "then": [
          "$gt0033|k|=87.2*(0.96^($gt0004|Age|.magnitude-15))"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 7,
        "when": [
          "$gt0003|Gender|==local::at0005|Male|",
          "$gt0004|Age|>=18&&$gt0004|Age|<25"
        ],
        "then": [
          "$gt0033|k|=77.1"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 6,
        "when": [
          "$gt0003|Gender|==local::at0006|Female|",
          "$gt0004|Age|>=1&&$gt0004|Age|<15"
        ],
        "then": [
          "$gt0033|k|=79.9*(1.004^($gt0004|Age|.magnitude-12))"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 5,
        "when": [
          "$gt0003|Gender|==local::at0006|Female|",
          "$gt0004|Age|>=12&&$gt0004|Age|<18"
        ],
        "then": [
          "$gt0033|k|=79.9*(0.974^($gt0004|Age|.magnitude-12))"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 4,
        "when": [
          "$gt0003|Gender|==local::at0006|Female|",
          "$gt0004|Age|>=18&&$gt0004|Age|<25"
        ],
        "then": [
          "$gt0033|k|=68.3"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 1,
        "then": [
          "$gt0030|sCr-based eGFR|.precision=2",
          "$gt0039|Estimated Glomerular Filtration Rate|.magnitude=($gt0030|sCr-based eGFR|.magnitude-$gt0037|cycC-based eGFR|.magnitude)/2"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 3,
        "then": [
          "$gt0030|sCr-based eGFR|.magnitude=$gt0033|k|*(($gt0006|Height/Length|.magnitude/100)/$gt0008|Serum creatinine|.magnitude)"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 2,
        "then": [
          "$gt0037|cycC-based eGFR|.magnitude=$gt0033|k|*(1/$gt0010|Cystatin C|.magnitude)"
        ]
      }
    }
  },
