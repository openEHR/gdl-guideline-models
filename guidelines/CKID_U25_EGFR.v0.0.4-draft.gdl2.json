    "pre_conditions" : [
      "$gt0003|Equation|!=null",
      "$gt0019|Gender|!=null",
      "$gt0020|Age|!=null"
    ],
    "rules" : {
      "gt0017" : {
        "id" : "gt0017",
        "priority" : 1,
        "when" : [
          "$gt0003|Equation|==local::at0007|CKiD U25 Creatinine|",
          "$gt0019|Gender|==local::at0005|Male|",
          "$gt0020|Age|.magnitude>=1",
          "$gt0020|Age|.magnitude<12"
        ],
        "then" : [
          "$gt0010|k|=39*(1.008^($gt0020|Age|.magnitude-12))"
        ]
      }
    }
