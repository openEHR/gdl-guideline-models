    "pre_conditions" : [
      "$gt0003|Equation|!=null",
      "$gt0019|Gender|!=null",
      "$gt0020|Age|!=null"
    ],
    "rules" : {
      "gt0017" : {
        "id" : "gt0017",
        "priority" : 15,
        "when" : [
          "$gt0003|Equation|==local::at0007|CKiD U25 Creatinine|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0005|Male|",
          "$gt0020|Age|.magnitude>=1",
          "$gt0020|Age|.magnitude<12"
        ],
        "then" : [
          "$gt0010|k|=39*(1.008^($gt0020|Age|.magnitude-12))"
        ]
      },
      "gt0027" : {
        "id" : "gt0027",
        "priority" : 14,
        "when" : [
          "$gt0003|Equation|==local::at0007|CKiD U25 Creatinine|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0005|Male|",
          "$gt0020|Age|.magnitude>=12",
          "$gt0020|Age|.magnitude<18"
        ],
        "then" : [
          "$gt0010|k|=39*(1.045^($gt0020|Age|.magnitude-12))"
        ]
      },
      "gt0028" : {
        "id" : "gt0028",
        "priority" : 13,
        "when" : [
          "$gt0003|Equation|==local::at0007|CKiD U25 Creatinine|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0005|Male|",
          "$gt0020|Age|.magnitude>=18",
          "$gt0020|Age|.magnitude<25"
        ],
        "then" : [
          "$gt0010|k|=50.8"
        ]
      },
      "gt0029" : {
        "id" : "gt0029",
        "priority" : 12,
        "when" : [
          "$gt0003|Equation|==local::at0007|CKiD U25 Creatinine|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0006|Female|",
          "$gt0020|Age|.magnitude>=1",
          "$gt0020|Age|.magnitude<12"
        ],
        "then" : [
          "$gt0010|k|=36.1*(1.008^($gt0020|Age|.magnitude-12))"
        ]
      },
      "gt0030" : {
        "id" : "gt0030",
        "priority" : 11,
        "when" : [
          "$gt0003|Equation|==local::at0007|CKiD U25 Creatinine|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0006|Female|",
          "$gt0020|Age|.magnitude>=12",
          "$gt0020|Age|.magnitude<18"
        ],
        "then" : [
          "$gt0010|k|=36.1*(1.023^($gt0020|Age|.magnitude-12))"
        ]
      },
      "gt0031" : {
        "id" : "gt0031",
        "priority" : 10,
        "when" : [
          "$gt0003|Equation|==local::at0007|CKiD U25 Creatinine|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0006|Female|",
          "$gt0020|Age|.magnitude>=18",
          "$gt0020|Age|.magnitude<25"
        ],
        "then" : [
          "$gt0010|k|=41.4"
        ]
      },
      "gt0032" : {
        "id" : "gt0032",
        "priority" : 9,
        "when" : [
          "$gt0003|Equation|==local::at0008|CKiD U25 Cystatin C|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0005|Male|",
          "$gt0020|Age|.magnitude>=1",
          "$gt0020|Age|.magnitude<15"
        ],
        "then" : [
          "$gt0010|k|=87.2*(1.011^($gt0020|Age|.magnitude-15))"
        ]
      },
      "gt0033" : {
        "id" : "gt0033",
        "priority" : 8,
        "when" : [
          "$gt0003|Equation|==local::at0008|CKiD U25 Cystatin C|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0005|Male|",
          "$gt0020|Age|.magnitude>=15",
          "$gt0020|Age|.magnitude<18"
        ],
        "then" : [
          "$gt0010|k|=87.2*(0.96^($gt0020|Age|.magnitude-15))"
        ]
      },
      "gt0034" : {
        "id" : "gt0034",
        "priority" : 7,
        "when" : [
          "$gt0003|Equation|==local::at0008|CKiD U25 Cystatin C|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0005|Male|",
          "$gt0020|Age|.magnitude>=18",
          "$gt0020|Age|.magnitude<25"
        ],
        "then" : [
          "$gt0010|k|=77.1"
        ]
      },
      "gt0035" : {
        "id" : "gt0035",
        "priority" : 6,
        "when" : [
          "$gt0003|Equation|==local::at0008|CKiD U25 Cystatin C|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0006|Female|",
          "$gt0020|Age|.magnitude>=1",
          "$gt0020|Age|.magnitude<12"
        ],
        "then" : [
          "$gt0010|k|=79.9*(1.004^($gt0020|Age|.magnitude-12))"
        ]
      },
      "gt0036" : {
        "id" : "gt0036",
        "priority" : 5,
        "when" : [
          "$gt0003|Equation|==local::at0008|CKiD U25 Cystatin C|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0006|Female|",
          "$gt0020|Age|.magnitude>=12",
          "$gt0020|Age|.magnitude<18"
        ],
        "then" : [
          "$gt0010|k|=79.9*(0.974^($gt0020|Age|.magnitude-12))"
        ]
      },
      "gt0037" : {
        "id" : "gt0037",
        "priority" : 4,
        "when" : [
          "$gt0003|Equation|==local::at0008|CKiD U25 Cystatin C|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0019|Gender|==local::at0006|Female|",
          "$gt0020|Age|.magnitude>=18",
          "$gt0020|Age|.magnitude<25"
        ],
        "then" : [
          "$gt0010|k|=68.3"
        ]
      },
      "gt0039" : {
        "id" : "gt0039",
        "priority" : 3,
        "when" : [
          "$gt0019|Gender|!=null",
          "$gt0020|Age|!=null",
          "$gt0026|Height/Length|!=null",
          "$gt0022|Serum creatinine|!=null",
          "$gt0003|Equation|==local::at0007|CKiD U25 Creatinine|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|"
        ],
        "then" : [
          "$gt0041|Estimated GFR by CKiD U25 Creatinine|.magnitude=$gt0010|k|*(($gt0026|Height/Length|.magnitude/100)/$gt0022|Serum creatinine|.magnitude)"
        ]
      },
      "gt0038" : {
        "id" : "gt0038",
        "priority" : 2,
        "when" : [
          "$gt0019|Gender|!=null",
          "$gt0020|Age|!=null",
          "$gt0026|Height/Length|!=null",
          "$gt0003|Equation|==local::at0008|CKiD U25 Cystatin C|||$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|",
          "$gt0024|Cystatin C|!=null"
        ],
        "then" : [
          "$gt0014|Estimated GFR by CKiD U25 Cystatin C|.magnitude=$gt0010|k|*(1/$gt0024|Cystatin C|.magnitude)"
        ]
      },
      "gt0040" : {
        "id" : "gt0040",
        "priority" : 1,
        "when" : [
          "$gt0041|Estimated GFR by CKiD U25 Creatinine|.magnitude!=null",
          "$gt0014|Estimated GFR by CKiD U25 Cystatin C|!=null",
          "$gt0003|Equation|==local::at0009|CKiD U25 Creatinine-Cystatin C|"
        ],
        "then" : [
          "$gt0044|Estimated GFR by CKiD U25 Creatinine-Cystatin C|=($gt0041|Estimated GFR by CKiD U25 Creatinine|.magnitude+$gt0014|Estimated GFR by CKiD U25 Cystatin C|.magnitude)/2"
        ]
      }
    }
  },
