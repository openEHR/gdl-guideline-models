  "ontology": {
    "term_definitions": {
      "en": {
        "id": "en",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "DSM-5 criteria for Post-Traumatic Stress Disorder (PTSD)",
            "description": "An assessment score that assists the diagnosis of Post-Traumatic Stress Disorder (PTSD) using the Diagnostic and Statistical Manual of Mental Disorders (DSM-5) Criteria."
          },
          "gt0033": {
            "id": "gt0033",
            "text": "Intrusion symptoms Score",
            "description": "*"
          },
          "gt0047": {
            "id": "gt0047",
            "text": "Interpretation",
            "description": ""
          },
          "gt0048": {
            "id": "gt0048",
            "text": "No PTSD diagnosis rule"
          },
          "gt0049": {
            "id": "gt0049",
            "text": "PTSD diagnosis rule"
          },
          "gt0050": {
            "id": "gt0050",
            "text": "Interpretation text",
            "description": ""
          },
          "gt0051": {
            "id": "gt0051",
            "text": "Diagnosis of Posttraumatic Stress Disorder (PTSD)",
            "description": ""
          },
          "gt0052": {
            "id": "gt0052",
            "text": "No Posttraumatic Stress Disorder (PTSD)",
            "description": ""
          },
          "gt0053": {
            "id": "gt0053",
            "text": "Interpretation rule"
          },
          "gt0054": {
            "id": "gt0054",
            "text": "With dissociative symptoms == Depersonalization rule"
          },
          "gt0055": {
            "id": "gt0055",
            "text": "With delayed expression rule"
          },
          "gt0059": {
            "id": "gt0059",
            "text": "Dissociative symptoms interpretation text",
            "description": ""
          },
          "gt0060": {
            "id": "gt0060",
            "text": "with dissociative symptoms (Depersonalization)",
            "description": ""
          },
          "gt0061": {
            "id": "gt0061",
            "text": "with delayed expression (6 or more months after traumatic event)",
            "description": ""
          },
          "gt0062": {
            "id": "gt0062",
            "text": "With dissociative symptoms == Derealization rule"
          },
          "gt0063": {
            "id": "gt0063",
            "text": "with dissociative symptoms (Derealization)",
            "description": ""
          },
          "gt0064": {
            "id": "gt0064",
            "text": "with dissociative symptoms (Depersonalization and Derealization)",
            "description": ""
          },
          "gt0065": {
            "id": "gt0065",
            "text": "Delayed expression interpretation text",
            "description": ""
          },
          "gt0066": {
            "id": "gt0066",
            "text": "With dissociative symptoms == both"
          },
          "gt0069": {
            "id": "gt0069",
            "text": "Interpretation with DS subset rule"
          },
          "gt0070": {
            "id": "gt0070",
            "text": "Interpretation with DE subset rule"
          },
          "gt0071": {
            "id": "gt0071",
            "text": "Interpretation with DS and DE subset rule"
          },
          "gt0073": {
            "id": "gt0073",
            "text": "Direct experience",
            "description": "Directly experiencing the traumatic event(s)."
          },
          "gt0074": {
            "id": "gt0074",
            "text": "Witnessing the event",
            "description": "Witnessing, in person, the event(s) as it occurred to others."
          },
          "gt0075": {
            "id": "gt0075",
            "text": "Traumatic event occurred to family or friend",
            "description": "Learning that the traumatic event(s) occurred to a close family member or close friend. In cases of actual or threatened death of a family member or friend, the event(s) must have been violent or accidental."
          },
          "gt0076": {
            "id": "gt0076",
            "text": "Repetitive or extreme exposure",
            "description": "Experiencing repeated or extreme exposure to aversive details of the traumatic event(s) (e.g., first responders collecting human remains; police officers repeatedly exposed to details of child abuse).\n\nNote: Criterion A4 does not apply to exposure through electronic media, television, movies, or pictures, unless this exposure is work related."
          },
          "gt0077": {
            "id": "gt0077",
            "text": "Exposure score"
          },
          "gt0078": {
            "id": "gt0078",
            "text": "Memories of the traumatic event",
            "description": "Recurrent, involuntary, and intrusive distressing memories of the traumatic event(s).\n\nNote: In children older than 6 years, repetitive play may occur in which themes or aspects of the traumatic event(s) are expressed."
          },
          "gt0079": {
            "id": "gt0079",
            "text": "Distressing dreams of the traumatic event",
            "description": "Recurrent distressing dreams in which the content and/or affect of the dream are related to the traumatic event(s).\n\nNote: In children, there may be frightening dreams without recognizable content."
          },
          "gt0080": {
            "id": "gt0080",
            "text": "Dissociative reactions",
            "description": "Dissociative reactions (e.g., flashbacks) in which the individual feels or acts as if the traumatic event(s) were recurring. (Such reactions may occur on a continuum, with the most extreme expression being a complete loss of awareness of present surroundings.)\n\nNote: In children, trauma-specific reenactment may occur in play."
          },
          "gt0081": {
            "id": "gt0081",
            "text": "Intense or prolonged psychological distress at exposure",
            "description": "Intense or prolonged psychological distress at exposure to internal or external cues that symbolize or resemble an aspect of the traumatic event(s)."
          },
          "gt0082": {
            "id": "gt0082",
            "text": "Marked physiological reactions",
            "description": "Marked physiological reactions to internal or external cues that symbolize or resemble an aspect of the traumatic event(s)."
          },
          "gt0083": {
            "id": "gt0083",
            "text": "Intrusion symptoms score"
          },
          "gt0084": {
            "id": "gt0084",
            "text": "Efforts to avoid distressing memories thoughts or feelings",
            "description": "Avoidance of or efforts to avoid distressing memories, thoughts, or feelings about or closely associated with the traumatic event(s)."
          },
          "gt0085": {
            "id": "gt0085",
            "text": "Efforts to avoid external reminders",
            "description": "Avoidance of or efforts to avoid external reminders (people, places, conversations, activities, objects, situations) that arouse distressing memories, thoughts, or feelings about or closely associated with the traumatic event(s)."
          },
          "gt0086": {
            "id": "gt0086",
            "text": "Avoidance score"
          },
          "gt0088": {
            "id": "gt0088",
            "text": "Exposure Score",
            "description": "*"
          },
          "gt0089": {
