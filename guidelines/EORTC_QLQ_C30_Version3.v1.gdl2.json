    "rules": {
      "gt0135": {
        "id": "gt0135",
        "priority": 34,
        "when": [
          "$gt0004|1. Do you need help with eating, dressing, washing  |!=null",
          "$gt0005|2. Do you need to stay in bed or a chair during the day? |!=null",
          "$gt0006|3. Do you have any trouble taking a short walk outside of the house?|!=null",
          "$gt0007|4. Do you have any trouble taking a long walk?|!=null",
          "$gt0008|5. Do you have any trouble doing strenuous activities,  |!=null"
        ],
        "then": [
          "$gt0065|Physical functioning RS (revised)|.precision=2",
          "$gt0065|Physical functioning RS (revised)|.magnitude=(((($gt0004.value+$gt0005.value)+$gt0006.value)+$gt0007.value)+$gt0008.value)/5.0"
        ]
      },
      "gt0136": {
        "id": "gt0136",
        "priority": 33,
        "when": [
          "$gt0010|6. Were you limited in doing either your work or other daily activities?|!=null",
          "$gt0011|7. Were you limited in pursuing your hobbies or other |!=null"
        ],
        "then": [
          "$gt0066|Role functioning RS (revised)|.precision=2",
          "$gt0066|Role functioning RS (revised)|.magnitude=($gt0011.value+$gt0010.value)/2.0"
        ]
      },
      "gt0137": {
        "id": "gt0137",
        "priority": 32,
        "when": [
          "$gt0025|21. Did you feel tense?|!=null",
          "$gt0026|22. Did you worry?|!=null",
          "$gt0027|23. Did you feel irritable?|!=null",
          "$gt0028|24. Did you feel depressed?|!=null"
        ],
        "then": [
          "$gt0067|Emotional functioning RS|.precision=2",
          "$gt0067|Emotional functioning RS|.magnitude=((($gt0025.value+$gt0026.value)+$gt0027.value)+$gt0028.value)/4.0"
        ]
      },
      "gt0138": {
        "id": "gt0138",
        "priority": 31,
        "when": [
          "$gt0024|20. Have you had difficulty in concentrating on things, like reading a newspaper or watching tv?|!=null",
          "$gt0029|25. Have you had difficulty remembering things?|!=null"
        ],
        "then": [
          "$gt0068|Cognitive functioning RS|.precision=2",
          "$gt0068|Cognitive functioning RS|.magnitude=($gt0029.value+$gt0024.value)/2.0"
        ]
      },
      "gt0134": {
        "id": "gt0134",
        "priority": 30,
        "when": [
          "$gt0030|26. Has your physical condition or medical treatment interfered with your family life?|!=null",
          "$gt0031|27. Has your physical condition or medical treatment interfered with your social activities?|!=null"
        ],
        "then": [
          "$gt0069|Social functioning RS|.precision=2",
          "$gt0069|Social functioning RS|.magnitude=($gt0031.value+$gt0030.value)/2.0"
        ]
      },
      "gt0130": {
        "id": "gt0130",
        "priority": 29,
        "when": [
          "$gt0011|7. Were you limited in pursuing your hobbies or other |!=null",
          "$gt0010|6. Were you limited in doing either your work or other daily activities?|!=null",
          "$gt0025|21. Did you feel tense?|!=null",
          "$gt0026|22. Did you worry?|!=null",
          "$gt0027|23. Did you feel irritable?|!=null",
          "$gt0028|24. Did you feel depressed?|!=null",
          "$gt0004|1. Do you need help with eating, dressing, washing  |!=null",
          "$gt0005|2. Do you need to stay in bed or a chair during the day? |!=null",
          "$gt0006|3. Do you have any trouble taking a short walk outside of the house?|!=null",
          "$gt0007|4. Do you have any trouble taking a long walk?|!=null",
          "$gt0008|5. Do you have any trouble doing strenuous activities,  |!=null",
          "$gt0024|20. Have you had difficulty in concentrating on things, like reading a newspaper or watching tv?|!=null",
          "$gt0029|25. Have you had difficulty remembering things?|!=null",
          "$gt0030|26. Has your physical condition or medical treatment interfered with your family life?|!=null",
          "$gt0031|27. Has your physical condition or medical treatment interfered with your social activities?|!=null"
        ],
        "then": [
          "$gt0064|Functional raw score RS|.precision=2",
          "$gt0064|Functional raw score RS|.magnitude=(((((((((((((($gt0011.value+$gt0010.value)+$gt0025.value)+$gt0026.value)+$gt0027.value)+$gt0028.value)+$gt0004.value)+$gt0005.value)+$gt0006.value)+$gt0007.value)+$gt0008.value)+$gt0029.value)+$gt0024.value)+$gt0031.value)+$gt0030.value)/15.0"
        ]
      },
      "gt0132": {
        "id": "gt0132",
        "priority": 28,
        "when": [
          "$gt0033|29. How would you rate your overall health|!=null",
          "$gt0009|30. How would you rate your overall quality of life|!=null"
        ],
        "then": [
          "$gt0096|Global Health Status Raw Score|.precision=2",
          "$gt0096|Global Health Status Raw Score|.magnitude=($gt0033.value+$gt0009.value)/2.0"
        ]
      },
      "gt0139": {
        "id": "gt0139",
        "priority": 27,
        "when": [
          "$gt0022|18. Were you tired? |!=null",
          "$gt0014|10. Did you need to rest?|!=null",
          "$gt0016|12. Have you felt weak?|!=null"
        ],
        "then": [
          "$gt0077|Fatigue RS|.precision=2",
          "$gt0077|Fatigue RS|.magnitude=(($gt0022.value+$gt0014.value)+$gt0016.value)/3.0"
        ]
      },
      "gt0140": {
        "id": "gt0140",
        "priority": 26,
        "when": [
          "$gt0018|14. Have you felt nauseated? |!=null",
          "$gt0019|15. Have you vomited|!=null"
        ],
        "then": [
          "$gt0078|Nausea and vomiting RS|.precision=2",
          "$gt0078|Nausea and vomiting RS|.magnitude=($gt0018.value+$gt0019.value)/2.0"
        ]
      },
      "gt0141": {
        "id": "gt0141",
        "priority": 25,
        "when": [
          "$gt0013|9. Have you had pain? |!=null",
          "$gt0023|19. Did pain interfere with your daily activities?|!=null"
        ],
        "then": [
          "$gt0079|Pain RS|.precision=2",
          "$gt0079|Pain RS|.magnitude=($gt0023.value+$gt0013.value)/2.0"
        ]
      },
      "gt0142": {
        "id": "gt0142",
        "priority": 24,
        "when": [
          "$gt0012|8.Were you short of breath? |!=null"
        ],
        "then": [
          "$gt0080|Dyspnoea RS|.precision=2",
          "$gt0080|Dyspnoea RS|.magnitude=$gt0012.value"
        ]
      },
      "gt0143": {
        "id": "gt0143",
        "priority": 23,
        "when": [
          "$gt0015|11. Have you had trouble sleeping?|!=null"
        ],
        "then": [
          "$gt0081|Insomnia RS|.precision=2",
          "$gt0081|Insomnia RS|.magnitude=$gt0015.value"
        ]
      },
      "gt0144": {
        "id": "gt0144",
        "priority": 22,
        "when": [
          "$gt0017|13. Have you lacked appetite?|!=null"
        ],
        "then": [
          "$gt0082|Appetite loss RS|.precision=2",
          "$gt0082|Appetite loss RS|.magnitude=$gt0017.value"
        ]
      },
      "gt0145": {
        "id": "gt0145",
        "priority": 21,
        "when": [
          "$gt0020|16. Have you been constipated? |!=null"
        ],
        "then": [
          "$gt0083|Constipation RS|.precision=2",
          "$gt0083|Constipation RS|.magnitude=$gt0020.value"
        ]
      },
      "gt0146": {
        "id": "gt0146",
        "priority": 20,
        "when": [
          "$gt0021|17. Have you had diarrhea?|!=null"
        ],
        "then": [
          "$gt0084|Diarrhoea RS|.precision=2",
          "$gt0084|Diarrhoea RS|.magnitude=$gt0021.value"
        ]
      },
      "gt0147": {
        "id": "gt0147",
        "priority": 19,
        "when": [
          "$gt0032|28. Has your physical condition or medical treatment caused you financial difficulties?|!=null"
        ],
        "then": [
          "$gt0085|Financial difficulties RS|.precision=2",
          "$gt0085|Financial difficulties RS|.magnitude=$gt0032.value"
        ]
      },
      "gt0148": {
        "id": "gt0148",
        "priority": 18,
