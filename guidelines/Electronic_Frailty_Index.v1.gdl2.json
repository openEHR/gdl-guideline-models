    "rules": {
      "gt0114": {
        "id": "gt0114",
        "priority": 5,
        "when": [
          "$gt0003|Arthrits|!=null",
          "$gt0004|Asthma/copd|!=null",
          "$gt0005|Atrial fibrillation|!=null",
          "$gt0006|Cerebrovascular disease|!=null",
          "$gt0007|Chronic kidney disease|!=null",
          "$gt0008|Diabetes|!=null",
          "$gt0009|Foot problems|!=null",
          "$gt0010|Fragility fracture|!=null",
          "$gt0011|Heart failure|!=null",
          "$gt0012|Hypertension|!=null",
          "$gt0013|Hypotension/syncope|!=null",
          "$gt0014|Ischaemic Heart Disease|!=null",
          "$gt0015|Osteoporosis|!=null",
          "$gt0016|Parkinsonism and tremor|!=null",
          "$gt0017|Peptic ulcer|!=null",
          "$gt0018|Peripheral vascular disease|!=null",
          "$gt0019|Respiratory disease|!=null",
          "$gt0020|Skin ulcer|!=null",
          "$gt0021|Thyroid disease|!=null",
          "$gt0023|Activity limitation|!=null",
          "$gt0024|Housebound|!=null",
          "$gt0025|Hearing Impairment|!=null",
          "$gt0026|Mobility/transfer problems|!=null",
          "$gt0027|Requirement for care|!=null",
          "$gt0028|Social vulnerability|!=null",
          "$gt0029|Visual impairment|!=null",
          "$gt0030|Anaemia and haematinic deficiency|!=null",
          "$gt0031|Dizziness|!=null",
          "$gt0032|Dyspnoea|!=null",
          "$gt0033|Falls|!=null",
          "$gt0034|Memory/cognitive problems|!=null",
          "$gt0035|Polypharmacy|!=null",
          "$gt0036|Sleep disturbance|!=null",
          "$gt0038|Weight loss and anorexia|!=null",
          "$gt0115|Urinary incontinence disease state|!=null",
          "$gt0116|Urinary incontinence|!=null"
        ],
        "then": [
          "$gt0076|Total Score|.precision=2",
          "$gt0076|Total Score|.magnitude=($gt0003.value+$gt0004.value+$gt0005.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0013.value+$gt0014.value+$gt0015.value+$gt0016.value+$gt0017.value+$gt0018.value+$gt0019.value+$gt0020.value+$gt0021.value+$gt0023.value+$gt0024.value+$gt0025.value+$gt0026.value+$gt0027.value+$gt0028.value+$gt0029.value+$gt0030.value+$gt0031.value+$gt0032.value+$gt0033.value+$gt0034.value+$gt0035.value+$gt0036.value+$gt0038.value+$gt0115.value+$gt0116.value)/36"
        ]
      },
      "gt0120": {
        "id": "gt0120",
        "priority": 4,
        "when": [
          "$gt0076|Total Score|.magnitude<=0.12",
          "$gt0076|Total Score|.magnitude>=0"
        ],
        "then": [
          "$gt0118|Score categories|=0|local::at0003|Fit|",
          "$gt0119|Category descriptions|=0|local::at0008|People who have no or few long-term conditions that are usually well controlled. |"
        ]
      },
      "gt0121": {
        "id": "gt0121",
        "priority": 3,
        "when": [
          "$gt0076|Total Score|.magnitude<=0.24",
          "$gt0076|Total Score|.magnitude>=0.13"
        ],
        "then": [
          "$gt0118|Score categories|=1|local::at0004|Mild frailty|",
          "$gt0119|Category descriptions|=1|local::at0009|People who are slowing up in older age and may need help with personal activities of daily living|"
        ]
      },
      "gt0122": {
        "id": "gt0122",
        "priority": 2,
        "when": [
          "$gt0076|Total Score|.magnitude<=0.36",
          "$gt0076|Total Score|.magnitude>=0.25"
        ],
        "then": [
          "$gt0118|Score categories|=2|local::at0005|Moderate frailty|",
          "$gt0119|Category descriptions|=2|local::at0010|People who have difficulties with outdoor activities |"
        ]
      },
      "gt0123": {
        "id": "gt0123",
        "priority": 1,
        "when": [
          "$gt0076|Total Score|.magnitude>0.36"
        ],
        "then": [
          "$gt0118|Score categories|=3|local::at0006|Severe frailty|",
          "$gt0119|Category descriptions|=3|local::at0011|People who are often dependent for personal care and have a range of long-term conditions/multimorbidity. |"
        ]
      }
    }
