{
  "id": "FUNC_ICH_Score.v1",
  "gdl_version": "2.0",
  "concept": "gt0001",
  "language": {
    "original_language": "ISO_639-1::en"
  },
  "description": {
    "original_author": {
      "date": "2017-12-02",
      "name": "Syeeda S Farruque",
      "organisation": "Cambio Healthcare Systems",
