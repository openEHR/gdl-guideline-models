      "gt0025": {
        "id": "gt0025",
        "model_id": "openEHR-EHR-OBSERVATION.gipss_for_pmf.v0",
        "template_id": "openEHR-EHR-OBSERVATION.gipss_for_pmf.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0026": {
            "id": "gt0026",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0011]",
            "data_type": "DV_COUNT"
          }
        }
      },
      "gt0027": {
        "id": "gt0027",
        "model_id": "openEHR-EHR-EVALUATION.gipss_for_pmf_assessment.v0",
        "template_id": "openEHR-EHR-EVALUATION.gipss_for_pmf_assessment.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0028": {
            "id": "gt0028",
            "path": "/data[at0003]/items[at0004]",
            "data_type": "DV_CODED_TEXT",
            "allowed_values": [
              "local::at0007|Low|",
              "local::at0008|Intermediate-1|",
              "local::at0009|Intermediate-2|",
              "local::at0010|High|"
            ]
          },
          "gt0029": {
            "id": "gt0029",
            "path": "/data[at0003]/items[at0005]",
            "data_type": "DV_CODED_TEXT",
            "allowed_values": [
              "local::at0011|94%|",
              "local::at0012|73%|",
              "local::at0013|40%|",
              "local::at0014|14%|"
            ]
          },
          "gt0030": {
            "id": "gt0030",
            "path": "/data[at0003]/items[at0006]",
            "data_type": "DV_CODED_TEXT",
            "allowed_values": [
              "local::at0015|26.4 years|",
              "local::at0016|8.0 years|",
              "local::at0017|4.2 years|",
              "local::at0018|2 years|"
            ]
          }
        }
      }
    },
    "rules": {
      "gt0031": {
        "id": "gt0031",
        "priority": 5,
        "when": [
          "$gt0019|Karyotype classification|!=null",
          "$gt0020|Type 1/like CALR mutation|!=null",
          "$gt0021|ASXL1 mutation|!=null",
          "$gt0022|SRSF2 mutation|!=null",
          "$gt0023|U2AF1Q157 mutation|!=null"
        ],
        "then": [
          "$gt0026|Total score|.magnitude=$gt0019|Karyotype classification|.value+$gt0020|Type 1/like CALR mutation|.value+$gt0021|ASXL1 mutation|.value+$gt0022|SRSF2 mutation|.value+$gt0023|U2AF1Q157 mutation|.value"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 4,
        "when": [
          "$gt0026|Total score|==0"
        ],
        "then": [
          "$gt0028|Risk|=local::at0007|Low|",
          "$gt0029|5-year overall survival|=local::at0011|94%|",
          "$gt0030|Median overall survival|=local::at0015|26.4 years|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 3,
        "when": [
          "$gt0026|Total score|==1"
        ],
        "then": [
          "$gt0028|Risk|=local::at0008|Intermediate-1|",
          "$gt0029|5-year overall survival|=local::at0012|73%|",
          "$gt0030|Median overall survival|=local::at0016|8.0 years|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 2,
        "when": [
          "$gt0026|Total score|==2"
        ],
        "then": [
          "$gt0028|Risk|=local::at0009|Intermediate-2|",
          "$gt0029|5-year overall survival|=local::at0013|40%|",
          "$gt0030|Median overall survival|=local::at0017|4.2 years|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 1,
        "when": [
          "$gt0026|Total score|>2"
        ],
        "then": [
          "$gt0028|Risk|=local::at0010|High|",
          "$gt0029|5-year overall survival|=local::at0014|14%|",
          "$gt0030|Median overall survival|=local::at0018|2 years|"
        ]
      }
    }
  },
  "ontology": {
