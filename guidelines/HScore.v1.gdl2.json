    "rules": {
      "gt0025": {
        "id": "gt0025",
        "priority": 31,
        "when": [
          "($gt0016|Temperature|.magnitude<101.1&&$gt0016|Temperature|.unit=='[degF]')||($gt0016|Temperature|.magnitude<38.4&&$gt0016|Temperature|.unit=='Cel')"
        ],
        "then": [
          "$gt0003|Temperature, °F (°C)|=0|local::at0007|<101.1 (<38.4)|"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 30,
        "when": [
          "($gt0016|Temperature|.magnitude>=101.1&&$gt0016|Temperature|.magnitude<=102.9&&$gt0016|Temperature|.unit=='[degF]')||($gt0016|Temperature|.magnitude>=38.4&&$gt0016|Temperature|.magnitude<=39.4&&$gt0016|Temperature|.unit=='Cel')"
        ],
        "then": [
          "$gt0003|Temperature, °F (°C)|=33|local::at0008|101.1–102.9 (38.4-39.4)|"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 29,
        "when": [
          "($gt0016|Temperature|.magnitude>102.9&&$gt0016|Temperature|.unit=='[degF]')||($gt0016|Temperature|.magnitude>39.4&&$gt0016|Temperature|.unit=='Cel')"
        ],
        "then": [
          "$gt0003|Temperature, °F (°C)|=49|local::at0009|>102.9 (>39.4)|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 28,
        "when": [
          "$gt0018|Ferritin result|.magnitude<2000",
          "$gt0018|Ferritin result|.unit=='ng/mL'||$gt0018|Ferritin result|.unit=='ug/L'"
        ],
        "then": [
          "$gt0004|Ferritin, ng/mL (μg/L)|=0|local::at0018|<2000|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 27,
        "when": [
          "$gt0018|Ferritin result|.magnitude>=2000",
          "$gt0018|Ferritin result|.magnitude<=6000",
          "$gt0018|Ferritin result|.unit=='ng/mL'||$gt0018|Ferritin result|.unit=='ug/L'"
        ],
        "then": [
          "$gt0004|Ferritin, ng/mL (μg/L)|=35|local::at0019|2000–6000|"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 26,
        "when": [
          "$gt0018|Ferritin result|.magnitude>6000",
          "$gt0018|Ferritin result|.unit=='ng/mL'||$gt0018|Ferritin result|.unit=='ug/L'"
        ],
        "then": [
          "$gt0004|Ferritin, ng/mL (μg/L)|=50|local::at0020|>6000|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 25,
        "when": [
          "$gt0020|Triglycerides result|<132.7,mg/dl||$gt0020|Triglycerides result|<1.5,mmol/l"
        ],
        "then": [
          "$gt0005|Triglyceride, mg/dl (mmol/l)|=0|local::at0022|<132.7 (<1.5)|"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 24,
        "when": [
          "($gt0020|Triglycerides result|>=132.7,mg/dl&&$gt0020|Triglycerides result|<=354,mg/dl)||($gt0020|Triglycerides result|>=1.5,mmol/l&&$gt0020|Triglycerides result|<=4,mmol/l)"
        ],
        "then": [
          "$gt0005|Triglyceride, mg/dl (mmol/l)|=44|local::at0023|132.7-354 (1.5–4)|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 23,
        "when": [
          "$gt0020|Triglycerides result|>354,mg/dl||$gt0020|Triglycerides result|>4,mmol/l"
        ],
        "then": [
          "$gt0005|Triglyceride, mg/dl (mmol/l)|=64|local::at0024|>354 (>4)|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 22,
        "when": [
          "$gt0022|Fibrinogen result|>250,mg/dl||$gt0022|Fibrinogen result|>2.5,g/L"
        ],
        "then": [
          "$gt0006|Fibrinogen, mg/dl (g/L)|=0|local::at0026|>250 (>2.5)|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 21,
        "when": [
          "$gt0022|Fibrinogen result|<=250,mg/dl||$gt0022|Fibrinogen result|<=2.5,g/L"
        ],
        "then": [
          "$gt0006|Fibrinogen, mg/dl (g/L)|=30|local::at0027|≤250 (≤2.5)|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 20,
        "when": [
          "$gt0024|Aspartate aminotransferase (AST) result|<30,U/l"
        ],
        "then": [
          "$gt0007|AST, U/l|=0|local::at0029|<30|"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 19,
        "when": [
          "$gt0024|Aspartate aminotransferase (AST) result|>=30,U/l"
        ],
        "then": [
          "$gt0007|AST, U/l|=19|local::at0030|≥30|"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 18,
        "when": [
          "$gt0011|Known underlying immunosuppression|!=null",
          "$gt0012|Organomegaly|!=null",
          "$gt0013|Number of cytopenias|!=null",
          "$gt0014|Hemophagocytosis features on bone marrow aspirate|!=null",
          "$gt0016|Temperature|!=null",
          "$gt0018|Ferritin result|!=null",
          "$gt0020|Triglycerides result|!=null",
          "$gt0022|Fibrinogen result|!=null",
          "$gt0024|Aspartate aminotransferase (AST) result|!=null"
        ],
        "then": [
          "$gt0008|HScore|.magnitude=$gt0011.value+$gt0003.value+$gt0012.value+$gt0013.value+$gt0004.value+$gt0005.value+$gt0006.value+$gt0007.value+$gt0014.value"
        ]
      },
