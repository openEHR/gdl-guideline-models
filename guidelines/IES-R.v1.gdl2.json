    "rules": {
      "gt0032": {
        "id": "gt0032",
        "priority": 9,
        "when": [
          "!fired($gt0032)",
          "$gt0025|I felt watchful and on guard|!=null",
          "$gt0024|Reminders of it caused me to have physical reactions, such as sweating, trouble breathing, nausea, or a pounding heart|!=null",
          "$gt0004|I avoided letting myself get upset when I thought about it or was reminded of it|!=null",
          "$gt0005|I felt as if it hadn't happened or wasn't real|!=null",
          "$gt0006|I stayed away from reminders about it|!=null",
          "$gt0007|I tried not to think about it|!=null",
          "$gt0008|I was aware that I still had a lot of feelings about it, but I didn't deal with them|!=null",
          "$gt0009|My feelings about it were kind of numb|!=null",
          "$gt0010|I tried to remove it from my memory|!=null",
          "$gt0011|I tried not to talk about it|!=null",
          "$gt0012|Any reminder brought back feelings about it|!=null",
          "$gt0013|I had trouble staying asleep|!=null",
          "$gt0014|Other things kept making me think about it|!=null",
          "$gt0015|I thought about it when I didn't mean to|!=null",
          "$gt0016|Pictures about it popped into my mind|!=null",
          "$gt0017|I found myself acting or feeling like I was back at that time|!=null",
          "$gt0018| I had waves of strong feelings about it|!=null",
          "$gt0019|I had dreams about it|!=null",
          "$gt0020|I felt irritable and angry|!=null",
          "$gt0021|I was jumpy and easily startled|!=null",
          "$gt0022|I had trouble falling asleep|!=null",
          "$gt0023| I had trouble concentrating|!=null"
        ],
        "then": [
          "$gt0030|Total IES-R Score|.magnitude=(((((((((((((((((((($gt0004.value+$gt0005.value)+$gt0006.value)+$gt0007.value)+$gt0008.value)+$gt0009.value)+$gt0010.value)+$gt0011.value)+$gt0012.value)+$gt0013.value)+$gt0014.value)+$gt0015.value)+$gt0016.value)+$gt0017.value)+$gt0018.value)+$gt0019.value)+$gt0020.value)+$gt0021.value)+$gt0022.value)+$gt0023.value)+$gt0024.value)+$gt0025.value"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 8,
        "when": [
          "!fired($gt0033)",
          "$gt0030|Total IES-R Score|>=37"
        ],
        "then": [
          "$gt0031|IES-R score consequence|=local::at0151|PTSD very probable + immune system suppression|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 7,
        "when": [
          "!fired($gt0034)",
          "$gt0030|Total IES-R Score|>=33",
          "$gt0030|Total IES-R Score|<=36"
        ],
        "then": [
          "$gt0031|IES-R score consequence|=local::at0150|Probable PTSD diagnosis|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 6,
        "when": [
          "!fired($gt0035)",
          "$gt0030|Total IES-R Score|>=24",
          "$gt0030|Total IES-R Score|<=32"
        ],
        "then": [
          "$gt0031|IES-R score consequence|=local::at0149|PTSD is a clinical concern|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 5,
        "when": [
          "!fired($gt0036)",
          "$gt0030|Total IES-R Score|<=23"
        ],
        "then": [
          "$gt0031|IES-R score consequence|=local::at0148|No PTSD concern|"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 4,
        "when": [
          "!fired($gt0037)",
          "$gt0004|I avoided letting myself get upset when I thought about it or was reminded of it|!=null",
          "$gt0005|I felt as if it hadn't happened or wasn't real|!=null",
          "$gt0006|I stayed away from reminders about it|!=null",
          "$gt0007|I tried not to think about it|!=null",
          "$gt0008|I was aware that I still had a lot of feelings about it, but I didn't deal with them|!=null",
          "$gt0009|My feelings about it were kind of numb|!=null",
          "$gt0010|I tried to remove it from my memory|!=null",
          "$gt0011|I tried not to talk about it|!=null"
        ],
        "then": [
          "$gt0026|Avoidance mean|.magnitude=((((((($gt0004.value+$gt0005.value)+$gt0006.value)+$gt0007.value)+$gt0008.value)+$gt0009.value)+$gt0010.value)+$gt0011.value)/8"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 3,
        "when": [
          "!fired($gt0038)",
          "$gt0012|Any reminder brought back feelings about it|!=null",
          "$gt0013|I had trouble staying asleep|!=null",
          "$gt0014|Other things kept making me think about it|!=null",
          "$gt0015|I thought about it when I didn't mean to|!=null",
          "$gt0016|Pictures about it popped into my mind|!=null",
          "$gt0017|I found myself acting or feeling like I was back at that time|!=null",
          "$gt0018| I had waves of strong feelings about it|!=null",
          "$gt0019|I had dreams about it|!=null"
        ],
        "then": [
          "$gt0027|Intrusion mean|.magnitude=((((((($gt0012.value+$gt0013.value)+$gt0014.value)+$gt0015.value)+$gt0016.value)+$gt0017.value)+$gt0018.value)+$gt0019.value)/8"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 2,
        "when": [
          "!fired($gt0039)",
          "$gt0020|I felt irritable and angry|!=null",
          "$gt0021|I was jumpy and easily startled|!=null",
          "$gt0022|I had trouble falling asleep|!=null",
          "$gt0023| I had trouble concentrating|!=null",
          "$gt0024|Reminders of it caused me to have physical reactions, such as sweating, trouble breathing, nausea, or a pounding heart|!=null",
          "$gt0025|I felt watchful and on guard|!=null"
        ],
        "then": [
          "$gt0028|Hyperarousal mean|.magnitude=((((($gt0020.value+$gt0021.value)+$gt0022.value)+$gt0023.value)+$gt0024.value)+$gt0025.value)/6"
        ]
      },
      "gt0040": {
        "id": "gt0040",
        "priority": 1,
        "when": [
          "!fired($gt0040)",
          "$gt0026|Avoidance mean|!=null",
          "$gt0027|Intrusion mean|!=null",
          "$gt0028|Hyperarousal mean|!=null"
        ],
        "then": [
          "$gt0029|Total mean IES-R Score|.magnitude=($gt0026.magnitude+$gt0027.magnitude)+$gt0028.magnitude"
        ]
      }
    }
