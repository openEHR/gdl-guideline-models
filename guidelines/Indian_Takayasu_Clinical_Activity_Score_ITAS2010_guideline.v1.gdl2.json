    "details": {
      "en": {
        "id": "en",
        "purpose": "To differentiate between active and inactive disease in Takayasu arteritis (TA).",
        "keywords": [
          "Takayasu arteritis",
          "large vessel disease",
          "large vessel vasculitis (LVV)"
        ],
        "use": "Use to quantify disease activity in Takayasu arteritis (TA) for treatment decisions. \nThere are seven ITAS2010 items [five cardiovascular systems (CVS) items together with stroke and hypertension] which are scored 2, while all other items are scored 1.",
        "misuse": "Select feature only if an abnormality is present and new, within the past 3 months AND ascribed to current active vasculitis.",
        "copyright": "@CambioCDS"
      }
    },
    "other_details": {
      "references": "Misra R, Danda D, Rajappa SM, et al. Development and initial validation of the Indian Takayasu Clinical Activity Score (ITAS2010). Rheumatology (Oxford). 2013;52(10):1795-801."
    }
  },
