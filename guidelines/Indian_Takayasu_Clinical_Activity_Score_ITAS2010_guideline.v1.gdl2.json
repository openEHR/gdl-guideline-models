    "rules": {
      "gt0050": {
        "id": "gt0050",
        "priority": 3,
        "when": [
          "$gt0003|Malaise or weight loss >2 kg|!=null",
          "$gt0004|Myalgia, arthralgia, or arthritis|!=null",
          "$gt0005|Headache|!=null",
          "$gt0006|Severe abdominal pain|!=null",
          "$gt0007|Recent history of spontaneous abortion|!=null",
          "$gt0008|Systolic BP >140|!=null",
          "$gt0009|Diastolic BP >90|!=null",
          "$gt0010|Stroke|!=null",
          "$gt0011|Seizures (not hypertensive)|!=null",
          "$gt0012|Syncope|!=null",
          "$gt0013|Vertigo/dizziness|!=null",
          "$gt0014|Bruits (If present, may select location of bruits to improve diagnosis)|!=null",
          "$gt0059|Pulse inequality (if Yes, may answer Pulse and BP inequality to improve diagnosis)|!=null",
          "$gt0061|New loss of pulses (If yes, may select location of pulse loss to improve diagnosis)|!=null",
          "$gt0078|Claudication (If present, may select location of claudication to improve diagnosis)|!=null",
          "$gt0081|Carotidynia|!=null",
          "$gt0082|Aortic incompetence|!=null",
          "$gt0083|Myocardial infarct/angina|!=null",
          "$gt0084|Cardiomyopathy/cardiac failure|!=null"
        ],
        "then": [
          "$gt0048|ITAS2010 Score|.magnitude=$gt0003.value+$gt0004.value+$gt0005.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0013.value+$gt0014.value+$gt0053.value+$gt0054.value+$gt0055.value+$gt0056.value+$gt0057.value+$gt0058.value+$gt0059.value+$gt0060.value+$gt0061.value+$gt0062.value+$gt0063.value+$gt0064.value+$gt0065.value+$gt0066.value+$gt0067.value+$gt0068.value+$gt0069.value+$gt0070.value+$gt0071.value+$gt0072.value+$gt0073.value+$gt0074.value+$gt0075.value+$gt0076.value+$gt0077.value+$gt0078.value+$gt0079.value+$gt0080.value+$gt0081.value+$gt0082.value+$gt0083.value+$gt0084.value"
        ]
      },
      "gt0051": {
        "id": "gt0051",
        "priority": 2,
        "when": [
          "$gt0048|ITAS2010 Score|>=2"
        ],
        "then": [
          "$gt0049|Disease activity|='Active TA'"
        ]
      },
      "gt0052": {
        "id": "gt0052",
        "priority": 1,
        "when": [
          "$gt0048|ITAS2010 Score|<2"
        ],
        "then": [
          "$gt0049|Disease activity|='Inactive TA'"
        ]
      }
    }
