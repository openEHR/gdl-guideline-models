            "data_type" : "DV_ORDINAL",
            "allowed_values" : [
              "0|local::at0022",
              "1|local::at0023"
            ]
          },
          "gt0027" : {
            "id" : "gt0027",
            "path" : "/data[at0001]/events[at0002]/data[at0003]/items[at0020]",
            "data_type" : "DV_ORDINAL",
            "allowed_values" : [
              "0|local::at0024",
              "1|local::at0025"
            ]
          },
          "gt0028" : {
            "id" : "gt0028",
            "path" : "/data[at0001]/events[at0002]/data[at0003]/items[at0021]",
            "data_type" : "DV_ORDINAL",
            "allowed_values" : [
              "0|local::at0026",
              "1|local::at0027"
            ]
          }
        }
      },
      "gt0011" : {
        "id" : "gt0011",
        "model_id" : "openEHR-EHR-EVALUATION.health_risk.v1",
        "template_id" : "openEHR-EHR-EVALUATION.health_risk.v1",
        "type" : "OUTPUT",
        "elements" : {
          "gt0012" : {
            "id" : "gt0012",
            "path" : "/data[at0001]/items[at0003]",
            "data_type" : "DV_TEXT"
          }
        }
      },
      "gt0032" : {
        "id" : "gt0032",
        "model_id" : "openEHR-EHR-OBSERVATION.international_prognostic_score_for_asymptomatic_early_stage_cll.v0",
        "template_id" : "openEHR-EHR-OBSERVATION.international_prognostic_score_for_asymptomatic_early_stage_cll.v0",
        "type" : "OUTPUT",
        "elements" : {
          "gt0033" : {
            "id" : "gt0033",
            "path" : "/data[at0001]/events[at0002]/data[at0003]/items[at0010]",
            "data_type" : "DV_COUNT"
          }
        }
      }
    },
    "rules" : {
      "gt0019" : {
        "id" : "gt0019",
        "priority" : 4,
        "when" : [
          "$gt0026|Unmutated IGHV genes|!=null",
          "$gt0027|Absolute lymphocyte count >15×10^9/L|!=null",
          "$gt0028|Palpable lymph nodes|!=null"
        ],
        "then" : [
          "$gt0033|Total score|.magnitude=$gt0026|Unmutated IGHV genes|.value+$gt0027|Absolute lymphocyte count >15×10^9/L|.value+$gt0028|Palpable lymph nodes|.value"
        ]
      },
      "gt0020" : {
        "id" : "gt0020",
        "priority" : 3,
        "when" : [
          "$gt0033|Total score|==0"
        ],
        "then" : [
          "$gt0012|Risk for Treatment Need|=$gt0023|Low risk|.term"
        ]
      },
      "gt0021" : {
        "id" : "gt0021",
        "priority" : 2,
        "when" : [
          "$gt0033|Total score|==1"
        ],
        "then" : [
          "$gt0012|Risk for Treatment Need|=$gt0024|Intermediate risk|.term"
        ]
      },
      "gt0022" : {
        "id" : "gt0022",
        "priority" : 1,
        "when" : [
          "$gt0033|Total score|>1",
          "$gt0033|Total score|<=3"
        ],
        "then" : [
          "$gt0012|Risk for Treatment Need|=$gt0025|High risk|.term"
        ]
      }
    }
  },
