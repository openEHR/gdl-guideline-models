    "rules": {
      "gt0031": {
        "id": "gt0031",
        "priority": 21,
        "when": [
          "$gt0041|Creatinine|.unit=='umol/l'"
        ],
        "then": [
          "$gt0021|Creatinine|.precision=2",
          "$gt0021|Creatinine|.unit='mg/dl'",
          "$gt0021|Creatinine|.magnitude=$gt0041.magnitude/88.42"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 20,
        "when": [
          "$gt0041|Creatinine|.unit=='mg/dl'"
        ],
        "then": [
          "$gt0021|Creatinine|=$gt0041|Creatinine|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 19,
        "when": [
          "$gt0006|Total bilirubin|.magnitude>17.1",
          "$gt0006|Total bilirubin|.unit=='µmol/l'"
        ],
        "then": [
          "$gt0018|Total bilirubin|.precision=2",
          "$gt0018|Total bilirubin|.unit='mg/dl'",
          "$gt0018|Total bilirubin|.magnitude=$gt0006.magnitude/17.1"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 18,
        "when": [
          "$gt0006|Total bilirubin|.magnitude<=17.1",
          "$gt0006|Total bilirubin|.unit=='µmol/l'"
        ],
        "then": [
          "$gt0018|Total bilirubin|.unit='mg/dl'",
          "$gt0018|Total bilirubin|.magnitude=1"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 17,
        "when": [
          "$gt0006|Total bilirubin|.magnitude>1",
          "$gt0006|Total bilirubin|.unit=='mg/dl'"
        ],
        "then": [
          "$gt0018|Total bilirubin|=$gt0006|Total bilirubin|"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 16,
        "when": [
          "$gt0006|Total bilirubin|.unit=='mg/dl'",
          "$gt0006|Total bilirubin|<=1,mg/dl"
        ],
        "then": [
          "$gt0018|Total bilirubin|.unit='mg/dl'",
          "$gt0018|Total bilirubin|.magnitude=1"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 15,
        "when": [
          "$gt0003|Sodium|<=137,mmol/l",
          "$gt0003|Sodium|>=125,mmol/l"
        ],
        "then": [
          "$gt0020|Sodium|=$gt0003|Sodium|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 14,
        "when": [
          "$gt0003|Sodium|<125,mmol/l"
        ],
        "then": [
          "$gt0020|Sodium|=125,mmol/l"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 13,
        "when": [
          "$gt0003|Sodium|>137,mmol/l"
        ],
        "then": [
          "$gt0020|Sodium|=137,mmol/l"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 12,
        "when": [
          "$gt0008|INR|>1"
        ],
        "then": [
          "$gt0016|INR|=$gt0008|INR|"
        ]
      },
      "gt0040": {
        "id": "gt0040",
        "priority": 11,
        "when": [
          "$gt0008|INR|<=1"
        ],
        "then": [
          "$gt0016|INR|=1"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 10,
        "when": [
          "$gt0010|Dialysis history|!=null"
        ],
        "then": [
          "$gt0014|Dialysis history|=$gt0010|Dialysis history|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 9,
        "when": [
          "$gt0014|Dialysis history|==0|local::at0007|No|",
          "$gt0021|Creatinine|>=1,mg/dl",
          "$gt0021|Creatinine|<4,mg/dl"
        ],
        "then": [
          "$gt0012|Original MELD (oMELD) score|.precision=0",
          "$gt0012|Original MELD (oMELD) score|.unit='1'",
          "$gt0012|Original MELD (oMELD) score|.magnitude=((9.57*log($gt0021.magnitude))+(3.78*log($gt0006.magnitude)))+(11.2*log($gt0008^1))+6.43"
        ]
      },
      "gt0043": {
        "id": "gt0043",
        "priority": 8,
        "when": [
          "$gt0021|Creatinine|<1,mg/dl",
          "$gt0014|Dialysis history|==0|local::at0007|No|"
        ],
        "then": [
          "$gt0012|Original MELD (oMELD) score|.precision=0",
          "$gt0012|Original MELD (oMELD) score|.unit='1'",
          "$gt0012|Original MELD (oMELD) score|.magnitude=(((9.57*log(1))+(3.78*log($gt0006.magnitude)))+(11.2*log($gt0008^1)))+6.43"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 7,
        "when": [
          "($gt0014|Dialysis history|==1|local::at0008|Yes|)||($gt0021|Creatinine|>=4,mg/dl)"
        ],
        "then": [
          "$gt0012|Original MELD (oMELD) score|.precision=0",
          "$gt0012|Original MELD (oMELD) score|.unit='1'",
          "$gt0012|Original MELD (oMELD) score|.magnitude=(((9.57*log(4))+(3.78*log($gt0006.magnitude)))+(11.2*log($gt0008^1)))+6.43"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 6,
        "then": [
          "$gt0013|MELD score|.precision=0",
          "$gt0013|MELD score|.unit='1'",
          "$gt0013|MELD score|.magnitude=((1.32*(137-$gt0020.magnitude))-((0.033*$gt0012.magnitude)*(137-$gt0020.magnitude)))+$gt0012.magnitude"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 5,
        "when": [
          "$gt0013|MELD score|<=9,1"
        ],
        "then": [
          "$gt0045|90-day mortality|=1.9,%"
        ]
      },
      "gt0047": {
        "id": "gt0047",
        "priority": 4,
        "when": [
          "$gt0013|MELD score|<=19,1",
          "$gt0013|MELD score|>=10,1"
        ],
        "then": [
          "$gt0045|90-day mortality|=6,%"
        ]
      },
      "gt0048": {
        "id": "gt0048",
        "priority": 3,
        "when": [
          "$gt0013|MELD score|<=29,1",
          "$gt0013|MELD score|>=20,1"
        ],
        "then": [
          "$gt0045|90-day mortality|=19.6,%"
        ]
      },
      "gt0049": {
        "id": "gt0049",
        "priority": 2,
        "when": [
          "$gt0013|MELD score|<=39,1",
          "$gt0013|MELD score|>=30,1"
        ],
        "then": [
          "$gt0045|90-day mortality|=52.6,%"
        ]
      },
      "gt0050": {
        "id": "gt0050",
        "priority": 1,
        "when": [
          "$gt0013|MELD score|==40,1"
        ],
        "then": [
          "$gt0045|90-day mortality|=71.3,%"
        ]
      }
    }
