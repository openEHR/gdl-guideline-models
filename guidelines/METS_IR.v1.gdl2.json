  "id": "METS_IR.v1",
  "gdl_version": "2.1",
  "concept": "gt0001",
  "language": {
    "original_language": "ISO_639-1::en"
  },
  "description": {
    "original_author": {
      "name": "Zsolt Feh√©r",
      "email": "zsolt.feher@stud.ki.se"
    },
    "lifecycle_state": "DRAFTED",
    "details": {
      "en": {
        "id": "en",
        "purpose": "This tool can be used to detect insulin resistance at early stages, allowing clinicians to implement preventive strategies. Unlike other similar calculators, this tool does not require insulin measurements, making it more practical and convenient with reduced variability."
      }
    },
    "other_details": {
      "en": "Inputs & Outputs"
    }
  },
  "definition": {
    "data_bindings": {
      "gt0002": {
        "id": "gt0002",
        "model_id": "openEHR-EHR-OBSERVATION.lab_test_glucose.v1",
        "template_id": "openEHR-EHR-OBSERVATION.lab_test_glucose.v1",
        "type": "INPUT",
        "elements": {
          "gt0003": {
            "id": "gt0003",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0078]",
            "data_type": "DV_QUANTITY",
            "units": [
              "mg/dl",
              "mmol/l"
            ]
          }
        }
      },
      "gt0004": {
        "id": "gt0004",
        "model_id": "openEHR-EHR-OBSERVATION.lab_test-lipids.v1",
        "template_id": "openEHR-EHR-OBSERVATION.lab_test-lipids.v1",
        "type": "INPUT",
        "elements": {
          "gt0005": {
            "id": "gt0005",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0078.4]",
            "data_type": "DV_QUANTITY",
            "units": [
              "mmol/l",
              "mg/dl"
            ]
          }
        }
      },
      "gt0006": {
        "id": "gt0006",
        "model_id": "openEHR-EHR-OBSERVATION.body_mass_index.v2",
        "template_id": "openEHR-EHR-OBSERVATION.body_mass_index.v2",
        "type": "INPUT",
        "elements": {
          "gt0007": {
            "id": "gt0007",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0004]",
            "data_type": "DV_QUANTITY",
            "units": [
              "kg/m2"
            ]
          }
        }
      },
      "gt0010": {
        "id": "gt0010",
        "model_id": "openEHR-EHR-OBSERVATION.lab_test-lipids.v1",
        "template_id": "openEHR-EHR-OBSERVATION.lab_test-lipids.v1",
        "type": "INPUT",
        "elements": {
          "gt0011": {
            "id": "gt0011",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0078.3]",
            "data_type": "DV_QUANTITY",
            "units": [
              "mmol/l",
              "mg/dl"
            ]
          }
        }
      },
      "gt0016": {
        "id": "gt0016",
        "model_id": "openEHR-EHR-OBSERVATION.mets_ir.v1",
        "template_id": "openEHR-EHR-OBSERVATION.mets_ir.v1",
        "type": "OUTPUT",
        "elements": {
          "gt0017": {
            "id": "gt0017",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0009]",
            "data_type": "DV_QUANTITY"
          }
        }
      },
      "gt0018": {
        "id": "gt0018",
        "model_id": "openEHR-EHR-OBSERVATION.mets_ir.v1",
        "template_id": "openEHR-EHR-OBSERVATION.mets_ir.v1",
        "type": "OUTPUT",
        "elements": {
          "gt0019": {
            "id": "gt0019",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0010]",
            "data_type": "DV_ORDINAL",
            "allowed_values": [
              "1|local::at0011|Low|",
              "2|local::at0012|High|"
            ]
          }
        }
      }
    },
    "rules": {
      "gt0022": {
        "id": "gt0022",
        "priority": 6,
        "when": [
          "($gt0011|HDL-c|!=null)&&($gt0011|HDL-c|.unit=='mmol/l')"
        ],
        "then": [
          "$gt0011|HDL-c|.magnitude=$gt0011|HDL-c|.magnitude*38.67",
          "$gt0011|HDL-c|.unit='mg/dl'"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 5,
        "when": [
          "($gt0005|Triglyceride concentration|!=null)&&($gt0005|Triglyceride concentration|.unit=='mmol/l')"
        ],
        "then": [
          "$gt0005|Triglyceride concentration|.magnitude=$gt0005|Triglyceride concentration|.magnitude*88.57",
          "$gt0005|Triglyceride concentration|.unit='mg/dl'"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 4,
        "when": [
          "($gt0003|Fasting Glucose|!=null)&&($gt0003|Fasting Glucose|.unit=='mmol/l')"
        ],
        "then": [
          "$gt0003|Fasting Glucose|.magnitude=18*$gt0003|Fasting Glucose|.magnitude",
          "$gt0003|Fasting Glucose|.unit='mg/dl'"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 3,
        "when": [
          "($gt0003|Fasting Glucose|!=null)&&($gt0003|Fasting Glucose|.unit=='mg/dl')",
          "($gt0005|Triglyceride concentration|!=null)&&($gt0005|Triglyceride concentration|.unit=='mg/dl')",
          "$gt0007|Body mass index|!=null",
          "($gt0011|HDL-c|!=null)&&($gt0011|HDL-c|.unit=='mg/dl')"
        ],
        "then": [
          "$gt0017|METS-IR score|.precision=1",
          "$gt0017|METS-IR score|.magnitude=(log(($gt0003|Fasting Glucose|.magnitude*2)+$gt0005|Triglyceride concentration|.magnitude)*$gt0007|Body mass index|.magnitude)/(1*log($gt0011|HDL-c|.magnitude))"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 2,
        "when": [
          "$gt0017|METS-IR score|!=null",
          "$gt0017|METS-IR score|.magnitude<=50.39"
        ],
        "then": [
          "$gt0019|METS-IR Interpretation|='at0011'",
          "$gt0019|METS-IR Interpretation|.label='Risk for developing T2D: Low'"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 1,
        "when": [
          "$gt0017|METS-IR score|!=null",
          "$gt0017|METS-IR score|.magnitude>50.39"
        ],
        "then": [
          "$gt0019|METS-IR Interpretation|.label='at0012'",
          "$gt0019|METS-IR Interpretation|.label='Risk for developing T2D: High'"
        ]
      }
    }
  },
