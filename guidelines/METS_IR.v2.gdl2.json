              "kg/m2"
            ]
          }
        }
      },
      "gt0010": {
        "id": "gt0010",
        "model_id": "openEHR-EHR-OBSERVATION.lab_test-lipids.v1",
        "template_id": "openEHR-EHR-OBSERVATION.lab_test-lipids.v1",
        "type": "INPUT",
        "elements": {
          "gt0011": {
            "id": "gt0011",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0078.3]",
            "data_type": "DV_QUANTITY",
            "units": [
              "mmol/l",
              "mg/dl"
            ]
          }
        }
      },
      "gt0026": {
        "id": "gt0026",
        "model_id": "openEHR-EHR-OBSERVATION.mets_ir.v6",
        "template_id": "openEHR-EHR-OBSERVATION.mets_ir.v6",
        "type": "OUTPUT",
        "elements": {
          "gt0027": {
            "id": "gt0027",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0010]",
            "data_type": "DV_ORDINAL"
          },
          "gt0028": {
            "id": "gt0028",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0009]",
            "data_type": "DV_QUANTITY",
            "units": [
              "1"
            ]
          }
        }
      }
    },
    "default_actions": [
      "$gt0029|Local variable: HDL-c conversion factor|=1",
      "$gt0030|Local variable: Glucose conversion factor|=1",
      "$gt0031|Local variable: Triglyceride conversion factor|=1"
    ],
    "rules": {
      "gt0022": {
        "id": "gt0022",
        "priority": 6,
        "when": [
          "($gt0011|HDL-c|!=null)&&($gt0011|HDL-c|.unit=='mmol/l')"
        ],
        "then": [
          "$gt0029|Local variable: HDL-c conversion factor|=38.67"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 5,
        "when": [
          "($gt0005|Triglyceride concentration|!=null)&&($gt0005|Triglyceride concentration|.unit=='mmol/l')"
        ],
        "then": [
          "$gt0031|Local variable: Triglyceride conversion factor|=88.57"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 4,
        "when": [
          "($gt0003|Fasting Glucose|!=null)&&($gt0003|Fasting Glucose|.unit=='mmol/l')"
        ],
        "then": [
          "$gt0030|Local variable: Glucose conversion factor|=18"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 3,
        "when": [
          "$gt0003|Fasting Glucose|!=null",
          "$gt0005|Triglyceride concentration|!=null",
          "$gt0007|Body mass index|!=null",
          "$gt0011|HDL-c|!=null"
        ],
        "then": [
          "$gt0028|METS-IR score|.precision=1",
          "$gt0028|METS-IR score|.magnitude=(log((($gt0003|Fasting Glucose|.magnitude*$gt0030|Local variable: Glucose conversion factor|)*2)+($gt0005|Triglyceride concentration|.magnitude*$gt0031|Local variable: Triglyceride conversion factor|))*$gt0007|Body mass index|.magnitude)/(1*log($gt0011|HDL-c|.magnitude*$gt0029|Local variable: HDL-c conversion factor|))",
          "$gt0028|METS-IR score|.unit='1'"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 2,
        "when": [
          "$gt0028|METS-IR score|!=null",
          "$gt0028|METS-IR score|.magnitude<=50.39"
        ],
        "then": [
          "$gt0027|METS-IR Interpretation|=1|local::at0011|Low|"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 1,
        "when": [
          "$gt0028|METS-IR score|!=null",
          "$gt0028|METS-IR score|.magnitude>50.39"
        ],
        "then": [
          "$gt0027|METS-IR Interpretation|=2|local::at0012|High|"
        ]
      }
    }
  },
  "ontology": {
    "term_definitions": {
      "en": {
        "id": "en",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "METS-IR",
            "description": "The Metabolic Score for Insulin Resistance (METS-IR) is a novel, non-insulin-based metabolic index developed to quantify peripheral insulin sensitivity and assess the risk of insulin resistance and related cardiometabolic diseases in humans. Unlike traditional methods that require direct insulin measurement, METS-IR uses routinely available clinical data—fasting plasma glucose, triglycerides, high-density lipoprotein cholesterol (HDL-C), and body mass index (BMI)—to provide a simple and cost-effective alternative for evaluating insulin resistance"
          },
