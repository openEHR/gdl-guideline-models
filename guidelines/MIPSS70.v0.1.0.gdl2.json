    "pre_conditions": [
      "$gt0003|Karyotype information|==local::at0055|Not available|"
    ],
    "rules": {
      "gt0022": {
        "id": "gt0022",
        "priority": 4,
        "when": [
          "$gt0004|Hemoglobin <10 g/dL|!=null",
          "$gt0005|Platelet count <100,000 /µL|!=null",
          "$gt0006|Leukocyte count >25,000  /µL|!=null",
          "$gt0007|Circulating blasts ≥2%|!=null",
          "$gt0008|Fibrosis grade ≥2|!=null",
          "$gt0009|Constitutional symptoms|!=null",
          "$gt0010|Absence of CALR type 1-like mutation|!=null",
          "$gt0011|HMR category|!=null",
          "$gt0012|Two or more HMR mutations|!=null"
        ],
        "then": [
          "$gt0046|Total points|.magnitude=$gt0004|Hemoglobin <10 g/dL|.value+$gt0005|Platelet count <100,000 /µL|.value+$gt0006|Leukocyte count >25,000  /µL|.value+$gt0007|Circulating blasts ≥2%|.value+$gt0008|Fibrosis grade ≥2|.value+$gt0009|Constitutional symptoms|.value+$gt0010|Absence of CALR type 1-like mutation|.value+$gt0011|HMR category|.value+$gt0012|Two or more HMR mutations|.value"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 3,
        "when": [
          "$gt0046|Total points|<=1"
        ],
        "then": [
          "$gt0043|Risk MIPSS70|=local::at0009|Low|",
          "$gt0044|Median Overall Survival|=local::at0014|27.7|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 2,
        "when": [
          "$gt0046|Total points|>=2&&$gt0046|Total points|<=4"
        ],
        "then": [
          "$gt0043|Risk MIPSS70|=local::at0010|Intermediate|",
          "$gt0044|Median Overall Survival|=local::at0016|7.1|"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 1,
        "when": [
          "$gt0046|Total points|>=5"
        ],
        "then": [
          "$gt0043|Risk MIPSS70|=local::at0011|High|",
          "$gt0044|Median Overall Survival|=local::at0019|2.3|"
        ]
      }
    }
  },
