        "model_id": "openEHR-EHR-EVALUATION.mipps70_assessment.v0",
        "template_id": "openEHR-EHR-EVALUATION.mipps70_assessment.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0043": {
            "id": "gt0043",
            "path": "/data[at0003]/items[at0004]",
            "data_type": "DV_CODED_TEXT",
            "allowed_values": [
              "local::at0008|Very Low|",
              "local::at0009|Low|",
              "local::at0010|Intermediate|",
              "local::at0011|High|",
              "local::at0012|Very High|"
            ]
          },
          "gt0044": {
            "id": "gt0044",
            "path": "/data[at0003]/items[at0005]",
            "data_type": "DV_CODED_TEXT",
            "allowed_values": [
              "local::at0013|N/A|",
              "local::at0014|27.7|",
              "local::at0015|10.3|",
              "local::at0016|7.1|",
              "local::at0017|7.0|",
              "local::at0018|3.5|",
              "local::at0019|2.3|",
              "local::at0020|1.8|"
            ]
          },
          "gt0045": {
            "id": "gt0045",
            "path": "/data[at0003]/items[at0007]",
            "data_type": "DV_COUNT"
          }
        }
      }
    },
    "pre_conditions": [
      "$gt0003|Karyotype information|==local::at0054|Available|"
    ],
    "rules": {
      "gt0021": {
        "id": "gt0021",
        "priority": 6,
        "when": [
          "$gt0013|Hemoglobin, mg/dL|!=null",
          "$gt0014|Unfavorable karyotype|!=null",
          "$gt0015|Very high-risk karyotype|!=null",
          "$gt0007|Circulating blasts ≥2%|!=null",
          "$gt0039|Absence of CALR type 1-like mutation|!=null",
          "$gt0038|Constitutional symptoms|!=null",
          "$gt0040|HMR category|!=null",
          "$gt0041|Two or more HMR mutations|!=null"
        ],
        "then": [
          "$gt0045|Total points|.magnitude=$gt0013|Hemoglobin, mg/dL|.value+$gt0038|Constitutional symptoms|.value+$gt0039|Absence of CALR type 1-like mutation|.value+$gt0040|HMR category|.value+$gt0041|Two or more HMR mutations|.value+$gt0014|Unfavorable karyotype|.value+$gt0015|Very high-risk karyotype|.value"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 5,
        "when": [
          "$gt0045|Total points|==0"
        ],
        "then": [
          "$gt0043|Risk MIPSS70|=local::at0008|Very Low|",
          "$gt0044|Median Overall Survival|=local::at0013|N/A|"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 4,
        "when": [
          "$gt0045|Total points|>=1&&$gt0045|Total points|<=2"
        ],
        "then": [
          "$gt0043|Risk MIPSS70|=local::at0009|Low|",
          "$gt0044|Median Overall Survival|=local::at0015|10.3|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 1,
        "when": [
          "$gt0045|Total points|>9"
        ],
        "then": [
          "$gt0043|Risk MIPSS70|=local::at0012|Very High|",
          "$gt0044|Median Overall Survival|=local::at0020|1.8|"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 2,
        "when": [
          "$gt0045|Total points|>=5&&$gt0045|Total points|<=8"
        ],
        "then": [
          "$gt0043|Risk MIPSS70|=local::at0011|High|",
          "$gt0044|Median Overall Survival|=local::at0018|3.5|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 3,
        "when": [
          "$gt0045|Total points|>=3&&$gt0045|Total points|<=4"
        ],
        "then": [
          "$gt0043|Risk MIPSS70|=local::at0010|Intermediate|",
          "$gt0044|Median Overall Survival|=local::at0017|7.0|"
        ]
      }
    }
  },
