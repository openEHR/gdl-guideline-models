{
  "id": "Modified_SOAR_Score_for_Stroke.v1",
  "gdl_version": "2.0",
  "concept": "gt0001",
  "language": {
    "original_language": "ISO_639-1::en"
  },
  "description": {
    "original_author": {
      "date": "2020-02-29",
      "name": "Jack MSonkho",
      "organisation": "Cambio CDS",
