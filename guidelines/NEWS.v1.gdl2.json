    "rules": {
      "gt0026": {
        "id": "gt0026",
        "priority": 37,
        "when": [
          "$gt0017|Respiration rate|==null",
          "$gt0018|Oxygen saturation|==null",
          "$gt0019|Supplemental oxygen|==null",
          "$gt0020|Body temperature|==null",
          "$gt0022|Systolic blood pressure|==null",
          "$gt0023|Heart rate|==null",
          "$gt0024|Level of consciousness|==null"
        ],
        "then": [
          "$gt0017|Respiration rate|=0|local::at0018|12-20|",
          "$gt0018|Oxygen saturation|=0|local::at0030|≥96|",
          "$gt0019|Supplemental oxygen|=0|local::at0036|No|",
          "$gt0020|Body temperature|=0|local::at0022|36.1-38.0|",
          "$gt0022|Systolic blood pressure|=0|local::at0014|111-219|",
          "$gt0023|Heart rate|=0|local::at0013|51-90|",
          "$gt0024|Level of consciousness|=0|local::at0024|A|"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 36,
        "when": [
          "$gt0010|Respiration rate|.magnitude<=20",
          "$gt0010|Respiration rate|.magnitude>=12"
        ],
        "then": [
          "$gt0017|Respiration rate|=0|local::at0018|12-20|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 35,
        "when": [
          "$gt0010|Respiration rate|.magnitude<=11",
          "$gt0010|Respiration rate|.magnitude>=9"
        ],
        "then": [
          "$gt0017|Respiration rate|=1|local::at0019|9-11|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 34,
        "when": [
          "$gt0010|Respiration rate|.magnitude<=24",
          "$gt0010|Respiration rate|.magnitude>=21"
        ],
        "then": [
          "$gt0017|Respiration rate|=2|local::at0020|21-24|"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 33,
        "when": [
          "($gt0010|Respiration rate|.magnitude<=8)||($gt0010|Respiration rate|.magnitude>=25)"
        ],
        "then": [
          "$gt0017|Respiration rate|=3|local::at0021|≤8 or ≥25|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 32,
        "when": [
          "$gt0012|SaO2|.magnitude>=96"
        ],
        "then": [
          "$gt0018|Oxygen saturation|=0|local::at0030|≥96|"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 31,
        "when": [
          "$gt0012|SaO2|.magnitude>=94",
          "$gt0012|SaO2|.magnitude<=95"
        ],
        "then": [
          "$gt0018|Oxygen saturation|=1|local::at0031|94-95|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 30,
        "when": [
          "$gt0012|SaO2|.magnitude>=92",
          "$gt0012|SaO2|.magnitude<=93"
        ],
        "then": [
          "$gt0018|Oxygen saturation|=2|local::at0032|92-93|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 29,
        "when": [
          "$gt0012|SaO2|.magnitude<=91"
        ],
        "then": [
          "$gt0018|Oxygen saturation|=3|local::at0033|≤91|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 28,
        "when": [
          "$gt0015|Temperature|.magnitude>=36.1",
          "$gt0015|Temperature|.magnitude<=38.0"
        ],
        "then": [
          "$gt0020|Body temperature|=0|local::at0022|36.1-38.0|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 27,
        "when": [
          "(($gt0015|Temperature|.magnitude>=35.1)&&($gt0015|Temperature|.magnitude<=36.0))||(($gt0015|Temperature|.magnitude>=38.1)&&($gt0015|Temperature|.magnitude<=39.0))"
        ],
        "then": [
          "$gt0020|Body temperature|=1|local::at0023|35.1-36.0 or 38.1-39.0|"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 26,
        "when": [
          "$gt0015|Temperature|.magnitude>=39.1"
        ],
        "then": [
          "$gt0020|Body temperature|=2|local::at0038|≥39.1|"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 25,
        "when": [
          "$gt0015|Temperature|.magnitude<=35.0"
        ],
        "then": [
          "$gt0020|Body temperature|=3|local::at0039|≤35.0|"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 24,
        "when": [
          "$gt0014|Systolic|.magnitude>=111",
          "$gt0014|Systolic|.magnitude<=219"
        ],
        "then": [
          "$gt0022|Systolic blood pressure|=0|local::at0014|111-219|"
        ]
      },
      "gt0040": {
        "id": "gt0040",
        "priority": 23,
        "when": [
          "$gt0014|Systolic|.magnitude>=101",
          "$gt0014|Systolic|.magnitude<=110"
        ],
        "then": [
          "$gt0022|Systolic blood pressure|=1|local::at0015|101-110|"
        ]
      },
      "gt0041": {
        "id": "gt0041",
        "priority": 22,
        "when": [
          "$gt0014|Systolic|.magnitude>=91",
          "$gt0014|Systolic|.magnitude<=100"
        ],
        "then": [
          "$gt0022|Systolic blood pressure|=2|local::at0016|91-100|"
        ]
      },
      "gt0042": {
        "id": "gt0042",
        "priority": 21,
        "when": [
          "($gt0014|Systolic|.magnitude<=90)||($gt0014|Systolic|.magnitude>=220)"
        ],
        "then": [
          "$gt0022|Systolic blood pressure|=3|local::at0017|≤90 or ≥220|"
        ]
      },
      "gt0043": {
        "id": "gt0043",
        "priority": 20,
        "when": [
          "$gt0013|Rate|.magnitude>=51",
          "$gt0013|Rate|.magnitude<=90"
        ],
        "then": [
          "$gt0023|Heart rate|=0|local::at0013|51-90|"
        ]
      },
      "gt0044": {
        "id": "gt0044",
        "priority": 19,
        "when": [
          "(($gt0013|Rate|.magnitude>=41)&&($gt0013|Rate|.magnitude<=50))||(($gt0013|Rate|.magnitude>=91)&&($gt0013|Rate|.magnitude<=110))"
        ],
        "then": [
          "$gt0023|Heart rate|=1|local::at0012|41-50 or 91-110|"
        ]
      },
      "gt0045": {
        "id": "gt0045",
        "priority": 18,
        "when": [
          "$gt0013|Rate|.magnitude>=111",
          "$gt0013|Rate|.magnitude<=130"
        ],
        "then": [
          "$gt0023|Heart rate|=2|local::at0011|111-130|"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 17,
        "when": [
          "($gt0013|Rate|.magnitude<=40)||($gt0013|Rate|.magnitude>=131)"
        ],
        "then": [
          "$gt0023|Heart rate|=3|local::at0010|≤40 or ≥131|"
        ]
      },
      "gt0047": {
        "id": "gt0047",
        "priority": 16,
        "when": [
          "$gt0016|AVPU Observation|==local::at0005|Alert|"
        ],
        "then": [
          "$gt0024|Level of consciousness|=0|local::at0024|A|"
        ]
      },
      "gt0048": {
        "id": "gt0048",
        "priority": 15,
        "when": [
          "$gt0016|AVPU Observation|!=null",
          "!($gt0016|AVPU Observation|==local::at0005|Alert|)"
        ],
        "then": [
          "$gt0024|Level of consciousness|=3|local::at0025|V, P or U|"
        ]
      },
      "gt0049": {
        "id": "gt0049",
        "priority": 14,
        "when": [
          "$gt0011|Oxygen therapy|==0|local::at0006|Absent|"
        ],
        "then": [
          "$gt0019|Supplemental oxygen|=0|local::at0036|No|"
        ]
      },
      "gt0050": {
        "id": "gt0050",
        "priority": 13,
        "when": [
          "$gt0011|Oxygen therapy|==1|local::at0007|Present|"
        ],
        "then": [
          "$gt0019|Supplemental oxygen|=2|local::at0037|Yes|"
        ]
      },
      "gt0051": {
        "id": "gt0051",
        "priority": 12,
        "then": [
          "$gt0025|Total score|.magnitude=((((($gt0017.value+$gt0018.value)+$gt0019.value)+$gt0020.value)+$gt0022.value)+$gt0023.value)+$gt0024.value"
        ]
      },
      "gt0055": {
        "id": "gt0055",
        "priority": 11,
        "when": [
          "$gt0025|Total score|==0"
        ],
        "then": [
          "$gt0053|NEWS Assessment|=0|local::at0004|Zero points|"
        ]
      },
      "gt0056": {
        "id": "gt0056",
        "priority": 10,
        "when": [
          "$gt0025|Total score|<=6",
          "$gt0025|Total score|>=5"
        ],
        "then": [
          "$gt0053|NEWS Assessment|=2|local::at0006|Medium-risk group|"
        ]
      },
      "gt0057": {
        "id": "gt0057",
        "priority": 9,
        "when": [
          "($gt0018|Oxygen saturation|==3|local::at0033|≤91|)||($gt0022|Systolic blood pressure|==3|local::at0017|≤90 or ≥220|)",
          "$gt0053|NEWS Assessment|==null"
        ],
        "then": [
          "$gt0053|NEWS Assessment|=2|local::at0006|Medium-risk group|"
        ]
      },
      "gt0058": {
        "id": "gt0058",
        "priority": 8,
        "when": [
          "($gt0020|Body temperature|==3|local::at0039|≤35.0|)||($gt0024|Level of consciousness|==3|local::at0025|V, P or U|)",
          "$gt0053|NEWS Assessment|==null"
        ],
        "then": [
          "$gt0053|NEWS Assessment|=2|local::at0006|Medium-risk group|"
        ]
      },
      "gt0059": {
        "id": "gt0059",
        "priority": 7,
        "when": [
          "($gt0023|Heart rate|==3|local::at0010|≤40 or ≥131|)||($gt0017|Respiration rate|==3|local::at0021|≤8 or ≥25|)",
          "$gt0053|NEWS Assessment|==null"
        ],
        "then": [
          "$gt0053|NEWS Assessment|=2|local::at0006|Medium-risk group|"
        ]
      },
      "gt0060": {
        "id": "gt0060",
        "priority": 6,
        "when": [
          "$gt0053|NEWS Assessment|==null",
          "$gt0025|Total score|>0",
          "$gt0025|Total score|<=4"
        ],
        "then": [
          "$gt0053|NEWS Assessment|=1|local::at0005|Low-risk group|"
        ]
      },
      "gt0061": {
        "id": "gt0061",
        "priority": 5,
        "when": [
          "$gt0025|Total score|>=7"
        ],
        "then": [
          "$gt0053|NEWS Assessment|=3|local::at0007|High-risk group|"
        ]
      },
      "gt0062": {
        "id": "gt0062",
        "priority": 4,
        "when": [
          "$gt0053|NEWS Assessment|==0|local::at0004|Zero points|"
        ],
        "then": [
          "$gt0054|Frequency of monitoring|=0|local::at0008|Minimum 12 hourly|"
        ]
      },
      "gt0063": {
        "id": "gt0063",
        "priority": 3,
        "when": [
          "$gt0053|NEWS Assessment|==1|local::at0005|Low-risk group|"
        ],
        "then": [
          "$gt0054|Frequency of monitoring|=1|local::at0009|Minimum 4-6 hourly|"
        ]
      },
      "gt0064": {
        "id": "gt0064",
        "priority": 2,
        "when": [
          "$gt0053|NEWS Assessment|==2|local::at0006|Medium-risk group|"
        ],
        "then": [
          "$gt0054|Frequency of monitoring|=2|local::at0010|Increased frequency to a minimum of 1 hourly|"
        ]
      },
      "gt0065": {
        "id": "gt0065",
        "priority": 1,
        "when": [
          "$gt0053|NEWS Assessment|==3|local::at0007|High-risk group|"
        ],
        "then": [
          "$gt0054|Frequency of monitoring|=3|local::at0011|Continuous monitoring of vital signs|"
        ]
      }
    }
  },
  "ontology": {
    "term_definitions": {
      "sv": {
        "id": "sv",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "National Early Warning Score",
            "description": "National Early Warning Score (NEWS) är en standardiserad metod utvecklad för att objektivt gradera och utvärdera vitala parametrar. Den aktuella versionen är utvecklad av UK Royal College of Physicians."
          },
          "gt0010": {
            "id": "gt0010",
            "text": "Andningsfrekvens",
            "description": "Rate of respiration."
          },
          "gt0011": {
            "id": "gt0011",
            "text": "Pågående syrgasbehandling",
            "description": "The patient is receiving oxygen therapy by mask or nasal cannulae."
          },
          "gt0012": {
            "id": "gt0012",
            "text": "Syremättnad",
            "description": "The saturation of haem binding with oxygen."
          },
          "gt0013": {
            "id": "gt0013",
            "text": "Pulsfrekvens",
            "description": "The rate, measured in beats per minute."
          },
          "gt0014": {
            "id": "gt0014",
            "text": "Systoliskt blodtryck",
            "description": "Peak systemic arterial blood pressure  - measured in systolic or contraction phase of the heart cycle."
          },
          "gt0015": {
            "id": "gt0015",
            "text": "Temperatur",
            "description": "The measured body temperature (as a surrogate for the whole body)."
          },
          "gt0016": {
            "id": "gt0016",
            "text": "AVPU",
            "description": "The observation of the patient's responsiveness."
          },
          "gt0017": {
            "id": "gt0017",
            "text": "Andningsfrekvens",
            "description": "Range category for the respiratory rate (RR) measurement of a patient."
          },
          "gt0018": {
            "id": "gt0018",
            "text": "Syremättnad",
            "description": "Range category for the oxygen saturation measurement of a patient."
          },
          "gt0019": {
            "id": "gt0019",
            "text": "Syrgastillförsel",
            "description": "Range category depending on whether the patient is receiving supplemental oxygen."
