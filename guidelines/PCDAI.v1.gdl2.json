    "rules": {
      "gt0045": {
        "id": "gt0045",
        "priority": 24,
        "when": [
          "$gt0056|Gender|!=null"
        ],
        "then": [
          "$gt0055|Gender|=$gt0056|Gender|"
        ]
      },
      "gt0044": {
        "id": "gt0044",
        "priority": 15,
        "when": [
          "$gt0057|Birthdate|!=null"
        ],
        "then": [
          "$gt0058|Age|.unit='a'",
          "$gt0058|Age|.magnitude=$currentDateTime.year-$gt0057.year"
        ]
      },
      "gt0041": {
        "id": "gt0041",
        "priority": 14,
        "when": [
          "(($gt0058|Age|<10,a)&&($gt0028|Packed cell volume (PCV)|>33,%))||((($gt0055|Gender|==local::at0006|Female|)&&(($gt0058|Age|<=19,a)&&(($gt0058|Age|>=11,a)&&($gt0028|Packed cell volume (PCV)|>=34,%))))||((($gt0055|Gender|==local::at0005|Male|)&&(($gt0058|Age|<=14,a)&&(($gt0058|Age|>=11,a)&&($gt0028|Packed cell volume (PCV)|>=35,%))))||(($gt0055|Gender|==local::at0005|Male|)&&(($gt0058|Age|<=19,a)&&(($gt0058|Age|>=15,a)&&($gt0028|Packed cell volume (PCV)|>=37,%))))))"
        ],
        "then": [
          "$gt0012|2x HCT score|=0|local::at0046|>33% depending on age and sex|"
        ]
      },
      "gt0042": {
        "id": "gt0042",
        "priority": 13,
        "when": [
          "((($gt0058|Age|<10,a)&&(($gt0028|Packed cell volume (PCV)|<=32,%)&&($gt0028|Packed cell volume (PCV)|>=28,%)))||(($gt0055|Gender|==local::at0006|Female|)&&(($gt0058|Age|<=19,a)&&(($gt0058|Age|>=11,a)&&(($gt0028|Packed cell volume (PCV)|<=33,%)&&($gt0028|Packed cell volume (PCV)|>=29,%))))))||(((($gt0058|Age|<=14,a)&&(($gt0058|Age|>=11,a)&&(($gt0028|Packed cell volume (PCV)|<=34,%)&&($gt0028|Packed cell volume (PCV)|>=30,%))))||(($gt0058|Age|<=19,a)&&(($gt0058|Age|>=15,a)&&(($gt0028|Packed cell volume (PCV)|<=36,%)&&($gt0028|Packed cell volume (PCV)|>=32,%)))))&&($gt0055|Gender|==local::at0005|Male|))"
        ],
        "then": [
          "$gt0012|2x HCT score|=5|local::at0048|28-36% depending on age and sex|"
        ]
      },
      "gt0043": {
        "id": "gt0043",
        "priority": 12,
        "when": [
          "((($gt0058|Age|<10,a)&&($gt0028|Packed cell volume (PCV)|<28,%))||(($gt0055|Gender|==local::at0006|Female|)&&(($gt0058|Age|<=19,a)&&(($gt0058|Age|>=11,a)&&($gt0028|Packed cell volume (PCV)|<29,%)))))||(((($gt0058|Age|<=14,a)&&(($gt0058|Age|>=11,a)&&($gt0028|Packed cell volume (PCV)|<30,%)))||(($gt0058|Age|>=15,a)&&(($gt0058|Age|<=19,a)&&($gt0028|Packed cell volume (PCV)|<32,%))))&&($gt0055|Gender|==local::at0005|Male|))"
        ],
        "then": [
          "$gt0012|2x HCT score|=10|local::at0049|<32% depending on age and sex|"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 11,
        "when": [
          "$gt0029|Erythrocyte sedimentation rate (ESR)|<20,mm/h"
        ],
        "then": [
          "$gt0013|ESR score|=0|local::at0050|< 20|"
        ]
      },
      "gt0047": {
        "id": "gt0047",
        "priority": 10,
        "when": [
          "$gt0029|Erythrocyte sedimentation rate (ESR)|>=20,mm/h",
          "$gt0029|Erythrocyte sedimentation rate (ESR)|<=50,mm/h"
        ],
        "then": [
          "$gt0013|ESR score|=5|local::at0051|20-50|"
        ]
      },
      "gt0048": {
        "id": "gt0048",
        "priority": 9,
        "when": [
          "$gt0029|Erythrocyte sedimentation rate (ESR)|>50,mm/h"
        ],
        "then": [
          "$gt0013|ESR score|=10|local::at0052|> 50|"
        ]
      },
      "gt0049": {
        "id": "gt0049",
        "priority": 8,
        "when": [
          "$gt0025|Serum albumin|>=3.5,gm/dl"
        ],
        "then": [
          "$gt0014|Albumin score|=0|local::at0053|≥3.5 (35)|"
        ]
      },
      "gt0050": {
        "id": "gt0050",
        "priority": 7,
        "when": [
          "$gt0025|Serum albumin|<=3.4,gm/dl",
          "$gt0025|Serum albumin|>=3.1,gm/dl"
        ],
        "then": [
          "$gt0014|Albumin score|=5|local::at0054|3.1–3.4 (31–34)|"
        ]
      },
      "gt0051": {
        "id": "gt0051",
        "priority": 6,
        "when": [
          "$gt0025|Serum albumin|<=3,gm/dl"
        ],
        "then": [
          "$gt0014|Albumin score|=10|local::at0055|≤3.0 (30)|"
        ]
      },
      "gt0052": {
        "id": "gt0052",
        "priority": 5,
        "when": [
          "$gt0022|Peri-rectal disease|!=null",
          "$gt0016|Abdominal pain|!=null",
          "$gt0017|Stools per day|!=null",
          "$gt0018|General well-being|!=null",
          "$gt0020|Height at diagnosis (or follow up) score|!=null",
          "$gt0021|Abdomen|!=null",
          "$gt0023|Extra-intestinal manifestations|!=null",
          "$gt0040|Weight score|!=null",
          "$gt0012|2x HCT score|!=null",
          "$gt0013|ESR score|!=null",
          "$gt0014|Albumin score|!=null"
        ],
        "then": [
          "$gt0015|Total Score|.precision=2",
          "$gt0015|Total Score|.magnitude=$gt0022.value+$gt0023.value+$gt0012.value/2+$gt0013.value+$gt0020.value+$gt0021.value+$gt0014.value+$gt0016.value+$gt0017.value+$gt0018.value+$gt0040.value"
        ]
      },
      "gt0061": {
        "id": "gt0061",
        "priority": 4,
        "when": [
          "$gt0015|Total Score|.magnitude<10"
        ],
        "then": [
          "$gt0060|Score interpretation|=0|local::at0003|Remission|"
        ]
      },
      "gt0062": {
        "id": "gt0062",
        "priority": 3,
        "when": [
          "$gt0015|Total Score|.magnitude<=27.5",
          "$gt0015|Total Score|.magnitude>=10"
        ],
        "then": [
          "$gt0060|Score interpretation|=1|local::at0004|Mild Crohn’s Disease|"
        ]
      },
      "gt0063": {
        "id": "gt0063",
        "priority": 2,
        "when": [
          "$gt0015|Total Score|.magnitude<=37.5",
          "$gt0015|Total Score|.magnitude>=30"
        ],
        "then": [
          "$gt0060|Score interpretation|=2|local::at0005|Moderate Crohn’s Disease|"
        ]
      },
      "gt0064": {
        "id": "gt0064",
        "priority": 1,
        "when": [
          "$gt0015|Total Score|.magnitude>=40"
        ],
        "then": [
          "$gt0060|Score interpretation|=3|local::at0006|Severe Crohn’s Disease |"
        ]
      }
    }
  },
