      "gt0082": {
        "id": "gt0082",
        "priority": 12,
        "when": [
          "(($gt0122|Leukocytosis for Age|==true)||($gt0123|Leukopenia|==true))||(($gt0120|Significant Child Fever (>38.5 C)|==true)||($gt0131|Hypothermia|==true))"
        ]
      },
      "gt0083": {
        "id": "gt0083",
        "priority": 11,
        "when": [
          "($gt0125|Tachycardia for Age (fever-corrected) |==true)||($gt0128|Tachypnea for Age (fever corrected) |==true)"
        ]
      },
      "gt0072": {
        "id": "gt0072",
        "priority": 10,
        "when": [
          "!fired($gt0072)",
          "fired($gt0083)",
          "fired($gt0082)"
        ],
        "then": [
          "$gt0133|Pediatric mSIRS|=true"
        ]
      },
      "gt0073": {
        "id": "gt0073",
        "priority": 9,
        "when": [
          "$gt0018|Age in Days|>6570,d",
          "(((($gt0123|Leukopenia|==true)||($gt0121|Leukocytosis|==true))&&($gt0127|Tachypnea|==true))||((($gt0123|Leukopenia|==true)||($gt0121|Leukocytosis|==true))&&($gt0124|Relative Tachycardia (>90)|==true)))||((((($gt0119|Fever|==true)||($gt0131|Hypothermia|==true))&&($gt0127|Tachypnea|==true))||((($gt0119|Fever|==true)||($gt0131|Hypothermia|==true))&&($gt0124|Relative Tachycardia (>90)|==true)))||(((($gt0121|Leukocytosis|==true)||($gt0123|Leukopenia|==true))&&(($gt0119|Fever|==true)||($gt0131|Hypothermia|==true)))||(($gt0127|Tachypnea|==true)&&($gt0124|Relative Tachycardia (>90)|==true))))",
          "!fired($gt0073)"
        ],
        "then": [
          "$gt0134|Adult SIRS|=true"
        ]
      },
      "gt0074": {
        "id": "gt0074",
        "priority": 8,
        "when": [
          "$gt0018|Age in Days|>6570,d",
          "$gt0011|Systolic BP|<=100,mm[Hg]",
          "$gt0012|Respiration rate|>=22,/min"
        ],
        "then": [
          "$gt0135|2 of 3 qSOFA|=true"
        ]
      },
      "gt0075": {
        "id": "gt0075",
        "priority": 7,
        "when": [
          "$gt0149|Shock Index|>=0.8"
        ],
        "then": [
          "$gt0136|Elevated Shock Index|=true"
        ]
      },
      "gt0088": {
        "id": "gt0088",
        "priority": 6,
        "when": [
          "$gt0011|Systolic BP|!=null",
          "$gt0009|Heart rate|!=null",
          "$gt0018|Age in Days|>6570,d"
        ],
        "then": [
          "$gt0149|Shock Index|.precision=2",
          "$gt0149|Shock Index|.magnitude=$gt0009.magnitude/$gt0011.magnitude"
        ]
      },
      "gt0093": {
        "id": "gt0093",
        "priority": 5,
        "when": [
          "$gt0010|Temperature|<36"
        ],
        "then": [
          "$gt0131|Hypothermia|=true"
        ]
      },
      "gt0094": {
        "id": "gt0094",
        "priority": 4,
        "when": [
          "$gt0011|Systolic BP|<60,mm[Hg]",
          "$gt0018|Age in Days|<=30,d"
        ],
        "then": [
          "$gt0132|Hypotension for Age|=true"
        ]
      },
      "gt0096": {
        "id": "gt0096",
        "priority": 3,
        "when": [
          "$gt0018|Age in Days|>30,d",
          "$gt0018|Age in Days|<=730,d",
          "$gt0011|Systolic BP|<70,mm[Hg]"
        ],
        "then": [
          "$gt0132|Hypotension for Age|=true"
        ]
      },
      "gt0095": {
        "id": "gt0095",
        "priority": 2,
        "when": [
          "$gt0011|Systolic BP|.magnitude<(70+(2*($gt0018.magnitude/365)))",
          "$gt0018|Age in Days|<3650,d",
          "$gt0018|Age in Days|>=730,d"
        ],
        "then": [
          "$gt0132|Hypotension for Age|=true"
        ]
      },
      "gt0097": {
        "id": "gt0097",
        "priority": 1,
        "when": [
          "$gt0011|Systolic BP|<90,mm[Hg]",
          "$gt0018|Age in Days|>3650,d"
        ],
        "then": [
          "$gt0137|Hypotension|=true"
        ]
      }
    }
