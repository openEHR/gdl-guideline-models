    "rules": {
      "gt0025": {
        "id": "gt0025",
        "priority": 45,
        "when": [
          "$gt0008|Birthdate|!=null"
        ],
        "then": [
          "$gt0018|Age in Days|.unit='d'",
          "$gt0018|Age in Days|.magnitude=($currentDateTime-$gt0008)/86400000"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 44,
        "when": [
          "$gt0018|Age in Days|<730,d",
          "$gt0012|Respiration rate|!=null",
          "$gt0010|Temperature|!=null"
        ],
        "then": [
          "$gt0031|Temperature Corrected RR|.unit='/min'",
          "$gt0031|Temperature Corrected RR|.magnitude=$gt0012.magnitude-(($gt0010.magnitude-37)*7)"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 43,
        "when": [
          "$gt0018|Age in Days|<6570,d",
          "$gt0018|Age in Days|>=730,d",
          "$gt0012|Respiration rate|!=null",
          "$gt0010|Temperature|!=null"
        ],
        "then": [
          "$gt0031|Temperature Corrected RR|.magnitude=$gt0012.magnitude-(($gt0010.magnitude-37)*5)",
          "$gt0031|Temperature Corrected RR|.unit='/min'"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 42,
        "when": [
          "$gt0018|Age in Days|<6570,d",
          "$gt0010|Temperature|!=null",
          "$gt0009|Heart rate|!=null"
        ],
        "then": [
          "$gt0029|Temperature Corrected HR|.magnitude=$gt0009.magnitude-(($gt0010.magnitude-37)*10)",
          "$gt0029|Temperature Corrected HR|.unit='/min'"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 41,
        "when": [
          "$gt0018|Age in Days|<6570,d",
          "$gt0010|Temperature|>=38.5"
        ],
        "then": [
          "$gt0120|Significant Child Fever (>38.5 C)|=true"
        ]
      },
      "gt0043": {
        "id": "gt0043",
        "priority": 40,
        "when": [
          "$gt0010|Temperature|>=38",
          "$gt0018|Age in Days|>=6570,d"
        ],
        "then": [
          "$gt0119|Fever|=true"
        ]
      },
      "gt0059": {
        "id": "gt0059",
        "priority": 39,
        "when": [
          "$gt0029|Temperature Corrected HR|>=180,/min",
          "$gt0018|Age in Days|<=7,d"
        ],
        "then": [
          "$gt0125|Tachycardia for Age (fever-corrected) |=true"
        ]
      },
      "gt0061": {
        "id": "gt0061",
        "priority": 38,
        "when": [
          "$gt0029|Temperature Corrected HR|>=190,/min",
          "$gt0018|Age in Days|<=30,d",
          "$gt0018|Age in Days|>7,d"
        ],
        "then": [
          "$gt0128|Tachypnea for Age (fever corrected) |=true"
        ]
      },
      "gt0058": {
        "id": "gt0058",
        "priority": 37,
        "when": [
          "$gt0029|Temperature Corrected HR|>=180,/min",
          "$gt0018|Age in Days|<=730,d",
          "$gt0018|Age in Days|>30,d"
        ],
        "then": [
          "$gt0125|Tachycardia for Age (fever-corrected) |=true"
        ]
      },
      "gt0057": {
        "id": "gt0057",
        "priority": 36,
        "when": [
          "$gt0018|Age in Days|>730,d",
          "$gt0018|Age in Days|<=1825,d",
          "$gt0029|Temperature Corrected HR|>=160,/min"
        ],
        "then": [
          "$gt0125|Tachycardia for Age (fever-corrected) |=true"
        ]
      },
      "gt0056": {
        "id": "gt0056",
        "priority": 35,
        "when": [
          "$gt0018|Age in Days|>1825,d",
          "$gt0018|Age in Days|<=4380,d",
          "$gt0029|Temperature Corrected HR|>=140,/min"
        ],
        "then": [
          "$gt0125|Tachycardia for Age (fever-corrected) |=true"
        ]
      },
      "gt0044": {
        "id": "gt0044",
        "priority": 34,
        "when": [
          "$gt0018|Age in Days|<=6570,d",
          "$gt0018|Age in Days|>4380,d",
          "$gt0029|Temperature Corrected HR|>=130,/min"
        ],
        "then": [
          "$gt0125|Tachycardia for Age (fever-corrected) |=true"
        ]
      },
      "gt0055": {
        "id": "gt0055",
        "priority": 33,
        "when": [
          "$gt0018|Age in Days|>6570,d",
          "$gt0009|Heart rate|>=90,/min"
        ],
        "then": [
          "$gt0124|Relative Tachycardia (>90)|=true"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 32,
        "when": [
          "$gt0009|Heart rate|<100,/min",
          "$gt0018|Age in Days|<=30,d"
        ],
        "then": [
          "$gt0130|Bradycardia for Age|=true"
        ]
      },
      "gt0047": {
        "id": "gt0047",
        "priority": 31,
        "when": [
          "$gt0009|Heart rate|<90,/min",
          "$gt0018|Age in Days|<=730,d",
          "$gt0018|Age in Days|>30,d"
        ],
        "then": [
          "$gt0130|Bradycardia for Age|=true"
        ]
      },
      "gt0048": {
        "id": "gt0048",
        "priority": 30,
        "when": [
          "$gt0013|White cell count|>=34,10*9/l",
          "$gt0018|Age in Days|<=7,d"
        ],
        "then": [
          "$gt0122|Leukocytosis for Age|=true"
        ]
      },
      "gt0049": {
        "id": "gt0049",
        "priority": 29,
        "when": [
          "$gt0013|White cell count|>=19.5,10*9/l",
          "$gt0018|Age in Days|<=30,d",
          "$gt0018|Age in Days|>7,d"
        ],
        "then": [
          "$gt0122|Leukocytosis for Age|=true"
        ]
      },
      "gt0050": {
        "id": "gt0050",
        "priority": 28,
        "when": [
          "$gt0013|White cell count|>=17.5,10*9/l",
          "$gt0018|Age in Days|<=730,d",
          "$gt0018|Age in Days|>30,d"
        ],
        "then": [
          "$gt0122|Leukocytosis for Age|=true"
        ]
      },
      "gt0051": {
