        ]
      },
      "gt0070": {
        "id": "gt0070",
        "priority": 14,
        "when": [
          "$gt0018|Age in Days|>1825,d",
          "$gt0018|Age in Days|<=6570,d",
          "$gt0013|White cell count|<4.5,10*9/l"
        ],
