    "lifecycle_state" : "DRAFTED",
    "details" : {
      "en" : {
        "id" : "en",
        "purpose" : "To generate a recommendation to abstain or perform a prostate biopsy.",
        "keywords" : [
          "prostate ",
          "cancer",
          "biopsy",
          "PI-RADS",
          " PSA",
          "density",
          "recommendation",
          "score"
        ],
        "use" : "Use to evaluate the need for a biopsy in patients using the PI-RADS score and PSA Density.",
        "misuse" : "Not to be used in patients without recorded PI-RADS score and PSA density.\nNot to be used in isolation without considering other clinical factors."
      }
    },
    "other_details" : {
      "references" : "Schoots IG, Padhani AR. Risk-adapted biopsy decision based on prostate magnetic resonance imaging and prostate-specific antigen density for enhanced biopsy avoidance in first prostate cancer diagnostic evaluation. BJU Int. 2021 Feb;127(2):175-178. doi: 10.1111/bju.15277. Epub 2020 Nov 13. PMID: 33089586; PMCID: PMC7894174.\n\nNationellt vårdprogram prostatacancer [Internet]. Regionala cancercentrum i samverkan; 2023 [cited 2025 Nov 20]. Available from: https://kunskapsbanken.cancercentrum.se/diagnoser/prostatacancer/vardprogram/klinisk-diagnostik/#chapter-7-6-Prostatabiopsier",
      "en" : "Recommendations regarding prostate biopsies based on PI-RADS levels and PSA density."
    }
  },
  "definition" : {
    "data_bindings" : {
      "gt0002" : {
        "id" : "gt0002",
        "model_id" : "openEHR-EHR-OBSERVATION.prostate_biopsy_recommendation.v0",
        "template_id" : "openEHR-EHR-OBSERVATION.prostate_biopsy_recommendation.v0",
        "type" : "INPUT",
        "elements" : {
          "gt0003" : {
            "id" : "gt0003",
            "path" : "/data[at0001]/events[at0002]/data[at0003]/items[at0006]",
            "data_type" : "DV_CODED_TEXT",
            "allowed_values" : [
              "local::at0007",
              "local::at0008",
              "local::at0009"
            ]
          },
          "gt0004" : {
            "id" : "gt0004",
            "path" : "/data[at0001]/events[at0002]/data[at0003]/items[at0010]",
            "data_type" : "DV_CODED_TEXT",
            "allowed_values" : [
              "local::at0011",
              "local::at0012",
              "local::at0013"
            ]
          }
        }
      },
      "gt0005" : {
        "id" : "gt0005",
        "model_id" : "openEHR-EHR-EVALUATION.prostate_biopsy_recommendation_assessment.v0",
        "template_id" : "openEHR-EHR-EVALUATION.prostate_biopsy_recommendation_assessment.v0",
        "type" : "OUTPUT",
        "elements" : {
          "gt0006" : {
            "id" : "gt0006",
            "path" : "/data[at0003]/items[at0004]",
            "data_type" : "DV_CODED_TEXT"
          }
        }
      }
    },
    "pre_conditions" : [
      "$gt0003|PI-RADS Score|!=null",
      "$gt0004|PSA density|!=null"
    ],
    "rules" : {
      "gt0007" : {
        "id" : "gt0007",
        "priority" : 4,
        "when" : [
          "($gt0003|PI-RADS Score|==local::at0007|PI-RADS 1–2|&&($gt0004|PSA density|==local::at0011|< 0,10|||$gt0004|PSA density|==local::at0012|0,10–0,199|))||($gt0003|PI-RADS Score|==local::at0008|PI-RADS 3|&&$gt0004|PSA density|==local::at0011|< 0,10|)"
        ],
        "then" : [
          "$gt0006|Recommendation for biopsy|=local::at0005|Refrain from biopsy|"
        ]
      },
      "gt0008" : {
        "id" : "gt0008",
        "priority" : 3,
        "when" : [
          "($gt0003|PI-RADS Score|==local::at0009|PI-RADS 4–5|&&($gt0004|PSA density|==local::at0011|< 0,10|||$gt0004|PSA density|==local::at0012|0,10–0,199|||$gt0004|PSA density|==local::at0013|≥ 0,20|))||($gt0003|PI-RADS Score|==local::at0008|PI-RADS 3|&&$gt0004|PSA density|==local::at0012|0,10–0,199|)"
        ],
        "then" : [
          "$gt0006|Recommendation for biopsy|=local::at0006|Perform targeted biopsy|"
        ]
      },
      "gt0009" : {
        "id" : "gt0009",
        "priority" : 2,
        "when" : [
          "$gt0003|PI-RADS Score|==local::at0007|PI-RADS 1–2|&&$gt0004|PSA density|==local::at0013|≥ 0,20|"
        ],
        "then" : [
          "$gt0006|Recommendation for biopsy|=local::at0007|Perform systematic biopsies|"
        ]
      },
      "gt0010" : {
        "id" : "gt0010",
        "priority" : 1,
        "when" : [
          "$gt0003|PI-RADS Score|==local::at0008|PI-RADS 3|&&$gt0004|PSA density|==local::at0013|≥ 0,20|"
        ],
        "then" : [
          "$gt0006|Recommendation for biopsy|=local::at0008|Perform targeted and systematic biopsies|"
        ]
      }
    }
  },
