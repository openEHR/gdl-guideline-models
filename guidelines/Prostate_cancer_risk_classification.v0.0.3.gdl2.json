    "rules": {
      "gt0011": {
        "id": "gt0011",
        "priority": 6,
        "when": [
          "$gt0003|T-stage|==local::at0003|T1c|",
          "$gt0004|Gleason score|==local::at0009|6 (ISUP 1)|",
          "$gt0005|PSA|==local::at0016|< 10 µg/l|",
          "$gt0006|PSA density < 0.15 ug/l/cm3|==local::at0021|Yes|",
          "$gt0007|Histological findings ≤ 8 mm cancer in total in 1–4 of 8–12 systematic biopsy cores|==local::at0021|Yes|"
        ],
        "then": [
          "$gt0010|Risk group|=local::at0031|Very low risk|"
        ]
      },
      "gt0012": {
        "id": "gt0012",
        "priority": 5,
        "when": [
          "$gt0003|T-stage|==local::at0004|T1-T2a|",
          "$gt0004|Gleason score|==local::at0009|6 (ISUP 1)|",
          "$gt0005|PSA|==local::at0016|< 10 µg/l|",
          "$gt0006|PSA density < 0.15 ug/l/cm3|==local::at0022|No|",
          "$gt0007|Histological findings ≤ 8 mm cancer in total in 1–4 of 8–12 systematic biopsy cores|==local::at0022|No|"
        ],
        "then": [
          "$gt0010|Risk group|=local::at0032|Low risk|"
        ]
      },
      "gt0013": {
        "id": "gt0013",
        "priority": 4,
        "when": [
          "$gt0003|T-stage|==local::at0005|T2b/T2c|||$gt0004|Gleason score|==local::at0010|7 (3+4) (ISUP 2)|||$gt0005|PSA|==local::at0017|10–19 µg/l|"
        ],
        "then": [
          "$gt0010|Risk group|=local::at0033|Favorable intermediate risk|"
        ]
      },
      "gt0014": {
        "id": "gt0014",
        "priority": 3,
        "when": [
          "($gt0003|T-stage|==local::at0005|T2b/T2c|&&$gt0004|Gleason score|==local::at0010|7 (3+4) (ISUP 2)|)||($gt0003|T-stage|==local::at0005|T2b/T2c|&&$gt0005|PSA|==local::at0017|10–19 µg/l|)||($gt0004|Gleason score|==local::at0010|7 (3+4) (ISUP 2)|&&$gt0005|PSA|==local::at0017|10–19 µg/l|)||local::at0012|7 (4+3) (ISUP 3)|||local::at0011|7 (3+4) (ISUP 2)  in more than half of systematic biopsy specimens|"
        ],
        "then": [
          "$gt0010|Risk group|=local::at0034|Unfavorable intermediate risk|"
        ]
      },
      "gt0015": {
        "id": "gt0015",
        "priority": 2,
        "when": [
          "$gt0003|T-stage|==local::at0006|T3a|||$gt0008|Strong suspicion of extraprostatic growth on MRI (EPE/SVI 5)|==local::at0021|Yes|||$gt0004|Gleason score|==local::at0013|8 (ISUP 4)|||$gt0005|PSA|==local::at0018|20–39 µg/l|"
        ],
        "then": [
          "$gt0010|Risk group|=local::at0035|High risk|"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 1,
        "when": [
          "$gt0003|T-stage|==local::at0006|T3a|&&$gt0008|Strong suspicion of extraprostatic growth on MRI (EPE/SVI 5)|==local::at0021|Yes|||($gt0003|T-stage|==local::at0006|T3a|&&$gt0004|Gleason score|==local::at0013|8 (ISUP 4)|)||$gt0003|T-stage|==local::at0006|T3a|&&$gt0005|PSA|==local::at0018|20–39 µg/l|||$gt0008|Strong suspicion of extraprostatic growth on MRI (EPE/SVI 5)|==local::at0021|Yes|&&$gt0004|Gleason score|==local::at0013|8 (ISUP 4)|||$gt0008|Strong suspicion of extraprostatic growth on MRI (EPE/SVI 5)|==local::at0021|Yes|&&$gt0005|PSA|==local::at0018|20–39 µg/l|||$gt0004|Gleason score|==local::at0013|8 (ISUP 4)|&&$gt0005|PSA|==local::at0018|20–39 µg/l|",
          "$gt0003|T-stage|==local::at0007|T3b/T4|||$gt0004|Gleason score|==local::at0010|7 (3+4) (ISUP 2)|||$gt0005|PSA|==local::at0019|≥ 40 µg/l|"
        ],
        "then": [
          "$gt0010|Risk group|=local::at0036|Very high risk|"
        ]
      }
    }
  },
