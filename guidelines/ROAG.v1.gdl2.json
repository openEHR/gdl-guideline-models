      "gt0077": {
        "id": "gt0077",
        "priority": 3,
        "when": [
          "$gt0072|Swallowing|==1|local::at0042|Normal swallow|"
        ],
        "then": [
          "$gt0074|Swallowing Recommendation|=0|local::at0054|No action|",
          "$gt0073|I. Swallowing|=1|local::at0042|Normal swallow|"
