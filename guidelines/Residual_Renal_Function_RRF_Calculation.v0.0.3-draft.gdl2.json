    "pre_conditions" : [
      "$gt0003|Interdialytic urine volume|!=null",
      "$gt0005|Urine urea concentration|!=null",
      "$gt0007|BUN 1 (after 1st dialysis of week)|!=null",
      "$gt0009|BUN 2 (prior to 2nd dialysis of week)|!=null",
      "$gt0017|Interdialytic period (minutes)|!=null"
    ],
    "rules" : {
      "gt0012" : {
        "id" : "gt0012",
        "priority" : 2,
        "when" : [
          "$gt0007|BUN 1 (after 1st dialysis of week)|!=null",
          "$gt0009|BUN 2 (prior to 2nd dialysis of week)|!=null"
        ],
        "then" : [
          "$gt0011|Mean BUN|=($gt0007|BUN 1 (after 1st dialysis of week)|.magnitude+$gt0009|BUN 2 (prior to 2nd dialysis of week)|.magnitude)/2"
        ]
      },
      "gt0013" : {
        "id" : "gt0013",
        "priority" : 1,
        "when" : [
          "$gt0011|Mean BUN|!=null"
        ],
        "then" : [
          "$gt0015|Residual Renal Function (mL/min)|=($gt0003|Interdialytic urine volume|.magnitude*$gt0005|Urine urea concentration|.magnitude)/$gt0025|Interdialytic period (minutes)|"
        ]
      }
    }
