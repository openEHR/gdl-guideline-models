        "priority": 4,
        "when": [
          "$gt0004|Torso|.code=='at0016'"
        ],
        "then": [
          "$gt0013|Torso burn area|=0,%"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 3,
        "when": [
          "$gt0004|Torso|.code=='at0017'"
        ],
        "then": [
          "$gt0013|Torso burn area|=9,%"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 2,
        "when": [
          "$gt0004|Torso|.code=='at0018'"
        ],
        "then": [
          "$gt0013|Torso burn area|=18,%"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 1,
        "when": [
          "$gt0012|Head and face burn area|!=null",
          "$gt0013|Torso burn area|!=null"
        ],
        "then": [
          "$gt0020|Total body surface area affect by burns|.magnitude=$gt0012|Head and face burn area|.magnitude+$gt0013|Torso burn area|.magnitude",
          "$gt0020|Total body surface area affect by burns|.unit='%'",
          "$gt0020|Total body surface area affect by burns|.precision=1"
        ]
      }
