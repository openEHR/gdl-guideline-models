        ],
        "then": [
          "$gt0040|Bilirubin Score|=9|local::at0081|≥6.0 mg/dL (≥102.6 µmol/L)|"
        ]
      },
      "gt0078": {
        "id": "gt0078",
        "priority": 6,
        "when": [
