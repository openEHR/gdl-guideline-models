    "rules": {
      "gt0130": {
        "id": "gt0130",
        "priority": 7,
        "when": [
          "$gt0052|Tension in jaws|!=null",
          "$gt0053|Muscle pain|!=null",
          "$gt0054|Muscle stiffness|!=null",
          "$gt0055|Muscle exhaustion|!=null",
          "$gt0056|General weariness|!=null",
          "$gt0057|Irritability|!=null",
          "$gt0058|Tingling sensations in the body|!=null",
          "$gt0059|Numbness in arms/hands, legs/feet|!=null",
          "$gt0060|Burning sensations in the body|!=null",
          "$gt0061|Disturbed sleep|!=null",
          "$gt0062|Irritation in the eyes|!=null",
          "$gt0063|Dry mouth|!=null",
          "$gt0064|Hypersensitivity to fragrances, lights, sounds|!=null",
          "$gt0065|Weather susceptibility|!=null",
          "$gt0066|Swelling sensations in hands/feet|!=null",
          "$gt0067|Fumbling hands/fingers|!=null",
          "$gt0068|Trembling hands|!=null",
          "$gt0069|Dizziness|!=null",
          "$gt0070|Varying loose/hard stools|!=null",
          "$gt0071|Restlessness|!=null",
          "$gt0072|Itching|!=null",
          "$gt0073|Cold hands/feet|!=null",
          "$gt0074|Cold/sweaty alternating sensations|!=null",
          "$gt0075|Frequent micturition|!=null",
          "$gt0076|Reduced concentration|!=null",
          "$gt0077|Reduced memory|!=null",
          "$gt0078|Pain in the skin when touched|!=null",
          "$gt0079|Boiling sensations in the body|!=null",
          "$gt0080|Reduced appetite|!=null",
          "$gt0081|Fever sensation without fever|!=null",
          "$gt0082|Palpitations|!=null",
          "$gt0083|Weight over the chest/heavy breathing|!=null",
          "$gt0084|Frequent headache|!=null",
          "$gt0085|Reduced libido|!=null",
          "$gt0086|Globus/sensation of lump in the throat|!=null"
        ],
        "then": [
          "$gt0039|Total score|.magnitude=$gt0052.value+$gt0053.value+$gt0054.value+$gt0055.value+$gt0056.value+$gt0057.value+$gt0058.value+$gt0059.value+$gt0060.value+$gt0061.value+$gt0062.value+$gt0063.value+$gt0064.value+$gt0065.value+$gt0066.value+$gt0067.value+$gt0068.value+$gt0069.value+$gt0070.value+$gt0071.value+$gt0072.value+$gt0073.value+$gt0074.value+$gt0075.value+$gt0076.value+$gt0077.value+$gt0078.value+$gt0079.value+$gt0080.value+$gt0081.value+$gt0082.value+$gt0083.value+$gt0084.value+$gt0085.value+$gt0086.value"
        ]
      },
      "gt0134": {
        "id": "gt0134",
        "priority": 6,
        "when": [
          "$gt0039|Total score|.magnitude<25"
        ],
        "then": [
          "$gt0132|Autonomic dysfunction assessment|=0|local::at0003|No dysfunction|"
        ]
      },
      "gt0135": {
        "id": "gt0135",
        "priority": 5,
        "when": [
          "$gt0039|Total score|.magnitude>=25"
        ],
        "then": [
          "$gt0132|Autonomic dysfunction assessment|=1|local::at0004|Probable autonomic dysfunction|"
        ]
      },
      "gt0136": {
        "id": "gt0136",
        "priority": 4,
        "when": [
          "$gt0039|Total score|.magnitude>=25",
          "$gt0039|Total score|.magnitude<=50"
        ],
        "then": [
          "$gt0133|SCI-93 assessment|=0|local::at0006|Mild effect on ADL|"
        ]
      },
      "gt0137": {
        "id": "gt0137",
        "priority": 3,
        "when": [
          "$gt0039|Total score|.magnitude>=51",
          "$gt0039|Total score|.magnitude<=75"
        ],
        "then": [
          "$gt0133|SCI-93 assessment|=1|local::at0007|Considerable effect on ADL - can remain working or return to part-time conditions|"
        ]
      },
      "gt0138": {
        "id": "gt0138",
        "priority": 2,
        "when": [
          "$gt0039|Total score|.magnitude>=76",
          "$gt0039|Total score|.magnitude<=100"
        ],
        "then": [
          "$gt0133|SCI-93 assessment|=2|local::at0008|Major effect on ADL including difficulties maintaining a job|"
        ]
      },
      "gt0139": {
        "id": "gt0139",
        "priority": 1,
        "when": [
          "$gt0039|Total score|.magnitude>105"
        ],
        "then": [
          "$gt0133|SCI-93 assessment|=3|local::at0009|Severe effect - treatment required ahead of rehabilitation process|"
        ]
      }
    }
  },
