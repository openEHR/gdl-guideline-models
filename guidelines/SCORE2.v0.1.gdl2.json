|Male|"], "then": ["$gt0089|TC.age value|=0-0.0281"]}, "gt0064": {"id": "gt0064", "priority": 20, "when": ["$gt0025|Gender|==local::at0006|Female|"], "then": ["$gt0089|TC.age value|=0-0.0226"]}, "gt0069": {"id": "gt0069", "priority": 19, "when": ["$gt0025|Gender|==local::at0005|Male|"], "then": ["$gt0090|HDL.age value|=0.0426"]}, "gt0070": {"id": "gt0070", "priority": 18, "when": ["$gt0025|Gender|==local::at0006|Female|"], "then": ["$gt0090|HDL.age value|=0.0613"]}, "gt0080": {"id": "gt0080", "priority": 17, "when": ["$gt0157|Cigarrete smoking|==local::at0006|Yes|"], "then": ["$gt0079|smoking|=1"]}, "gt0081": {"id": "gt0081", "priority": 16, "when": ["$gt0157|Cigarrete smoking|==local::at0007|No|"], "then": ["$gt0079|smoking|=0"]}, "gt0066": {"id": "gt0066", "priority": 15, "when": ["$gt0025|Gender|==local::at0005|Male|"], "then": ["$gt0065|baseline_survival|=0.9605"]}, "gt0067": {"id": "gt0067", "priority": 14, "when": ["$gt0025|Gender|==local::at0006|Female|"], "then": ["$gt0065|baseline_survival|=0.9776"]}, "gt0100": {"id": "gt0100", "priority": 13, "when": ["$gt0025|Gender|==local::at0005|Male|"], "then": ["$gt0101|scale_one|=0-0.1565"]}, "gt0103": {"id": "gt0103", "priority": 12, "when": ["$gt0025|Gender|==local::at0005|Male|"], "then": ["$gt0102|scale_two|=0.8009"]}, "gt0104": {"id": "gt0104", "priority": 11, "when": ["$gt0025|Gender|==local::at0006|Female|"], "then": ["$gt0101|scale_one|=0-0.3143"]}, "gt0105": {"id": "gt0105", "priority": 10, "when": ["$gt0025|Gender|==local::at0006|Female|"], "then": ["$gt0102|scale_two|=0.7701"]}, "gt0082": {"id": "gt0082", "priority": 9, "then": ["$gt0083|sum_risk|.magnitude=($gt0091|age.transformed|.magnitude*$gt0072|Age value|)+($gt0079|smoking|*$gt0084|smoking value|)+($gt0092|SBP.transformed|.magnitude*$gt0073|SBP value|)+($gt0075|Total.cholesterol transfor|.magnitude*$gt0085|Total cholesterol value|)+($gt0077|HDL.cholesterol tranfor|.magnitude*$gt0086|HDL value|)+($gt0084|smoking value|*$gt0091|age.transformed|.magnitude*$gt0087|Smoking.age value|)+($gt0092|SBP.transformed|.magnitude*$gt0091|age.transformed|.magnitude*$gt0088|SBP.age value|)+($gt0075|Total.cholesterol transfor|.magnitude*$gt0091|age.transformed|.magnitude*$gt0089|TC.age value|)+($gt0077|HDL.cholesterol tranfor|.magnitude*$gt0091|age.transformed|.magnitude*$gt0090|HDL.age value|)", "$gt0083|sum_risk|.precision=4"]}, "gt0093": {"id": "gt0093", "priority": 8, "then": ["$gt0108|10_year_uncalibrated|.magnitude=1-($gt0065|baseline_survival|^exp($gt0083|sum_risk|.magnitude))", "$gt0108|10_year_uncalibrated|.precision=4"]}, "gt0106": {"id": "gt0106", "priority": 7, "then": ["$gt0159|10 year risk score|.magnitude=(1-exp(0-exp($gt0101|scale_one|+$gt0102|scale_two|*log(0-log(1-$gt0108|10_year_uncalibrated|.magnitude)))))*100", "$gt0159|10 year risk score|.precision=1", "$gt0159|10 year risk score|.unit='%'"]}, "gt0133": {"id": "gt0133", "priority": 6, "when": ["$gt0159|10 year risk score|.magnitude<2.5", "$gt0018|Age|<50,a", "$gt0159|10 year risk score|!=null"], "then": ["$gt0160|Interpretation|=1|local::at0010|Low to moderate risk|"]}, "gt0136": {"id": "gt0136", "priority": 5, "when": ["$gt0159|10 year risk score|.magnitude>=2.5", "$gt0159|10 year risk score|.magnitude<7.5", "$gt0018|Age|<50,a", "$gt0159|10 year risk score|!=null"], "then": ["$gt0160|Interpretation|=2|local::at0011|High risk|"]}, "gt0137": {"id": "gt0137", "priority": 4, "when": ["$gt0159|10 year risk score|.magnitude>=7.5", "$gt0018|Age|<50,a", "$gt0159|10 year risk score|!=null"], "then": ["$gt0160|Interpretation|=3|local::at0012|Very high risk|"]}, "gt0138": {"id": "gt0138", "priority": 3, "when": ["$gt0159|10 year risk score|.magnitude<5", "$gt0018|Age|>=50,a", "$gt0018|Age|<=69,a", "$gt0159|10 year risk score|!=null"], "then": ["$gt0160|Interpretation|=1|local::at0010|Low to moderate risk|"]}, "gt0140": {"id": "gt0140", "priority": 2, "when": ["$gt0159|10 year risk score|.magnitude>=5", "$gt0159|10 year risk score|.magnitude<10", "$gt0018|Age|>=50,a", "$gt0018|Age|<=69,a", "$gt0159|10 year risk score|!=null"], "then": ["$gt0160|Interpretation|=2|local::at0011|High risk|"]}, "gt0141": {"id": "gt0141", "priority": 1, "when": ["$gt0159|10 year risk score|.magnitude>=10", "$gt0018|Age|>=50,a", "$gt0018|Age|<=69,a", "$gt0159|10 year risk score|!=null"], "then": ["$gt0160|Interpretation|=3|local::at0012|Very high risk|"]}}}, "ontology": {"term_definitions": {"en": {"id": "en", "terms": {"gt0001": {"id": "gt0001", "text": "SCORE2", "description": "SCORE2 (Systematic Coronary Risk Evaluation 2) is an advanced cardiovascular risk prediction algorithm developed to estimate the 10-year probability of both fatal and non-fatal cardiovascular disease (CVD) events. It is specifically designed for use in individuals aged 40 to 69 years who do not have pre-existing cardiovascular conditions or diabetes. The algorithm evaluates risk based on a combination of modifiable and non-modifiable factors, including age, sex, smoking status, systolic blood pressure, total cholesterol and HDL cholesterol. By integrating these variables, SCORE2 provides a personalized risk assessment that can guide preventive strategies, such as lifestyle modifications or pharmacological interventions.One of the key strengths of SCORE2 is its ability to support clinical decision-making by stratifying patients into different risk categories, helping healthcare professionals identify individuals who may benefit from early intervention.\n\nInterpretation\n\nLow-to-Moderate Cardiovascular Risk:\n- Risk &lt;2.5% for individuals aged &lt;50 years\n- Risk &lt;5% for individuals aged 50–69 years\n\nRecommendation:\nRisk factor treatment is generally not recommended.\n\nHigh Cardiovascular Risk:\n- Risk between 2.5% and &lt;7.5% for ages &lt;50 years\n- Risk between 5% and &lt;10% for ages 50–69 years\n\nRecommendation:\nRisk factor treatment should be considered.\n\nVery High Cardiovascular Risk:\n- Risk ≥7.5% for ages &lt;50 years\n- Risk ≥10% for ages 50–69 years\n\nRecommendation:\nRisk factor treatment is generally recommended."}, "gt0002": {"id": "gt0002", "text": "openEHR-EHR-OBSERVATION.basic_demographic_lm_rev.v1"}, "gt0006": {"id": "gt0006", "text": "openEHR-EHR-OBSERVATION.blood_pressure.v2"}, "gt0007": {"id": "gt0007", "text": "Systolic"}, "gt0018": {"id": "gt0018", "text": "Age"}, "gt0025": {"id": "gt0025", "text": "Gender"}, "gt0042": {"id": "gt0042", "text": "Set Male to 0.3742", "description": ""}, "gt0043": {"id": "gt0043", "text": "Set Female to 0.4648", "description": ""}, "gt0044": {"id": "gt0044", "text": "Set Male smoking to 0.6012", "description": ""}, "gt0048": {"id": "gt0048", "text": "Set Female smoking to 0.7744", "description": ""}, "gt0055": {"id": "gt0055", "text": "Gender value"}, "gt0047": {"id": "gt0047", "text": "Set Male SBP to 0.2777", "description": ""}, "gt0046": {"id": "gt0046", "text": "Set Female SBP to 0.3131", "description": ""}, "gt0045": {"id": "gt0045", "text": "Set Male Total Cholesterol to 0.1458", "description": ""}, "gt0056": {"id": "gt0056", "text": "Set Female Total Cholesterol to 0.1002", "description": ""}, "gt0057": {"id": "gt0057", "text": "Set Male HDL to -0.2698", "description": ""}, "gt0058": {"id": "gt0058", "text": "Set Female HDL to -0.2606", "description": ""}, "gt0059": {"id": "gt0059", "text": "Set Male Smoking age to -0.0755", "description": ""}, "gt0060": {"id": "gt0060", "text": "Set Female smoking age to -0.1088", "description": ""}, "gt0061": {"id": "gt0061", "text": "Set Male SBP age to -0.0255", "description": ""}, "gt0062": {"id": "gt0062", "text": "Set Female SBP age to \t-0.0277", "description": ""}, "gt0063": {"id": "gt0063", "text": "Set Male TC age to -0.0281", "description": ""}, "gt0065": {"id": "gt0065", "text": "baseline_survival"}, "gt0066": {"id": "gt0066", "text": "Baseline survival Male to 0.9605", "description": ""}, "gt0067": {"id": "gt0067", "text": "Baseline survival Female to 0.9776", "description": ""}, "gt0064": {"id": "gt0064", "text"