    "default_actions": [
      "$gt0023|Pathological T category|=0|local::at0012|pT1|",
      "$gt0024|Regional lymph node status|=0|local::at0016|pNx or pN0|",
      "$gt0025|Metastasis category|=0|local::at0018|M0|",
      "$gt0026|Tumor size|=0|local::at0020|<5 cm|",
      "$gt0027|Tumor (nuclear) grade|=0|local::at0022|Stage 1 or 2|",
      "$gt0028|Tumor necrosis present|=0|local::at0025|No|"
    ],
    "rules": {
      "gt0030": {
        "id": "gt0030",
        "priority": 19,
        "when": [
          "$gt0020|Tumor size|!=null"
        ],
        "then": [
          "$gt0026|Tumor size|=$gt0020|Tumor size|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 18,
        "when": [
          "$gt0021|Tumor necrosis present|!=null"
        ],
        "then": [
          "$gt0028|Tumor necrosis present|=$gt0021|Tumor necrosis present|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 17,
        "when": [
          "($gt0033|Tumor sizing|==2|local::at0011|TIa - Organ confined, ≤4 cm|)||($gt0033|Tumor sizing|==3|local::at0012|TIb - Organ confined, 4-7 cm|)"
        ],
        "then": [
          "$gt0023|Pathological T category|=0|local::at0012|pT1|"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 16,
        "when": [
          "($gt0033|Tumor sizing|==4|local::at0013|T2a - Organ confined, 7-10 cm|)||($gt0033|Tumor sizing|==5|local::at0014|T2b - Organ confined, >10 cm|)"
        ],
        "then": [
          "$gt0023|Pathological T category|=1|local::at0013|pT2|"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 15,
        "when": [
          "(($gt0033|Tumor sizing|==6|local::at0015|T3a - Perinephric tissue, renal sinus, or renal vein|)||($gt0033|Tumor sizing|==7|local::at0016|T3b - Vena cava below the diaphragm|))||($gt0033|Tumor sizing|==8|local::at0017|T3c - Vena cava above diaphragm or into wall of vena cava at any level|)"
        ],
        "then": [
          "$gt0023|Pathological T category|=2|local::at0014|pT3a-c|"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 14,
        "when": [
          "$gt0033|Tumor sizing|==9|local::at0018|T4 - Beyond Gerota’s fascia or directly into adrenal gland|"
        ],
        "then": [
          "$gt0023|Pathological T category|=4|local::at0015|pT4|"
        ]
      },
      "gt0040": {
        "id": "gt0040",
        "priority": 13,
        "when": [
          "($gt0034|Node involvement|==0|local::at0020|NX - Regional nodes cannot be assessed|)||($gt0034|Node involvement|==1|local::at0021|N0 - No regional lymph node metastasis|)"
        ],
        "then": [
          "$gt0024|Regional lymph node status|=0|local::at0016|pNx or pN0|"
        ]
      },
      "gt0041": {
        "id": "gt0041",
        "priority": 12,
        "when": [
          "($gt0034|Node involvement|==2|local::at0022|N1 - Metastasis in a single regional lymph node|)||($gt0034|Node involvement|==3|local::at0023|N2 - Metastasis in more than one regional lymph node|)"
        ],
        "then": [
          "$gt0024|Regional lymph node status|=2|local::at0017|pN1 or pN2|"
        ]
      },
      "gt0042": {
        "id": "gt0042",
        "priority": 11,
        "when": [
          "$gt0035|Metastases|==1|local::at0025|M0 - No distant metastasis|"
        ],
        "then": [
          "$gt0025|Metastasis category|=0|local::at0018|M0|"
        ]
      },
      "gt0043": {
        "id": "gt0043",
        "priority": 10,
        "when": [
          "$gt0035|Metastases|==2|local::at0026|M1 - Distant metastasis|"
        ],
        "then": [
          "$gt0025|Metastasis category|=4|local::at0019|M1|"
        ]
      },
      "gt0044": {
        "id": "gt0044",
        "priority": 9,
        "when": [
          "($gt0049|Grade|==1|local::at0022|Grade I|)||($gt0049|Grade|==2|local::at0023|Grade II|)"
        ],
        "then": [
          "$gt0027|Tumor (nuclear) grade|=0|local::at0022|Stage 1 or 2|"
        ]
      },
      "gt0045": {
        "id": "gt0045",
        "priority": 8,
        "when": [
          "$gt0049|Grade|==3|local::at0024|Grade III|"
        ],
        "then": [
          "$gt0027|Tumor (nuclear) grade|=1|local::at0023|Stage 3|"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 7,
        "when": [
          "$gt0049|Grade|==4|local::at0025|Grade IV|"
        ],
        "then": [
          "$gt0027|Tumor (nuclear) grade|=3|local::at0024|Stage 4|"
        ]
      },
      "gt0047": {
        "id": "gt0047",
        "priority": 6,
        "then": [
          "$gt0029|Total score|.magnitude=(((($gt0023.value+$gt0024.value)+$gt0025.value)+$gt0026.value)+$gt0027.value)+$gt0028.value"
        ]
      },
      "gt0056": {
        "id": "gt0056",
        "priority": 5,
        "when": [
          "$gt0029|Total score|<=2"
        ],
        "then": [
          "$gt0051|1 Year Survival|=0|local::at0008|99.7%|",
          "$gt0052|3 Year Survival|=0|local::at0013|98.7%|",
          "$gt0053|5 Year Survival|=0|local::at0018|97.3%|",
          "$gt0054|7 Year Survival|=0|local::at0023|96%|",
          "$gt0055|10 Year Survival|=0|local::at0028|93.6%|"
        ]
      },
      "gt0057": {
        "id": "gt0057",
        "priority": 4,
        "when": [
          "$gt0029|Total score|>=3",
          "$gt0029|Total score|<=4"
        ],
        "then": [
          "$gt0051|1 Year Survival|=1|local::at0009|98.1%|",
          "$gt0052|3 Year Survival|=1|local::at0014|93.4%|",
          "$gt0053|5 Year Survival|=1|local::at0019|89.8%|",
          "$gt0054|7 Year Survival|=1|local::at0024|84%|",
          "$gt0055|10 Year Survival|=1|local::at0029|77.9%|"
        ]
      },
      "gt0058": {
        "id": "gt0058",
        "priority": 3,
        "when": [
          "$gt0029|Total score|<=6",
          "$gt0029|Total score|>=5"
        ],
        "then": [
          "$gt0051|1 Year Survival|=2|local::at0010|92.9%|",
          "$gt0052|3 Year Survival|=2|local::at0015|83.8%|",
          "$gt0053|5 Year Survival|=2|local::at0020|74.1%|",
          "$gt0054|7 Year Survival|=2|local::at0025|65%|",
          "$gt0055|10 Year Survival|=2|local::at0030|57.3%|"
        ]
      },
      "gt0059": {
        "id": "gt0059",
        "priority": 2,
        "when": [
          "$gt0029|Total score|<=9",
          "$gt0029|Total score|>=7"
        ],
        "then": [
          "$gt0051|1 Year Survival|=3|local::at0011|76.5%|",
          "$gt0052|3 Year Survival|=3|local::at0016|46.9%|",
          "$gt0053|5 Year Survival|=3|local::at0021|38.6%|",
          "$gt0054|7 Year Survival|=3|local::at0026|29%|",
          "$gt0055|10 Year Survival|=3|local::at0031|25.9%|"
        ]
      },
      "gt0060": {
        "id": "gt0060",
        "priority": 1,
        "when": [
          "$gt0029|Total score|>=10"
        ],
        "then": [
          "$gt0051|1 Year Survival|=4|local::at0012|43.3%|",
          "$gt0052|3 Year Survival|=4|local::at0017|21.9%|",
          "$gt0053|5 Year Survival|=4|local::at0022|19.2%|",
          "$gt0054|7 Year Survival|=4|local::at0027|19.2%|",
          "$gt0055|10 Year Survival|=4|local::at0032|19.2%|"
        ]
      }
    }
  },
