    "pre_conditions" : [
      "$gt0003|Primary Tumor|!=null",
      "$gt0004|Lymph Nodes|!=null",
      "$gt0005|Distant Metastasis|!=null"
    ],
    "rules" : {
      "gt0010" : {
        "id" : "gt0010",
        "priority" : 22,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0009|No evidence of primary tumor|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0007|T0|"
        ]
      },
      "gt0011" : {
        "id" : "gt0011",
        "priority" : 21,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0010|Primary tumor cannot be assessed|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0008|Tx|"
        ]
      },
      "gt0012" : {
        "id" : "gt0012",
        "priority" : 20,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0011|Clinically inapparent tumor that is not palpable|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0009|T1|"
        ]
      },
      "gt0013" : {
        "id" : "gt0013",
        "priority" : 19,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0012|Tumor incidental histologic finding in 5% or less of tissue resected|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0010|T1a|"
        ]
      },
      "gt0022" : {
        "id" : "gt0022",
        "priority" : 18,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0013|Tumor incidental histologic finding in more than 5% of tissue resected|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0011|T1b|"
        ]
      },
      "gt0023" : {
        "id" : "gt0023",
        "priority" : 17,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0014|Tumor identified by needle biopsy found in one or both sides, but not palpable|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0012|T1c|"
        ]
      },
      "gt0024" : {
        "id" : "gt0024",
        "priority" : 16,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0015|Tumor is palpable and confined within prostate|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0013|T2|"
        ]
      },
      "gt0025" : {
        "id" : "gt0025",
        "priority" : 15,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0016|Tumor involves one-half of one side or less|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0014|T2a|"
        ]
      },
      "gt0026" : {
        "id" : "gt0026",
        "priority" : 14,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0017|Tumor involves more than one-half of one side but not both sides|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0015|T2b|"
        ]
      },
      "gt0027" : {
        "id" : "gt0027",
        "priority" : 13,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0018|Tumor involves both sides|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0016|T2c|"
        ]
      },
      "gt0028" : {
        "id" : "gt0028",
        "priority" : 12,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0019|Extraprostatic tumor that is not fixed or does not invade adjacent structures|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0017|T3|"
        ]
      },
      "gt0029" : {
        "id" : "gt0029",
        "priority" : 11,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0020|Extraprostatic extension (unilateral or bilateral)|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0018|T3a|"
        ]
      },
      "gt0030" : {
        "id" : "gt0030",
        "priority" : 10,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0021|Tumor invades seminal vesicle(s)|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0019|T3b|"
        ]
      },
      "gt0031" : {
        "id" : "gt0031",
        "priority" : 9,
        "when" : [
          "$gt0003|Primary Tumor|==local::at0022|Tumor is fixed or invades adjacent structures other than seminal vesicles, such as external sphincter, rectum, bladder, levator muscles, and/or pelvic wall|"
        ],
        "then" : [
          "$gt0007|Clinical T stage|=local::at0020|T4|"
        ]
      },
      "gt0014" : {
        "id" : "gt0014",
        "priority" : 8,
        "when" : [
          "$gt0004|Lymph Nodes|==local::at0023|No positive regional lymph nodes|"
        ],
        "then" : [
          "$gt0008|Clinical N stage|=local::at0021|N0|"
        ]
      },
      "gt0016" : {
        "id" : "gt0016",
        "priority" : 7,
        "when" : [
          "$gt0004|Lymph Nodes|==local::at0024|Regional lymph nodes were not assessed|"
        ],
        "then" : [
          "$gt0008|Clinical N stage|=local::at0022|Nx|"
        ]
      },
      "gt0017" : {
        "id" : "gt0017",
        "priority" : 6,
        "when" : [
          "$gt0004|Lymph Nodes|==local::at0025|Metastases in regional lymph node(s)|"
        ],
        "then" : [
          "$gt0008|Clinical N stage|=local::at0023|N1|"
        ]
      },
      "gt0015" : {
        "id" : "gt0015",
        "priority" : 5,
        "when" : [
          "$gt0005|Distant Metastasis|==local::at0026|No distant metastasis|"
        ],
        "then" : [
          "$gt0009|Clinical M stage|=local::at0024|M0|"
        ]
      },
      "gt0018" : {
        "id" : "gt0018",
        "priority" : 4,
        "when" : [
          "$gt0005|Distant Metastasis|==local::at0027|Distant metastasis|"
        ],
        "then" : [
          "$gt0009|Clinical M stage|=local::at0025|M1|"
        ]
      },
      "gt0019" : {
        "id" : "gt0019",
        "priority" : 3,
        "when" : [
          "$gt0005|Distant Metastasis|==local::at0028|Nonregional lymph node(s)|"
        ],
        "then" : [
          "$gt0009|Clinical M stage|=local::at0026|M1a|"
        ]
      },
      "gt0020" : {
        "id" : "gt0020",
        "priority" : 2,
        "when" : [
          "$gt0005|Distant Metastasis|==local::at0029|Bone(s)|"
        ],
        "then" : [
          "$gt0009|Clinical M stage|=local::at0027|M1b|"
        ]
      },
      "gt0021" : {
        "id" : "gt0021",
        "priority" : 1,
        "when" : [
          "$gt0005|Distant Metastasis|==local::at0030|Other site(s) with or without bone disease|"
        ],
        "then" : [
          "$gt0009|Clinical M stage|=local::at0028|M1c|"
        ]
      }
    }
  },
