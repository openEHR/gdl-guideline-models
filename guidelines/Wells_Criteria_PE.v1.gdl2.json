    "rules": {
      "gt0023": {
        "id": "gt0023",
        "priority": 7,
        "when": [
          "$gt0003|Heart Rate|!=null",
          "$gt0003|Heart Rate|>100,/min"
        ],
        "then": [
          "$gt0016|Heart rate > 100 BPM?|=3|local::at0018|Yes|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 6,
        "when": [
          "$gt0003|Heart Rate|!=null",
          "$gt0003|Heart Rate|<=100,/min"
        ],
        "then": [
          "$gt0016|Heart rate > 100 BPM?|=0|local::at0017|No|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 5,
        "when": [
          "$gt0005|Clinical signs and symptoms of DVT?|!=null",
          "$gt0006|Is PE the number one diagnosis OR equally likely?|!=null",
          "$gt0008|Immobilization at least 3 days OR surgery in the Previous 4 weeks?|!=null",
          "$gt0009|Previous, objectively diagnosed PE or DVT?|!=null",
          "$gt0010|Hemoptysis?|!=null",
          "$gt0011|Malignancy with treatment within 6 months or palliative?|!=null",
          "$gt0016|Heart rate > 100 BPM?|!=null"
        ],
        "then": [
          "$gt0021|Total score|.precision=1",
          "$gt0021|Total score|.magnitude=($gt0005.value+$gt0006.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0016.value)/2"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 4,
        "when": [
          "$gt0021|Total score|.magnitude<=1.5"
        ],
        "then": [
          "$gt0036|Risk Incidence|=0|local::at0021|12.1% risk incidence|",
          "$gt0034|2 Tier Risk Level|=0|local::at0008|Pulmonary embolism unlikely (with d-dimer)|",
          "$gt0037|Recommendation|=0|local::at0010|Consider d-dimer testing to rule out pulmonary embolism or a rule out criteria such as PERC|",
          "$gt0035|Risk Prevelance|=0|local::at0015|1.3% risk prevalence|",
          "$gt0033|3 Tier Risk Level|=0|local::at0005|Low Risk|"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 3,
        "when": [
          "$gt0021|Total score|.magnitude>=2",
          "$gt0021|Total score|.magnitude<=4.5"
        ],
        "then": [
          "$gt0037|Recommendation|=1|local::at0011|Consider high sensitivity d-dimer testing or CTA|",
          "$gt0036|Risk Incidence|=0|local::at0021|12.1% risk incidence|",
          "$gt0035|Risk Prevelance|=1|local::at0017|16.2% risk prevalence|",
          "$gt0034|2 Tier Risk Level|=0|local::at0008|Pulmonary embolism unlikely (with d-dimer)|",
          "$gt0033|3 Tier Risk Level|=1|local::at0006|Moderate Risk|"
        ]
      },
      "gt0042": {
        "id": "gt0042",
        "priority": 2,
        "when": [
          "$gt0021|Total score|.magnitude<=6",
          "$gt0021|Total score|.magnitude>=5"
        ],
        "then": [
          "$gt0037|Recommendation|=3|local::at0013|Consider CTA testing|",
          "$gt0033|3 Tier Risk Level|=1|local::at0006|Moderate Risk|",
          "$gt0034|2 Tier Risk Level|=1|local::at0009|Pulmonary embolism likely (with CTA)|",
          "$gt0035|Risk Prevelance|=1|local::at0017|16.2% risk prevalence|",
          "$gt0036|Risk Incidence|=1|local::at0022|37.1% risk incidence|"
        ]
      },
      "gt0040": {
        "id": "gt0040",
        "priority": 1,
        "when": [
          "$gt0021|Total score|.magnitude>6"
        ],
        "then": [
          "$gt0037|Recommendation|=3|local::at0013|Consider CTA testing|",
          "$gt0036|Risk Incidence|=1|local::at0022|37.1% risk incidence|",
          "$gt0035|Risk Prevelance|=2|local::at0019|37.5% risk prevalence|",
          "$gt0034|2 Tier Risk Level|=1|local::at0009|Pulmonary embolism likely (with CTA)|",
          "$gt0033|3 Tier Risk Level|=2|local::at0007|High Risk|"
        ]
      }
    }
