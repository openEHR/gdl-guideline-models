    "lifecycle_state" : "DRAFTED",
    "details" : {
      "en" : {
        "id" : "en",
        "purpose" : "To stratify baseline cardiac risk in patients prior to anthracycline treatment.",
        "keywords" : [
          "anthracycline",
          "cardiac",
          "risk",
          "assessment",
          "breast",
          "cancer"
        ],
        "use" : "Use to stratify cardiac risk in patients considered for anthracycline-based therapy prior to treatment initiation using a simplified rule-based approach derived from predefined risk factors.\n\nThe guideline evaluates three groups of inputs:\n- Very high risk factors (boolean)\n- High risk factors (boolean)\n- Medium risk factors (ordinal, scored as 0, 1, or 2 points)\n\nRisk stratification is performed as follows:\n- Very high risk: presence of any very high risk factor\n- High risk: presence of any high risk factor or a total medium-risk score ≥ 5 points\n- Intermediate risk: total medium-risk score between 2 and 4 points",
        "misuse" : "Not to be used for:\n- diagnosis of cardiovascular disease\n- assessment of cardiac function or treatment response\n- prediction of individual cardiac outcomes",
        "copyright" : "© Cambio CDS"
      }
    },
    "other_details" : {
      "references" : "Regionala cancercentrum i samverkan. Nationellt vårdprogram bröstcancer: Kapitel 21 Kardioonkologi. Kunskapsbanken, 2025. Available at: https://kunskapsbanken.cancercentrum.se/diagnoser/brostcancer/vardprogram/kardioonkologi/\n",
      "en" : "A structured guideline used to stratify patients into cardiac risk categories prior to anthracycline treatment, based on predefined clinical, laboratory, and treatment-related risk factors."
    }
  },
