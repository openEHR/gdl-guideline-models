    "rules": {
      "gt0037": {
        "id": "gt0037",
        "priority": 2,
        "when": [
          "$gt0019|Cry|!=null",
          "$gt0020|Tremors|!=null",
          "$gt0021|Increased muscle tone|!=null",
          "$gt0022|Sleep|!=null",
          "$gt0023|Nasal stuffiness|!=null",
          "$gt0024|Respiratory rate|!=null",
          "$gt0025|Excessive sucking|!=null",
          "$gt0026|Poor feeding|!=null",
          "$gt0027|Feed tolerance|!=null",
          "$gt0028|Stools|!=null"
        ],
        "then": [
          "$gt0034|Total score|.magnitude=$gt0019.value+$gt0020.value+$gt0021.value+$gt0022.value+$gt0023.value+$gt0024.value+$gt0025.value+$gt0026.value+$gt0027.value+$gt0028.value"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 1,
        "when": [
          "$gt0034|Total score|!=null"
        ],
        "then": [
          "$gt0036|Recommendation|='Initiate pharmacologic therapy and supportive measures if three consecutive scores of 6 or greater or two consecutive scores of 10 or greater are observed. Continue monitoring score during treatment. '"
        ]
      }
    }
