    "rules": {
      "gt0043": {
        "id": "gt0043",
        "priority": 4,
        "when": [
          "$gt0053|Active cancer|!=null",
          "$gt0055|Recently bedridden for 3 days or more, or major surgery within the previous 12 wk requiring general or regional anesthesia|!=null",
          "$gt0058|Calf swelling at least 3 cm larger than that on the asymptomatic side|!=null",
          "$gt0060|Collateral superficial veins (nonvaricose)|!=null",
          "$gt0057|Entire leg swollen|!=null",
          "$gt0056|Localized tenderness along the distribution of the deep venous system|!=null",
          "$gt0059|Pitting oedema confined to the symptomatic leg|!=null",
          "$gt0054|Paralysis, paresis, or recent plaster immobilisation of the legs|!=null",
          "$gt0061|Alternative diagnosis at least as likely as DVT|!=null",
          "$gt0064|Previously documented DVT|!=null"
        ],
        "then": [
          "$gt0062|Total score|.magnitude=$gt0053.value+$gt0054.value+$gt0055.value+$gt0056.value+$gt0057.value+$gt0058.value+$gt0059.value+$gt0060.value+$gt0061.value+$gt0064.value"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 3,
        "when": [
          "$gt0062|Total score|<=0"
        ],
        "then": [
          "$gt0063|DVT probability|=$gt0050|Low risk group for DVT. “Unlikely” according to Wells’ DVT studies.|.term"
        ]
      },
      "gt0047": {
        "id": "gt0047",
        "priority": 1,
        "when": [
          "$gt0062|Total score|>=3"
        ],
        "then": [
          "$gt0063|DVT probability|=$gt0051|High risk group for DVT. “Likely” according to Wells’ DVT studies.|.term"
        ]
      },
      "gt0065": {
        "id": "gt0065",
        "priority": 2,
        "when": [
          "$gt0062|Total score|>=1&&$gt0062|Total score|<=2"
        ],
        "then": [
          "$gt0063|DVT probability|=$gt0066|Moderate risk group for DVT.|.term"
        ]
      }
    }
